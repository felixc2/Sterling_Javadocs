<!-- Either ShipmentKey or ShipmentNo is required -->
<!-- OverrideModificationRules is used to override the statuses of shipment that can be considered for pack. This is determined using the modification_type SHIPMENT_PACK -->
<Shipment OverrideModificationRules="Y" SellerOrganizationCode=""
    ShipNode="" ShipmentKey="" ShipmentNo="">
    <Containers>
        <!-- Only one Container will be processed in one call to this API. All other Container nodes will be ignored. Read Javadocs for additional details. -->
        <Container ActualFreightCharge="" AppliedWeight=""
            AppliedWeightUOM="" AstraCode="" BarcodeDiscount=""
            BasicFreightCharge="" CarriageValue="" CarrierBillAccount=""
            CarrierLocationID="" CommitmentCode=""
            ContainerGrossWeight="" ContainerGrossWeightUOM=""
            ContainerHeight="" ContainerHeightUOM="" ContainerLength=""
            ContainerLengthUOM="" ContainerNetWeight=""
            ContainerNetWeightUOM="" ContainerNo="" ContainerScm=""
            ContainerSeqNo="" ContainerType="" ContainerWidth=""
            ContainerWidthUOM="" CustCarrierAccountNo="" CustomsValue=""
            DeclaredValue="" DeliverBy="" DeliveryDay="" DimmedFlag=""
            DiscountAmount="" ExportLicenseExpDate="" ExportLicenseNo=""
            FormID="" OversizedFlag="" ParentContainerNo=""
            RoutingCode="" ServiceType="" ShipDate=""
            SpecialServicesSurcharge="" TrackingNo="" Zone="">
            <ContainerDetails>
                <!--One amongst ShipmentLineKey OR ShipmentLine child element must be passed-->
                <ContainerDetail ContainerDetailsKey="" Quantity="" ShipmentLineKey="">
                    <ShipmentLine DocumentType="" ItemID="" OrderNo=""
                        PrimeLineNo="" ProductClass="" ReleaseNo=""
                        ShipmentLineNo="Required" SubLineNo="" UnitOfMeasure=""/>
                    <ShipmentTagSerials>
                        <ShipmentTagSerial BatchNo="" LotAttribute1=""
                            LotAttribute2="" LotAttribute3=""
                            LotKeyReference="" LotNumber=""
                            ManufacturingDate="" Quantity=""
                            RevisionNo="" SerialNo="" ShipByDate="" ShipmentTagSerialKey=""/>
                    </ShipmentTagSerials>
                </ContainerDetail>
            </ContainerDetails>
            <Instructions>
                <Instruction InstructionText="" InstructionType=""
                    InstructionURL="" InstructionUsage="" SequenceNo=""/>
            </Instructions>
            <AdditionalAttributes>
                <AdditionalAttribute Name="" Value=""/>
            </AdditionalAttributes>
            <SpecialServices>
                <SpecialService SpecialServiceSurcharge="" SpecialServicesCode=""/>
            </SpecialServices>
        </Container>
    </Containers>
</Shipment>
