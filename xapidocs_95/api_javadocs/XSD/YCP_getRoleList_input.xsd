<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YCP/getRoleList/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YCP/getRoleList/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YCP_getRoleList_input.xsd" yfc:rootElement="Role">
        <xsd:appinfo source="yfc:structure">
            <Role hasAttributes="Y">
                <OrgRoleList>
                    <OrgRole hasAttributes="Y"/>
                </OrgRoleList>
                <RoleDocumentList>
                    <RoleDocument hasAttributes="Y">
                        <OrgDocumentList>
                            <OrgDocument hasAttributes="Y"/>
                        </OrgDocumentList>
                    </RoleDocument>
                </RoleDocumentList>
                <ServiceList>
                    <Service hasAttributes="Y">
                        <OrgServiceList>
                            <OrgService hasAttributes="Y"/>
                        </OrgServiceList>
                        <ResourceList>
                            <Resource hasAttributes="Y">
                                <ResourcePermissionList>
                                    <ResourcePermission hasAttributes="Y"/>
                                </ResourcePermissionList>
                            </Resource>
                        </ResourceList>
                    </Service>
                </ServiceList>
            </Role>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Role" type="yfc:RoleXSDType">
        <xsd:annotation yfc:tableName="YFS_ROLE">
            <xsd:documentation>Maintains all roles that an organization may participate as.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="RoleXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgRoleList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:RoleDocumentList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ServiceList"/>
        </xsd:all>
        <xsd:attribute name="FromRoleDescription"
            type="yfctype:Description" use="optional"/>
        <xsd:attribute name="FromRoleId" type="yfctype:Identifier" use="optional"/>
        <xsd:attribute name="FromRoleKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromRoleName" type="yfctype:RoleName" use="optional"/>
        <xsd:attribute name="RoleDescription" type="yfctype:Description" use="optional">
            <xsd:annotation yfc:columnName="ROLE_DESCRIPTION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE">
                <xsd:documentation>Description of a role.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RoleDescriptionQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="RoleId" type="yfctype:Identifier" use="optional">
            <xsd:annotation yfc:columnName="ROLE_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE">
                <xsd:documentation>Unique identifier to the role.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RoleIdQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="RoleKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ROLE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE">
                <xsd:documentation>Primary key attribute of YFS_ROLE.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RoleKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="RoleName" type="yfctype:RoleName" use="optional">
            <xsd:annotation yfc:columnName="ROLE_NAME"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE">
                <xsd:documentation>Name of the role.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RoleNameQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ToRoleDescription"
            type="yfctype:Description" use="optional"/>
        <xsd:attribute name="ToRoleId" type="yfctype:Identifier" use="optional"/>
        <xsd:attribute name="ToRoleKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToRoleName" type="yfctype:RoleName" use="optional"/>
    </xsd:complexType>
    <xsd:element name="OrgRoleList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgRole"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgRole">
        <xsd:complexType>
            <xsd:attribute name="FromOrganizationKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromRoleKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="OrganizationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                    <xsd:documentation>The primary key attribute for the YFS_ORGANIZATION table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="RoleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ROLE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE">
                    <xsd:documentation>Primary key attribute of YFS_ROLE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RoleKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToOrganizationKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToRoleKey" type="yfctype:Key" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="RoleDocumentList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:RoleDocument"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="RoleDocument">
        <xsd:annotation yfc:tableName="YFS_ROLE_DOCUMENT">
            <xsd:documentation>Some roles may require certain business documents for transacting with other participants. This table captures information on all documents that may requires across various roles.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgDocumentList"/>
            </xsd:all>
            <xsd:attribute name="DocumentDescription"
                type="yfctype:Description-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_DESCRIPTION"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE_DOCUMENT">
                    <xsd:documentation>Description of the document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentDescriptionQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DocumentId"
                type="yfctype:DocumentId-20" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_ID"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE_DOCUMENT">
                    <xsd:documentation>Identifier to a document required by a role.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentIdQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DocumentName"
                type="yfctype:DocumentName-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE_DOCUMENT">
                    <xsd:documentation>Name of the document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromDocumentDescription"
                type="yfctype:Description-40" use="optional"/>
            <xsd:attribute name="FromDocumentId"
                type="yfctype:DocumentId-20" use="optional"/>
            <xsd:attribute name="FromDocumentName"
                type="yfctype:DocumentName-40" use="optional"/>
            <xsd:attribute name="FromRoleDocumentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromRoleKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="RoleDocumentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ROLE_DOCUMENT_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE_DOCUMENT">
                    <xsd:documentation>Primary key attribute of YFS_ROLE_DOCUMENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RoleDocumentKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="RoleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ROLE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ROLE_DOCUMENT">
                    <xsd:documentation>A Role that requires the document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RoleKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToDocumentDescription"
                type="yfctype:Description-40" use="optional"/>
            <xsd:attribute name="ToDocumentId"
                type="yfctype:DocumentId-20" use="optional"/>
            <xsd:attribute name="ToDocumentName"
                type="yfctype:DocumentName-40" use="optional"/>
            <xsd:attribute name="ToRoleDocumentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToRoleKey" type="yfctype:Key" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgDocumentList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgDocument"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgDocument">
        <xsd:annotation yfc:tableName="YFS_ORG_DOCUMENT">
            <xsd:documentation>All document requirements for an organization are captured here.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BackupProtocolKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BACKUP_PROTOCOL_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Identifies a backup protocol to deliver a document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BackupProtocolKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DocumentFormatKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_FORMAT_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Format of the document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentFormatKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromBackupProtocolKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromDocumentFormatKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromOrgDocumentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromOrganizationKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromProtocolKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromRoleDocumentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromVariantReference1"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="FromVariantReference2"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="FromVariantReference3"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="FromVariantReference4"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="FromVariantReference5"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="OrgDocumentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORG_DOCUMENT_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Primary key attribute of YFS_ORG_DOCUMENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrgDocumentKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OrganizationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>An Organization for which the document requirements are maintained.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ProtocolKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PROTOCOL_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Identifies protocol used to deliver a document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProtocolKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="RoleDocumentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ROLE_DOCUMENT_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Identifies a role document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RoleDocumentKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToBackupProtocolKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToDocumentFormatKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOrgDocumentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOrganizationKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToProtocolKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToRoleDocumentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToVariantReference1"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="ToVariantReference2"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="ToVariantReference3"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="ToVariantReference4"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="ToVariantReference5"
                type="yfctype:VariantReference" use="optional"/>
            <xsd:attribute name="VariantReference1"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_1"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference1QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="VariantReference2"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_2"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference2QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="VariantReference3"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_3"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference3QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="VariantReference4"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_4"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference4QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="VariantReference5"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_5"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference5QryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ServiceList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Service"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Service">
        <xsd:annotation yfc:tableName="YFS_SERVICE">
            <xsd:documentation>Services are collection of roles. An organization providing a service would mean it plays all the roles listed under the service.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgServiceList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ResourceList"/>
            </xsd:all>
            <xsd:attribute name="FromRoleKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromServiceDesc"
                type="yfctype:Description-255" use="optional"/>
            <xsd:attribute name="FromServiceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromServiceName"
                type="yfctype:ServiceName" use="optional"/>
            <xsd:attribute name="RoleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ROLE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SERVICE">
                    <xsd:documentation>A role included as part of a service</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RoleKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ServiceDesc"
                type="yfctype:Description-255" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_DESC"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SERVICE">
                    <xsd:documentation>Description of the service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceDescQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ServiceName" type="yfctype:ServiceName" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SERVICE">
                    <xsd:documentation>Name of the service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToRoleKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToServiceDesc"
                type="yfctype:Description-255" use="optional"/>
            <xsd:attribute name="ToServiceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToServiceName"
                type="yfctype:ServiceName" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgServiceList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgService"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgService">
        <xsd:annotation yfc:tableName="YFS_ORG_SERVICE">
            <xsd:documentation>Stores services provided by an organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="FromOrgServiceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromOrganizationKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromServiceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="OrgServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORG_SERVICE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_ORG_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrgServiceKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OrganizationKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_SERVICE">
                    <xsd:documentation>Organization providing the service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORG_SERVICE">
                    <xsd:documentation>A service being provided by an organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToOrgServiceKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOrganizationKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToServiceKey" type="yfctype:Key" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ResourceList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Resource"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Resource">
        <xsd:annotation yfc:tableName="YFS_RESOURCE">
            <xsd:documentation>This table stores and maintains all resources in an application. The permissions are granted on a resource to a user group.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ResourcePermissionList"/>
            </xsd:all>
            <xsd:attribute name="ApplicationName"
                type="yfctype:ApplicationName" use="optional">
                <xsd:annotation yfc:columnName="APPLICATION_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RESOURCE">
                    <xsd:documentation>An application associated with the resource.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ApplicationNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="BehaviorClassName"
                type="yfctype:JavaClass" use="optional">
                <xsd:annotation yfc:columnName="BEHAVIOR_CLASS_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RESOURCE">
                    <xsd:documentation>A java class that controls the behavior of a graphical user interface form.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BehaviorClassNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="CanAddToMenu" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CAN_ADD_TO_MENU"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RESOURCE">
                    <xsd:documentation>This indicates whether a resource can be added to the menu. The resources added to menus are always at the leaf node of the menu hierarchy.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CanAddToMenuQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="EventComponent"
                type="yfctype:EventComponent" use="optional">
                <xsd:annotation yfc:columnName="EVENT_COMPONENT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RESOURCE">
                    <xsd:documentation>A java event component on a form.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EventComponentQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FormClassName" type="yfctype:JavaClass" use="optional">
                <xsd:annotation yfc:columnName="FORM_CLASS_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RESOURCE">
