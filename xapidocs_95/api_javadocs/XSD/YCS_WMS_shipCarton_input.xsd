            <xsd:attribute name="ShipperPostalCode"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipperStateProv" type="yfctype:String" use="optional"/>
            <xsd:attribute name="SourceTypeCode" type="yfctype:String" use="optional"/>
            <xsd:attribute name="UOMDim" type="yfctype:String" use="optional"/>
            <xsd:attribute name="UOMWeight" type="yfctype:String" use="optional"/>
            <xsd:attribute name="UPSServiceType" type="yfctype:String" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AccessorialRecord">
