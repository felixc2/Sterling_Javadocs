            <xsd:attribute name="SPFVersion" type="yfctype:String" use="optional"/>
            <xsd:attribute name="SatDeliveryInd" type="yfctype:String" use="optional"/>
            <xsd:attribute name="SaturdayPickupInd"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="SenderName" type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipmentCODCurrencyCode"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipmentChgType" type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipmentNumber" type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipmentReferenceNo1"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="ShipmentReferenceNo1Type"
