            <xsd:attribute name="ConsigneeCreditCardNo"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="ConsigneeFax" type="yfctype:String" use="optional"/>
            <xsd:attribute name="ConsigneeFaxDestinationInd"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="ConsigneeNumber" type="yfctype:String" use="optional"/>
            <xsd:attribute name="CustomerReferenceNumber3"
