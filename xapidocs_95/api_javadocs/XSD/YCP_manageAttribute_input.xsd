                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AttributeLocaleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_LOCALE_KEY" yfc:tableName="YFS_ATTRIBUTE_LOCALE">
