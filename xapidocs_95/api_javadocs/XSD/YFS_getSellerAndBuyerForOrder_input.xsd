            <xsd:annotation yfc:columnName="XREF_ALIAS_TYPE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The cross reference alias type.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="XrefAliasTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="XrefOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="XREF_ORGANIZATION_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="XrefOrganizationCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
