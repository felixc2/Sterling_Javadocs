<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YFS/getSellerAndBuyerForOrder/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YFS/getSellerAndBuyerForOrder/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation
        yfc:fileName="YFS_getSellerAndBuyerForOrder_input.xsd" yfc:rootElement="Organization">
        <xsd:appinfo source="yfc:structure">
            <Organization hasAttributes="Y">
                <OrderingInformation hasAttributes="Y"/>
                <CustomerList>
                    <Customer hasAttributes="Y"/>
                </CustomerList>
                <VendorList>
                    <Vendor hasAttributes="Y"/>
                </VendorList>
                <CorporatePersonInfo hasAttributes="Y"/>
                <ContactPersonInfo hasAttributes="Y"/>
                <BillingPersonInfo hasAttributes="Y"/>
            </Organization>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Organization" type="yfc:OrganizationXSDType">
        <xsd:annotation yfc:tableName="YFS_ORGANIZATION">
            <xsd:documentation>This table stores attributes that are common to all oganizations. Organizations may play different roles in trade. For example,  Buyer, Seller, Enterprise, ShipNode.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="OrganizationXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:OrderingInformation"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CustomerList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:VendorList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CorporatePersonInfo"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ContactPersonInfo"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:BillingPersonInfo"/>
        </xsd:all>
        <xsd:attribute name="AccountWithHub" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="ACCOUNT_WITH_HUB"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The account with the hub.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AccountWithHubQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AuthorityType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="AUTHORITY_TYPE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The nature of the authority that issued the tax exemption to the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AuthorityTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="BillingAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BILLING_ADDRESS_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The billing address that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BillingAddressKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CatalogOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="CATALOG_ORGANIZATION_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization that owns the catalog of this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CatalogOrganizationCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ContactAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CONTACT_ADDRESS_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The contact address that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ContactAddressKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CorporateAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CORPORATE_ADDRESS_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The corporate address that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CorporateAddressKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CreatorOrganizationKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CREATOR_ORGANIZATION_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization that created this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CreatorOrganizationKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="DunsNumber" type="yfctype:DunsNumber" use="optional">
            <xsd:annotation yfc:columnName="DUNS_NUMBER"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The DUNS number of the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DunsNumberQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="FromAccountWithHub" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="FromAuthorityType" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromBillingAddressKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromCatalogOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="FromContactAddressKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromCorporateAddressKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromCreatorOrganizationKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromDunsNumber" type="yfctype:DunsNumber" use="optional"/>
        <xsd:attribute name="FromInventoryOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="FromInventoryPublished" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsHubOrganization" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsLegalEntity" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsNode" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIsSourcingKept" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromIssuingAuthority"
            type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromItemXrefRule" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromLocaleCode" type="yfctype:Locale" use="optional"/>
        <xsd:attribute name="FromOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="FromOrganizationKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromOrganizationName"
            type="yfctype:OrganizationName" use="optional"/>
        <xsd:attribute name="FromParentOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="FromPaymentProcessingReqd"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromPrimaryEnterpriseKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromPrimarySicCode" type="yfctype:Text-10" use="optional"/>
        <xsd:attribute name="FromPrimaryUrl" type="yfctype:URL" use="optional"/>
        <xsd:attribute name="FromTaxExemptFlag" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromTaxExemptionCertificate"
            type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromTaxJurisdiction" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromTaxpayerId" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="FromXrefAliasType" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="FromXrefOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="InventoryOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_ORGANIZATION_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization that owns the inventory of this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryOrganizationCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="InventoryPublished" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_PUBLISHED"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether inventory is published by the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryPublishedQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsHubOrganization" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_HUB_ORGANIZATION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization is hub. There can be only one hub organization in the system, and that organization is the owner of the entire market place. A hub organization has maximum visibility in the market place.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsHubOrganizationQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsLegalEntity" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_LEGAL_ENTITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization is a legal entity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsLegalEntityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsNode" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_NODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization is a node.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsNodeQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IsSourcingKept" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_SOURCING_KEPT"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether sourcing is maintained for the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsSourcingKeptQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IssuingAuthority" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="ISSUING_AUTHORITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The authority that issued tax exemption certificate.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IssuingAuthorityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ItemXrefRule" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="ITEM_XREF_RULE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The rule used for cross referencing items.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemXrefRuleQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="LocaleCode" type="yfctype:Locale" use="optional">
            <xsd:annotation yfc:columnName="LOCALE_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The locale that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LocaleCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The unique identifier for an organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrganizationKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The primary key attribute for the YFS_ORGANIZATION table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrganizationName"
            type="yfctype:OrganizationName" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_NAME"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The name of the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationNameQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ParentOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="PARENT_ORGANIZATION_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This field indicates the parent of an organization. An organization can have multiple child organizations but only one parent.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ParentOrganizationCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PaymentProcessingReqd" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="PAYMENT_PROCESSING_REQD"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization requires payment processing.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PaymentProcessingReqdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PrimaryEnterpriseKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PRIMARY_ENTERPRISE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This field indicates the primary enterprise that is associated with this organization. Organizations may participate with multiple enterprises, one of which is primary.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimaryEnterpriseKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PrimarySicCode" type="yfctype:Text-10" use="optional">
            <xsd:annotation yfc:columnName="PRIMARY_SIC_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The primary SIC code of the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimarySicCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="PrimaryUrl" type="yfctype:URL" use="optional">
            <xsd:annotation yfc:columnName="PRIMARY_URL"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization's primary URL.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimaryUrlQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="TaxExemptFlag" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="TAX_EXEMPT_FLAG"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This indicates whether the organization is exempt from taxes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxExemptFlagQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="TaxExemptionCertificate"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="TAX_EXEMPTION_CERTIFICATE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The certificate of tax exemption.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxExemptionCertificateQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="TaxJurisdiction" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="TAX_JURISDICTION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The tax jurisdiction to which the organization belongs.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxJurisdictionQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="TaxpayerId" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="TAXPAYER_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The tax payer identifier for the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxpayerIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ToAccountWithHub" type="yfctype:ID-40" use="optional"/>
        <xsd:attribute name="ToAuthorityType" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="ToBillingAddressKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToCatalogOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="ToContactAddressKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToCorporateAddressKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToCreatorOrganizationKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToDunsNumber" type="yfctype:DunsNumber" use="optional"/>
        <xsd:attribute name="ToInventoryOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="ToInventoryPublished" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsHubOrganization" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsLegalEntity" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsNode" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIsSourcingKept" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToIssuingAuthority" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="ToItemXrefRule" type="yfctype:Text-40" use="optional"/>
        <xsd:attribute name="ToLocaleCode" type="yfctype:Locale" use="optional"/>
        <xsd:attribute name="ToOrganizationCode" type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="ToOrganizationKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToOrganizationName"
            type="yfctype:OrganizationName" use="optional"/>
        <xsd:attribute name="ToParentOrganizationCode"
            type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="ToPaymentProcessingReqd"
            type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="ToPrimaryEnterpriseKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToPrimarySicCode" type="yfctype:Text-10" use="optional"/>
        <xsd:attribute name="ToPrimaryUrl" type="yfctype:URL" use="optional"/>
