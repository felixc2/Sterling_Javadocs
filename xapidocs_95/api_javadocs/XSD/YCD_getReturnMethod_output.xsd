                </xsd:element>
            </xsd:all>
            <xsd:attribute name="IsCustomerKeepAllowed"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This attribute determines whether customer can keep the return line while creating returns or not. Default value returned by the API is Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsReceiptExpected"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This attribute determines whether a receipt is expected for this return or not</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsReturnNodeFound"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This attribute determines whether a return node is found for this return or not.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsReturnToStoreAllowed"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This attribute determines whether return to store is allowed or not.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsSchedulePickupAllowed"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>This attribute determines whether schedule pickup is allowed or not.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderLineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_LINE_KEY" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The primary key for the YFS_ORDER_LINE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderReturnMethod">
