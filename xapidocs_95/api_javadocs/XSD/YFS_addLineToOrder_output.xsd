                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncludeOrExclude"
                                    type="yfctype:EnumIncludeOrExclude" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCLUDE_OR_EXCLUDE" yfc:tableName="YPM_PRICING_RULE_ITEM">
