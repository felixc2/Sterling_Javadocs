                                    name="IsCustomRule"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_CUSTOM_RULE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates whether the rule is custom or not.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsFreeGiftRule"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_FREE_GIFT_RULE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates whether the rule is a free gift rule or not.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsItemAttributeValueRule"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_ITEM_ATTRIBUTE_VALUE_RULE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates that this item targeted rule is defined on item attribute values. Applicable for all types of rules.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsItemRule"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_ITEM_RULE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates whether the rule is targeted on items or not.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsManualRule"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_MANUAL_RULE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates whether this pricing rule is a manually applied adjustment rule or not. A manual pricing rule is suggested by the pricing service based on contents or total of the order, which then again should be specified with the amount of adjustment to be applied against the order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsNonInstantDiscountRule"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="IS_NON_INSTANT_DISCOUNT_RULE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates if the pricing rule is a Non-Instant Discount pricing Rule</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemOperatorCode"
                                    type="yfctype:EnumItemOperatorCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ITEM_OPERATOR_CODE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates how the trigger for the rule is evaluated. Applicable only for combination type of rule.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MinimizeNumberOfShipments"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MINIMIZE_NUMBER_OF_SHIPMENTS" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates whether to optimize on the number of shipments or not. Applicable only for shipping category of rules.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrganizationCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Pricing organization to which the configuration belongs.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OverallRedemptionLimit" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="OVERALL_REDEMPTION_LIMIT" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>This option limits the number of times that a pricing rule or coupon can be used in total, by all customers</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PositiveInteger"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PaymentType"
                                    type="yfctype:Text-20" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PAYMENT_TYPE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>The rule is applicable for this Payment Type.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingComplexity"
                                    type="yfctype:Text-12" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_COMPLEXITY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>This attribute is used by SBC application. It is a free text field. This attribute is not used by any pricing api internally.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Primary key attribute of YPM_PRICING_RULE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingRuleName"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_NAME" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Name of the pricing rule.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingStatus"
                                    type="yfctype:PricingStatus" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_STATUS" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates the status of the pricing rule.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Priority"
                                    type="yfctype:Priority" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRIORITY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>This attribute is used to prioritize Pricing Rule among a list of applicable pricing rules with same Exclusivity settings. Higher the value of this attribute, higher the priority of Pricing Rule, to be applied to an order/item.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RuleCategory"
                                    type="yfctype:EnumRuleCategory" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RULE_CATEGORY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates the category to which the rule belongs.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RuleType"
                                    type="yfctype:EnumRuleType" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="RULE_TYPE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Indicates the type of the rule.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SellerOrganizationCode"
                                    type="yfctype:OrgCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Seller who maintains this pricing rule. Promotion is defined by the seller.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="StartDateActive" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="START_DATE_ACTIVE" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>Date from which the rule is active.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="StartTime" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="START_TIME" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>If not void indicates that the rule is applicable from this time.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Time"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                </xsd:all>
                                <xsd:attribute name="DenialReason"
                                    type="yfctype:Text-200" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DENIAL_REASON" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>If the promotion is denied, the reason for the denial.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Description"
                                    type="yfctype:Text-200" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DESCRIPTION" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>Description of the promotion.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="PromotionApplied"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_APPLIED" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>A flag indicating if the promotion is applied. When ValidateReferences=Y the value can be 'P' meaning the promotion was partially applied.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="PromotionId"
                                    type="yfctype:PromoCode" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_ID" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>This identifies the promotion that is added to this order. This will be used as the CouponID when determining the price of the order.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="PromotionKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_KEY" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>The primary key of the YFS_PROMOTION table.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="PromotionType"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PROMOTION_TYPE" yfc:tableName="YFS_PROMOTION">
                                    <xsd:documentation>The type of promotion. Like DISCOUNT, BOGO.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="Awards">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="Award">
                            <xsd:annotation yfc:tableName="YFS_PROMOTION_AWARD">
                                <xsd:documentation>This table stores information regarding awards that are created out of the promotions or information regarding pricing rules applied on the order. A single promotion or pricing rule can result in multiple awards.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" ref="yfc:PromotionReferences"/>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="PricingRule">
                                    <xsd:annotation yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>This table provides the information for each pricing rule.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="Coupon">
                                    <xsd:annotation yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>This table stores information about the coupon. The coupon behavior is defined by the pricing rule associated with it.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="AssignedUniqueCodes"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ASSIGNED_UNIQUE_CODES" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Indicates  associated unique codes are individual customer assignments.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CodeFormat"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="CODE_FORMAT" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Format of generated part of unique code.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CouponID"
                                    type="yfctype:CouponId" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="COUPON_ID" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Unique identifier of a coupon.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CouponKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="COUPON_KEY" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Primary key attribute of YPM_COUPON table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="GenerateUniqueCodes"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="GENERATE_UNIQUE_CODES" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Specifies whether coupon definition has generated unique codes</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="GenerationStatus"
                                    type="yfctype:Text-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="GENERATION_STATUS" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Status of Unique Code generation</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ImportUniqueCodes"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IMPORT_UNIQUE_CODES" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Indicates associated unique codes are imported from an external system.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsExclusive"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IS_EXCLUSIVE" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Indicates whether the coupon is exclusive or not. Either a single exclusive coupon or multiple non-exclusive coupons can be applied on an order.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="NumberOfCodesToGenerate" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="NUMBER_OF_CODES_TO_GENERATE" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Specifies the number of unique codes to generate</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:NUMBER-10"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="OrganizationCode"
                                    type="yfctype:OrgCode" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Pricing organization to which the configuration belongs.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>Pricing rule associated with the coupon.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="PricingRulePriceTypeTriggerList">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="1" name="PricingRulePriceTypeTrigger">

                                    <xsd:annotation yfc:tableName="YPM_PRC_RULE_PRC_TYP_TRIG">
                                    <xsd:documentation>Maintains a list of Pricing Rules and the price types which contribute to the total price for Rule criteria.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="PriceTypeName"
                                    type="yfctype:PriceTypeName" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICE_TYPE_NAME" yfc:tableName="YPM_PRC_RULE_PRC_TYP_TRIG">
                                    <xsd:documentation>Price Type associated with this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRC_RULE_PRC_TYP_TRIG">
                                    <xsd:documentation>Primary Key attribute of the Pricing Rule associated with this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PricingRulePriceTypeTriggerKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRC_RULE_PRC_TYP_TRIG_KEY" yfc:tableName="YPM_PRC_RULE_PRC_TYP_TRIG">
                                    <xsd:documentation>Primary key attribute of YPM_PRC_RULE_PRC_TYP_TRIG table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="PricingRulePriceTypeTargetList">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="1" name="PricingRulePriceTypeTarget">

                                    <xsd:annotation yfc:tableName="YPM_PRC_RULE_PRC_TYP_TARG">
                                    <xsd:documentation>Maintains a list of Pricing Rules and their target Pricing Types.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="PriceTypeName"
                                    type="yfctype:PriceTypeName" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICE_TYPE_NAME" yfc:tableName="YPM_PRC_RULE_PRC_TYP_TARG">
                                    <xsd:documentation>Price Type associated with this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRC_RULE_PRC_TYP_TARG">
                                    <xsd:documentation>Primary Key attribute of the Pricing Rule associated with this record.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PricingRulePriceTypeTargetKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRC_RULE_PRC_TYP_TARG_KEY" yfc:tableName="YPM_PRC_RULE_PRC_TYP_TARG">
                                    <xsd:documentation>Primary key attribute of YPM_PRC_RULE_PRC_TYP_TARG table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="PricingRuleTargetAttributeValueList">
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PricingRuleTargetAttributeValue">

                                    <xsd:annotation yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>This table maintains information about the item attribute values of the items on which the pricing rule adjustments are targeted. Targeting by item attribute is allowed only for item quantity and surcharge rule types of pricing category only.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="GroupId" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="GROUP_ID" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>The group id of the attribute value. All attributes with same group id are evaluated together.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Integer"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IncludeOrExclude"
                                    type="yfctype:EnumIncludeOrExclude" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="INCLUDE_OR_EXCLUDE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>Indicates whether certain items should be included or excluded from adjustments depending on item's attribute values.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>The rule this target item attribute value is associated.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PricingRuleTargetAttributeValueKey"
                                    type="yfctype:Key" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRCNG_RULE_TARG_ATTR_VALUE_KEY" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>Primary key attribute of YPM_PRCNG_RULE_TARG_ATTR_VALUE table.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Quantity" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="QUANTITY" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>Quantity of the item.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Quantity"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TargetAttributeDomainID"
                                    type="yfctype:ID-40" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ATTRIBUTE_DOMAIN_ID" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                                    <xsd:documentation>The unique identifier of the attribute domain.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TargetAttributeDoubleValue" use="optional">

