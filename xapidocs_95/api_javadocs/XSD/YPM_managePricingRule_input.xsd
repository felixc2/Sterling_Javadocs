    <xsd:element name="PricingRuleTargetAttributeValue">
        <xsd:annotation yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
            <xsd:documentation>
                <div>
							This table maintains information about the item attribute values of the items on which the pricing rule adjustments are targeted. Targeting by item attribute is allowed only for item quantity and surcharge rule types of pricing category only
							<Validation>
                        <Condition>There is no attribute for passed <code>TargetAttributeID</code>, <code>TargetAttributeGroupID</code> and <code>TargetAttributeDomainID</code>.
								</Condition>
                        <Error Description="Invalid target attribute." ErrorCode="YPM10064"/>
                    </Validation>
                    <Validation>
                        <Condition>The value of the attribute specified is not a constant, it is derived, computed or referenced. Pricing rule doesn't support these Target attributes.
								</Condition>
                        <Error
                            Description="Invalid target attribute for pricing." ErrorCode="YPM10074"/>
                    </Validation>
                    <Validation>
                        <Condition>Passed <code>TargetAttributeValue</code> or <code>TargetAttributeIntegerValue</code> or <code>TargetAttributeDoubleValue</code> is not valid for the attribute.
								</Condition>
                        <Error
                            Description="Invalid target attribute value." ErrorCode="YPM10065"/>
                    </Validation>
                    <Validation>
                        <Condition>Rule Target Attributes are not allowed for <code>GLOBAL_EXCLUSION</code> rules.
								</Condition>
                        <Error
                            Description="API does not accept Rule Targets in the input for GLOBAL_EXCLUSION rules." ErrorCode="YPM10131"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="GroupId" use="optional">
                <xsd:annotation yfc:columnName="GROUP_ID" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>
                        <div> Indicates all the target attribute values with same group Id will be executed together. 
							</div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Integer"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IncludeOrExclude"
                type="yfctype:EnumIncludeOrExclude" use="optional">
                <xsd:annotation yfc:columnName="INCLUDE_OR_EXCLUDE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>
                        <div> Indicates whether certain items should be included or excluded from adjustments. Accepted values <code>INCLUDE</code> and <code>EXCLUDE</code>.
								<Validation>
                                <Condition>Only EXCLUDE Targets are not allowed for Rule. At least one INCLUDE target condition should be there for Combination, Item quantity and Shipping sur-charge rules. 
									</Condition>
                                <Error Description="Invalid ChargeName." ErrorCode="YPM10113"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Operation to perform
								<br/>Valid Values: 
								<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
								<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record. 
								<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing record.
								<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
								<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingRuleTargetAttributeValueKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="PRCNG_RULE_TARG_ATTR_VALUE_KEY" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>Primary key attribute of YPM_PRCNG_RULE_TARG_ATTR_VALUE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" use="optional">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>
                        <div> This Quantity is applicable for Combination pricing rules where value of <code>WhenToApply</code> is <code>EACH_COMBINATION</code>, For other cases target Quantity value will be set to void.
								  Target Quantity will not be considered for getLowestItemPrice API call. For the inclusion targets Quantity value is defaulted to 1. For the exclusion targets Quantity value is defaulted to void.  
								<Validation>
                                <Condition>Target Quantity should be same for all the inclusion target criteria. 
									</Condition>
                                <Error
                                    Description="Target Quantity should be same for all the inclusion target criteria." ErrorCode="YPM10153"/>
                            </Validation>
                            <Validation>
                                <Condition>Target Quantity can not be more than TargetOccurrenceLimit. 
									</Condition>
                                <Error
                                    Description="Target Quantity can not be more than TargetOccurrenceLimit." ErrorCode="YPM10154"/>
                            </Validation>
                            <Validation>
                                <Condition>Discountable Quantity should be a positive non zero value or void for the pricing rule.
									</Condition>
                                <Error
                                    Description="Invalid discountable quantity per each combination in pricing rule. Specify a positive non zero value." ErrorCode="YPM10157"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TargetAttributeDomainID"
                type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_DOMAIN_ID" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>The unique identifier of the attribute domain.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TargetAttributeDoubleValue" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_DOUBLE_VALUE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>The double value of the item attribute.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:QuantityPositiveAndNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TargetAttributeGroupID"
                type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_GROUP_ID" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>The unique identifier of the attribute group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TargetAttributeID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_ID" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>Unique identifier of the item attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TargetAttributeIntegerValue" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_INTEGER_VALUE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>The integer value of the item attribute.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Integer"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TargetAttributeValue"
                type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_VALUE" yfc:tableName="YPM_PRCNG_RULE_TARG_ATTR_VALUE">
                    <xsd:documentation>The TEXT value of the item attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
