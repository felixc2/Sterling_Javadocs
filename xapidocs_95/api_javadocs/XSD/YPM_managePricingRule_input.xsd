<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YPM/managePricingRule/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YPM/managePricingRule/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YPM_managePricingRule_input.xsd" yfc:rootElement="PricingRule">
        <xsd:appinfo source="yfc:structure">
            <PricingRule hasAttributes="Y">
                <Coupon hasAttributes="Y"/>
                <PricingRulePriceTypeTriggerList hasAttributes="Y">
                    <PricingRulePriceTypeTrigger hasAttributes="Y"/>
                </PricingRulePriceTypeTriggerList>
                <PricingRulePriceTypeTargetList hasAttributes="Y">
                    <PricingRulePriceTypeTarget hasAttributes="Y"/>
                </PricingRulePriceTypeTargetList>
                <PricingRuleTargetAttributeValueList hasAttributes="Y">
                    <PricingRuleTargetAttributeValue hasAttributes="Y"/>
                </PricingRuleTargetAttributeValueList>
                <PricingRuleTargetItemList hasAttributes="Y">
                    <PricingRuleTargetItem hasAttributes="Y"/>
                </PricingRuleTargetItemList>
                <PricingRuleCategoryList hasAttributes="Y">
                    <PricingRuleCategory hasAttributes="Y"/>
                </PricingRuleCategoryList>
                <PricingRuleTargetCategoryList hasAttributes="Y">
                    <PricingRuleTargetCategory hasAttributes="Y"/>
                </PricingRuleTargetCategoryList>
                <PricingRuleAttributeValueList hasAttributes="Y">
                    <PricingRuleAttributeValue hasAttributes="Y"/>
                </PricingRuleAttributeValueList>
                <PricingRuleItemList hasAttributes="Y">
                    <PricingRuleItem hasAttributes="Y"/>
                </PricingRuleItemList>
                <PricingRuleActionList hasAttributes="Y">
                    <PricingRuleAction hasAttributes="Y"/>
                </PricingRuleActionList>
                <PricingRuleLocaleList hasAttributes="Y">
                    <PricingRuleLocale hasAttributes="Y"/>
                </PricingRuleLocaleList>
                <PricingRulePricelistExclusionList hasAttributes="Y">
                    <PricingRulePricelistExclusion hasAttributes="Y"/>
                </PricingRulePricelistExclusionList>
            </PricingRule>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="PricingRule" type="yfc:PricingRuleXSDType">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE">
            <xsd:documentation>
                <div>
					Specify the pricing rule to be created, modified or deleted.  <code>PricingRuleKey</code> or <code>CouponKey</code> or combination of <code>OrganizationCode</code> and <code>PricingRuleName</code> or combaination of <code>OrganizationCode</code> and <code>CouponID</code> can be specified to identify a pricing rule.
				</div>
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="PricingRuleXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Coupon"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRulePriceTypeTriggerList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRulePriceTypeTargetList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleTargetAttributeValueList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleTargetItemList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleCategoryList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleTargetCategoryList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleAttributeValueList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleItemList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleActionList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRuleLocaleList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRulePricelistExclusionList"/>
        </xsd:all>
        <xsd:attribute name="AllowAdjustmentGreaterThanUnitPrice" use="optional">
            <xsd:annotation yfc:columnName="ADJUSTMENT_GRT_UNITPRICE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>The default value for this flag is <code>Y</code>.  When set to <code>N</code>, if the adjustment the rule applies is greater than the line items unit price, the adjustment will be reduced to be the unitprice of the line item.
			    </p>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="Y"/>
                    <xsd:enumeration value="N"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ApplyProratedAdjustment" use="optional">
            <xsd:annotation yfc:columnName="APPLY_PRORATED_ADJUST" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>When this flag is Y the prorated adjustment will be spread across the target and trigger items.
			        This flag is only supported for Combination rules that have <code>TargetItemDeterminationRule</code>
			        set to either <code>HIGH_PRICED_ITEM</code>, <code>LOW_PRICED_ITEM</code> or <code>SAME_LOW_PRICE_ITEM</code>.
			        This flag takes precedence over the <code>DistributeAdjustment</code> flag. This flag also takes precedence over
			        the system rule " Distribute non-uniform item adjustment across the lines for the same item "
			        </p>
                        <Validation>
                            <Condition>if <code>ApplyProratedAdjustment</code> is set to <code>Y</code> and the rule is not a combination rule or TargetItemDeterminationRule is not <code>HIGH_PRICED_ITEM</code>, <code>LOW_PRICED_ITEM</code> or <code>SAME_LOW_PRICE_ITEM</code>
                            </Condition>
                            <Error
                                Description="ApplyProratedAdjustment is only supported for Combination rules that have TargetItemDeterminationRule set to HIGH_PRICED_ITEM, LOW_PRICED_ITEM or SAME_LOW_PRICE_ITEM" ErrorCode="YPM10161"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="N"/>
                    <xsd:enumeration value="Y"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute
            name="AssociateExclusionCategoryAttributeForTarget"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ASSOC_EX_CAT_ATTR_TAR" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether exclusion target category and attributes should be evaluated as "AND" or "OR" condition when evaluating rule.  Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute
            name="AssociateExclusionCategoryAttributeForTrigger"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ASSOC_EX_CAT_ATTR_TRIG" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether exclusion trigger category and attributes should be evaluated as "AND" or "OR" condition when evaluating rule.  Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute
            name="AssociateInclusionCategoryAttributeForTarget"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ASSOC_IN_CAT_ATTR_TAR" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether inclusion target category and attributes should be evaluated as "AND" or "OR" condition when evaluating rule. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute
            name="AssociateInclusionCategoryAttributeForTrigger"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ASSOC_IN_CAT_ATTR_TRIG" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether inclusion trigger category and attributes should be evaluated as "AND" or "OR" condition when evaluating rule. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CarrierServiceCode" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>The carrier service availed for transporting the order. Applicable only for <code>RuleCategory</code>
                        <b>
                            <code>SHIPPING</code>
                        </b> else it is discarded.
						<Validation>
                            <Condition>
                                <code>CarrierServiceCode</code> is passed and it is not a valid carrier service.
							</Condition>
                            <Error
                                Description="Invalid Carrier service code." ErrorCode="YPM10032"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CommonTriggerQuantity" use="optional">
            <xsd:annotation yfc:columnName="COMMON_TRIGGER_QUANTITY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>Specifies a common trigger quantity that all trigger criteria - attributes, categories, and items - will count towards. Valid for Order total rules and Combination rules. If the quantity of any combination of items from any of the trigger criteria matches the <code>CommonTriggerQuantity</code>, the rule will be executed.<br/>
						When this quantity is passed, individual quantities for trigger items are not used. Quantity passed in PricingRuleItem, PricingRuleCategory, or PricingRuleAttributeValue elements will be ignored; this quantity will be set to null internally.
						 </p>
                        <Validation>
                            <Condition>If <code>CommonTriggerQuantity</code> is passed and is not 0, and <code>EnableMixNMatchItems</code> is not passed as <code>Y</code> or <code>ItemOperatorCode</code> is not passed as <code>ANY_ITEM</code>.
                            </Condition>
                            <Error
                                Description="CommonTriggerQuantity is only valid when EnableMixNMatchItems='Y' and ItemOperatorCode='ANY_ITEM'." ErrorCode="YPM10160"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:Quantity"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="CopyTriggerExclusionsToTargetExclusions"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="COPY_TRIG_EXCLUDES_TO_TARG" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates, Trigger Exclusions are copied to Target Exclusions. This flag is valid for Order total rules, Ship total rules and Combination rules. For other rules this flag is ignored.
					 	For Order total rule and Ship total rule, this flag's default value is Y. For Combination rule this flag's default value is N. 
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CopyTriggerInclusionsToTargetInclusions"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="COPY_TRIG_INCLUDES_TO_TARG" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates, Trigger Inclusions are copied to Target Inclusions. Default is N. This flag is valid for Order total rules and Combination rules. For other rules this flag is ignored.
					     Order Total rule resets the flag CopyTriggerInclusionsToTargetInclusions value to N, when any one of rule's PricingRuleAction has AdjustmentType as ABSOLUTE and value of CopyTriggerInclusionsToTargetInclusions flag is passed as Y.
					     Shipping Total rule resets the flag CopyTriggerInclusionsToTargetInclusions value to N when value of CopyTriggerInclusionsToTargetInclusions flag is passed as Y.
					     While copying trigger Inclusion to Target Inclusions, if there exists target Exclusion for the same criteria then target will remain the same as exclusion.
					     Value of <code>Quantity</code> attribute will not be copied to Target criteria.   
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Currency" type="yfctype:Currency" use="optional">
            <xsd:annotation yfc:columnName="CURRENCY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Currency in which this rule generates adjustments. Mandatory when rule can generate absolute adjustments, i.e. if any of the <code>PricingRuleAction</code> has <code>AdjustmentType</code> as <b>
                            <code>ABSOLUTE</code>
                        </b> or <b>
                            <code>FLAT_FEE</code>
                        </b> and not otherwise.
						<Validation>
                            <Condition>
                                <code>Currency</code> is not valid.
							</Condition>
                            <Error Description="YFS: Invalid currency." ErrorCode="YFS10325"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>Currency</code> is void and rule can generate absolute adjustments.
							</Condition>
                            <Error
                                Description="Currency cannot be void for rule generating absolute or flat fee adjustment." ErrorCode="YPM10032"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerRedemptionLimit" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_REDEMPTION_LIMIT" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>This option limits the number of times that a pricing rule or coupon can be used by a single customer.
			        </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:PositiveInteger"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="Description"
            type="yfctype:PricingRuleDescription" use="optional">
            <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>Description of the rule.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DiscountAllCombinationItems" use="optional">
            <xsd:annotation yfc:columnName="DISCOUNT_ALL_COMBO_ITEMS" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>When the flag is <code>Y</code> the target items will be grouped together as a combination (the same way triggers are treated) and the discount will be applied to all the items that make the combination. For example a rule like the following can be created: <quote>Buy 2 of A and 3 of B, get $10 off 3 C and 2 D</quote>.  The $10 discount will be applied to 3 instances of C and 2 instances of D (making the total discount $50).  The discount is applied to each target combination for each trigger combination or for each reference applied to the order.
			        </p>
                        <Validation>
                            <Condition>if <code>DiscountAllCombinationItems</code>=<code>Y</code> and <code>DistributeAdjustment</code>=<code>Y</code> or <code>TargetItemDetermination</code>=<code>ALL_ITEMS</code> or <code>ADJUSTMENT_TYPE</code>=<code>FIXED_LINE_PRICE</code>
                            </Condition>
                            <Error
                                Description="Invalid configuration for DiscountAllCombinationItems.  DiscountAllCombinationItems=Y is not support for Fixed Price rule, actions with TargetItemDetermination=ALL_ITEMS, or DistributeAdjustment=Y" ErrorCode="YPM10162"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="N"/>
                    <xsd:enumeration value="Y"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="DistributeAdjustment" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="DISTRIBUTE_ADJUSTMENT" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>Boolean indicating if the rule adjustments should be distributed to lines or not.<br/>
							When this flag is Y, the calculated discount is distributed among the qualified items. 
							This flag is ignored for FIXED_LINE_PRICE and Shipping surcharge rule.
							Default value is N.
							Note: no exception will be thrown for FIXED_LINE_PRICE and Shipping surcharge rule and the value for DistributeAdjustment is null for these cases.
						 </p>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DistributeAdjustmentAcrossCombo" use="optional">
            <xsd:annotation yfc:columnName="DIST_ADJ_ACROSS_COMBO" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>When the flag is <code>Y</code> the target items will be grouped together as a combination (the same way triggers are treated) and the discount will be distributed across the items in the combination. For example a rule like the following can be created: <quote>Buy 2 of A and 3 of B, get $10 off 3 C and 2 D</quote>.  The $10 discount will be distributed across 3 instances of C and 2 instances of D.  The discount is applied to each target combination for each trigger combination or for each reference applied to the order.
			        </p>
                        <Validation>
                            <Condition>if <code>DistributeAdjustmentAcrossCombo</code>=<code>Y</code> and  <code>ADJUSTMENT_TYPE</code> is not ABSOLUTE</Condition>
                            <Error
                                Description="Rules that have DistributeAdjustmentAcrossCombo=Y must have AdjustmentType ABSOLUTE" ErrorCode="YPM10167"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="N"/>
                    <xsd:enumeration value="Y"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="EnableMixNMatchItems" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="ENABLE_MIX_N_MATCH_ITEMS" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>Indicates that discount can be applied in the scenario where the combinations can not satisfied by single item, but can be satisfied by including Items which belongs to same category or having same attribute values defined in the rule. Default value is null.<br/>
							When this flag is enabled for Quantity rules and <code>WHEN_TO_APPLY=SINGLE_ITEM</code>, then field <code>TargetItemDeterminationRule</code> value is used for selecting to which item adjustment should be applied. If the <code>TargetItemDeterminationRule</code> value is null, this value will be defaulted to <code>HIGH_PRICED_ITEM</code>. 
							This flag is not applicable for getItemPrice API.
							Note: Item aggregated quantity will not be distributed among the rule's criteria to satisfy the criteria condition.
							Default value is N
						 </p>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EndDateActive" use="optional">
            <xsd:annotation yfc:columnName="END_DATE_ACTIVE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>After this date the rule is not active. Mandatory when <code>Operation</code> is <b>
                            <code>Create</code>
                        </b> and not otherwise.
						<Validation>
                            <Condition>
                                <code>EndDateActive</code> must be passed when <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EndDateActive</code> is modified to void.
							</Condition>
                            <Error
                                Description="The attribute cannot be void." ErrorCode="YPM10002"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>EndDateActive</code> is on or before <code>StartDateActive</code>.
							</Condition>
                            <Error
                                Description="StartDateActive cannot be on or after EndDateActive." ErrorCode="YPM10013"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="EndTime" use="optional">
            <xsd:annotation yfc:columnName="END_TIME" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If not void indicates that the rule is applicable till this time.
					</div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:Time"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="ExclusivityId" type="yfctype:Text-100" use="optional">
            <xsd:annotation yfc:columnName="EXCLUSIVITY_ID" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>The ExclusivityId this rule belongs to. For example <code>RETAILER</code>, 
                    <code>VENDOR</code> or <code>MANUFACTURE</code>.   If no Id is set then the string 
                    '<code>DEFAULT</code>' is used
                    <br/>
                        <br/>
                    All Order Total rules and Shipping Total Discount rules must be placed within the
                    same Id.  During Exclusivity processing all rules with <code>RuleType</code> set to 
                    <code>ORDER_TOTAL</code> will be treated as being in the same <code>ExclusivityId</code>
                    even if the <code>ExclusivityId</code> for each <code>ORDER_TOTAL</code> rule is 
                    different.
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExclusivityType"
            type="yfctype:EnumPricingExclusivityType" use="optional">
            <xsd:annotation yfc:columnName="EXCLUSIVITY_TYPE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>The Exclusivity Type for this rule.
                          <br/>
                        <br/>
                          If no exclusivity type is set then type <code>ALL</code> is assumed.
                          <br/>
                        <br/>
                          Existing Coupon rules that have <code>IsExclusive</code> set to 'Y' will be treated
                          as <code>RULE_TYPE_ITEM</code> if the ExclusivityType is not set.
                          <br/>
                        <br/>
                          Pricing rules that have <code>RuleType</code> set to <code>ORDER_TOTAL</code> and
                          <code>ExclusivityType</code> set to <code>RULE_TYPE_ITEM</code> will be treated as having
                          <code>ExclusivityType</code> set to <code>RULE_TYPE_ORDER</code>.
                          <br/>
                        <br/>
                        <Validation>
                            <Condition>If <code>ExclusivityType</code> is not one of the valid values listed above.
                            </Condition>
                            <Error
                                Description="Invalid value for pricing rule exclusivity type" ErrorCode="YPM10101"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IgnoreRuleOnManualAdjustment" use="optional">
            <xsd:annotation yfc:columnName="IGNORE_RULE_ON_MANUAL_ADJ" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates that rule is not applicable if manual adjustments or manual rule adjustments are already applied at a line or order. An item level rule with <code>IgnoreRuleOnManualAdjustment</code> set to <b>
                            <code>Y</code>
                        </b> is not applied if any of the lines that the rule applicable on has manual adjustments. An order level rule with <code>IgnoreRuleOnManualAdjustment</code> set to <b>
                            <code>Y</code>
                        </b> is not applied if the order has manual adjustments applied. Default value is N
						<Validation>
                            <Condition>If both <code>IsManualRule</code> and <code>IgnoreRuleOnManualAdjustment</code> are set to <b>
                                    <code>Y</code>
                                </b>.
							</Condition>
                            <Error
                                Description="A manual adjustment rule cannot be ignored on application of another manual adjustment rule." ErrorCode="YPM10082"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="N"/>
                    <xsd:enumeration value="Y"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="IgnoreRuleOnPriceLock" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IGNORE_RULE_ON_PRICE_LOCK" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule should not be fired if the line item's unit price is locked. The line item can be the target or the rule trigger. Applicable only for rules that are item targeted, like combination and item quantity rules. Default Value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnFriday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_FRIDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Friday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnMonday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_MONDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Monday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnSaturday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_SATURDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Saturday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnSunday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_SUNDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Sunday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnThursday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_THURSDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Thursday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnTuesday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_TUESDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Tuesday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsActiveOnWednesday" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_ACTIVE_ON_WEDNESDAY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If set indicates that the rule is applicable on Wednesday. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsCustomRule" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_CUSTOM_RULE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether the rule is custom or not. If set to 'Y', the pricing rule will allow multiple target items and multiple actions. Default value is N.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsFreeGiftRule" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_FREE_GIFT_RULE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether the rule is a free gift rule or not. Default value is N.
						<Validation>
                            <Condition>
                                <code>IsFreeGiftRule</code> is set to <code>Y</code> and the Rule has <code>PricingRulePriceTypeTriggers</code> or <code>PricingRulePriceTypeTargets</code>
                            </Condition>
                            <Error
                                Description="IsFreeGiftRule is not supported for rules that target price types." ErrorCode="YPM10197"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsItemAttributeValueRule"
            type="yfctype:Flag" use="optional">
            <xsd:annotation
                yfc:columnName="IS_ITEM_ATTRIBUTE_VALUE_RULE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>Indicates that this item targeted rule is defined on item attribute values. Applicable for all types of rules.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsManualRule" use="optional">
            <xsd:annotation yfc:columnName="IS_MANUAL_RULE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates whether this pricing rule is a manually applied adjustment rule or not. This flag is considered only during rule creation and ignored when passed during rule modification. A manual pricing rule is suggested by the pricing service based on contents or total of the order, which then again should be specified with the amount of adjustment to be applied against the order. Default value is N.
						<Validation>
                            <Condition>Coupon has <code>IsManualRule</code> as <b>
                                    <code>Y</code>
                                </b>.
							</Condition>
                            <Error
                                Description="A coupon cannot be a manual adjustment rule." ErrorCode="YPM10081"/>
                        </Validation>
                        <Validation>
                            <Condition>If both <code>IsManualRule</code> and <code>IgnoreRuleOnManualAdjustment</code> are set to <b>
                                    <code>Y</code>
                                </b>.
							</Condition>
                            <Error
                                Description="A manual adjustment rule cannot be ignored on application of another manual adjustment rule." ErrorCode="YPM10082"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>IsManualRule</code> is <b>
                                    <code>Y</code>
                                </b> and <code>RuleCategory</code> is <b>
                                    <code>SHIPPING</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Manual adjustment rules that belong to shipping category are not supported." ErrorCode="YPM10084"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>IsManualRule</code> is <b>
                                    <code>Y</code>
                                </b> and <code>AdjustmentType</code> for <b>
                                    <code>PricingRuleAction</code>
                                </b> is <code>FIXED_LINE_PRICE</code>.
                            </Condition>
                            <Error
                                Description="Pricing rule action can not have ADJUSTMENT_TYPE set to FIXED_LINE_PRICE when the Pricing rule is manual" ErrorCode="YPM10110"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>IsManualRule</code> is <b>
                                    <code>Y</code>
                                </b> and <code>RuleType</code> is <code>GLOBAL_EXCLUSION</code>.
                            </Condition>
                            <Error
                                Description="Global exclusion rule can not be a coupon rule or manual rule." ErrorCode="YPM10123"/>
                        </Validation>
                        <Validation>
                            <Condition>If <code>IsManualRule</code> is <b>
                                    <code>Y</code>
                                </b> and <code>RuleType</code> is any of <code>QUANTITY, COMBINATION or SURCHARGE</code>.
                            </Condition>
                            <Error
                                Description="Mix and Match is invalid for manual rules when RuleType is QUANTITY or COMBINATION." ErrorCode="YPM10148"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="N"/>
                    <xsd:enumeration value="Y"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="IsNonInstantDiscountRule"
            type="yfctype:Flag" use="optional">
            <xsd:annotation
                yfc:columnName="IS_NON_INSTANT_DISCOUNT_RULE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
                        <p>If this attribute is Y, then the discount is not provided instantly(The Discounts from this rule will not be considered when calculating the qualifying amount for other order total pricing Rules).If this attribute is not passed,PricingRules are automatically considered as instant discount rules. <br/>
							Note: This flag is not applicable for getItemPrice API.
						 </p>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemOperatorCode"
            type="yfctype:EnumItemOperatorCode" use="optional">
            <xsd:annotation yfc:columnName="ITEM_OPERATOR_CODE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates how the trigger for the rule is evaluated. 
						<p>Mandatory for combination rule and when <code>Operation</code> is <b>
                                <code>Create</code>
                            </b> and not otherwise.
						   In case of Order total and Shipment Order total rules, ItemOperatorCode is defaulted to <code>"ANY_ITEM"</code> when trigger inclusions exists for the rule.
						</p>
                        <Validation>
                            <Condition>
                                <code>ItemOperatorCode</code> must be passed when <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="MinimizeNumberOfShipments"
            type="yfctype:Flag" use="optional">
            <xsd:annotation
                yfc:columnName="MINIMIZE_NUMBER_OF_SHIPMENTS" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>Indicates whether to optimize on the number of shipments or not. Applicable only for shipping category of rules.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div> Operation to perform
						<br/>Valid Values: 
						<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record. 
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Pricing organization to with this rule belongs. Mandatory when <code>Operation</code> is <b>
                            <code>Create</code>
                        </b>.
						<Validation>
                            <Condition>
                                <code>OrganizationCode</code> must be passed when <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not valid.
							</Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OverallRedemptionLimit" use="optional">
            <xsd:annotation yfc:columnName="OVERALL_REDEMPTION_LIMIT" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>This option limits the number of times that a pricing rule or coupon can be used in total, by all customers
			        </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:PositiveInteger"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="PaymentType" type="yfctype:Text-20" use="optional">
            <xsd:annotation yfc:columnName="PAYMENT_TYPE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>The rule is applicable for this Payment Type.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PricingComplexity" type="yfctype:Text-12" use="optional">
            <xsd:annotation yfc:columnName="PRICING_COMPLEXITY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>
					This attribute is used by SBC application. It is a free text field. This attribute is not used by any pricing api internally.
					<br/>
                        <br/>
					Values used for this attribute are<br/>
                        <br/>
                        <code>Attribute</code>
                        <br/>
                        <code>Category</code>
                        <br/>
                        <code>Product</code>
                        <br/>
                        <code>Basic</code>
                        <br/>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PricingRuleKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>Primary key attribute of YPM_PRICING_RULE table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PricingRuleName"
            type="yfctype:PricingRuleName" use="optional">
            <xsd:annotation yfc:columnName="PRICING_RULE_NAME" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Name of the pricing rule. Mandatory if the rule is not a coupon and the <code>Operation</code> is <b>
                            <code>Create</code>
                        </b> and not otherwise. <code>PricingRuleName</code> is defaulted to <code>CouponID</code> if the rule is a coupon and <code>PricingRuleName</code> is not passed.
						<Validation>
                            <Condition>
                                <code>PricingRuleName</code> must be passed if it is not a coupon and the <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>PricingRuleName</code> is modified to void.
							</Condition>
                            <Error
                                Description="The attribute cannot be void." ErrorCode="YPM10002"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PricingStatus" type="yfctype:PricingStatus" use="optional">
            <xsd:annotation yfc:columnName="PRICING_STATUS" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates the status of the rule. It is defaulted to <b>
                            <code>INACTIVE</code>
                        </b> when <code>Operation</code> is <b>
                            <code>Create</code>
                        </b> and not passed in the input.
						<Validation>
                            <Condition>
                                <code>PricingStatus</code> is passed and not a valid value.
							</Condition>
                            <Error Description="Invalid PricingStatus." ErrorCode="YPM10012"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Priority" type="yfctype:Priority" use="optional">
            <xsd:annotation yfc:columnName="PRIORITY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Used for Exclusivity processing.  If set, the larger the value, the higher the precedence.
                    For example a rule with a priority of 200 is chosen over a rule with priority 100.
                    <br/>
                        <br/>
                    The priority can be set to <code>0</code> indicating no priority.  When no priority is set
                    for any of the rules under consideration, the rule that gives the best discount is the rule chosen.
                    <br/>
                        <br/>
                    Multiple rules can have the same priority.  When more than one rule has the same priority the rule that
                    gives the best discount is the rule chosen.
                    <br/>
                        <br/>
                    Setting priority for a rule that has an <code>ExclusivityType</code> of <code>ALL</code> has no effect.
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RefreshCache" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Attribute can be used to control whether the cache is updated or not.  If the value <code>N</code> the cache will not be refreshed.  The default value is <code>Y</code>.
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="yfctype:Flag">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="Y"/>
                    <xsd:enumeration value="N"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="RuleCategory"
            type="yfctype:EnumRuleCategory" use="optional">
            <xsd:annotation yfc:columnName="RULE_CATEGORY" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates the category to which the rule belongs. Mandatory when <code>Operation</code> is <b>
                            <code>Create</code>
                        </b> and not otherwise.
						<Validation>
                            <Condition>
                                <code>RuleCategory</code> must be passed when <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>Price Types are only supported for <code>RuleCategory</code>
                                <code>PRICING</code>.
							</Condition>
                            <Error
                                Description="Price Types are unsupported for the specified Rule Category." ErrorCode="YPM10196"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RuleType" type="yfctype:EnumRuleType" use="optional">
            <xsd:annotation yfc:columnName="RULE_TYPE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Indicates the type of the rule. Mandatory when <code>Operation</code> is <b>
                            <code>Create</code>
                        </b> and not otherwise.
						<Validation>
                            <Condition>
                                <code>RuleType</code> must be passed when <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SellerOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="SELLER_ORGANIZATION_CODE" yfc:tableName="YPM_PRICELIST_HDR">
                <xsd:documentation>
                    <div>Seller organization to with this price list belongs. By default SellerOrganizationCode is defaulted to the pricing organization.
						<Validation>
                            <Condition>
                                <code>OrganizationCode</code> is not valid.
							</Condition>
                            <Error
                                Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>SellerOrganizationCode</code> is not seller organization.
							</Condition>
                            <Error
                                Description="YFS:Invalid seller organization code." ErrorCode="YPM10100"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="StartDateActive" use="optional">
            <xsd:annotation yfc:columnName="START_DATE_ACTIVE" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>Date from which the rule is active. Mandatory when <code>Operation</code> is <b>
                            <code>Create</code>
                        </b> and not otherwise.
						<Validation>
                            <Condition>
                                <code>StartDateActive</code> must be passed when <code>Operation</code> is <b>
                                    <code>Create</code>
                                </b>.
							</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>StartDateActive</code> is modified to void.
							</Condition>
                            <Error
                                Description="The attribute cannot be void." ErrorCode="YPM10002"/>
                        </Validation>
                        <Validation>
                            <Condition>
                                <code>StartDateActive</code> is on or after <code>EndDateActive</code>.
							</Condition>
                            <Error
                                Description="StartDateActive cannot be on or after EndDateActive." ErrorCode="YPM10013"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:TimeStamp"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="StartTime" use="optional">
            <xsd:annotation yfc:columnName="START_TIME" yfc:tableName="YPM_PRICING_RULE">
                <xsd:documentation>
                    <div>If not void indicates that the rule is applicable from this time.
					</div>
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:union>
                    <xsd:simpleType>
                        <xsd:restriction base="yfctype:Time"/>
                    </xsd:simpleType>
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:enumeration value=""/>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:union>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="Coupon">
        <xsd:annotation yfc:tableName="YPM_COUPON">
            <xsd:documentation>
                <div>Stores information about the coupon. The coupon behavior is defined by the pricing rule associated with it.
					<Validation>
                        <Condition>An existing pricing rule is being modified to a coupon.</Condition>
                        <Error
                            Description="Coupon cannot be created with an existing pricing rule." ErrorCode="YPM10092"/>
                    </Validation>
                    <Validation>
                        <Condition>Coupons are not allowed for GLOBAL_EXCLUSION rules.</Condition>
                        <Error
                            Description="Coupons are not allowed for GLOBAL_EXCLUSION rules." ErrorCode="YPM10123"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CodeFormat" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CODE_FORMAT" yfc:tableName="YPM_COUPON">
                    <xsd:documentation>
                        <div>Format of generated part of unique code.
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CouponID" type="yfctype:CouponId" use="optional">
                <xsd:annotation yfc:columnName="COUPON_ID" yfc:tableName="YPM_COUPON">
                    <xsd:documentation>
                        <div>Unique identifier of a coupon. Mandatory if rule is for a coupon and the <code>Operation</code> is <b>
                                <code>Create</code>
                            </b> and not otherwise.
							<Validation>
                                <Condition>
                                    <code>CouponID</code> must be passed if rule is for a coupon and the <code>Operation</code> is <b>
                                    <code>Create</code>
                                    </b>.
								</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>CouponID</code> is modified to void.
								</Condition>
                                <Error
                                    Description="The attribute cannot be void." ErrorCode="YPM10002"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CouponKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="COUPON_KEY" yfc:tableName="YPM_COUPON">
                    <xsd:documentation>Primary key attribute of YPM_COUPON table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GenerateUniqueCodes"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="GENERATE_UNIQUE_CODES" yfc:tableName="YPM_COUPON">
                    <xsd:documentation>
                        <div>Specifies whether unique codes will be generated for the coupon definition
                           <Validation>
                                <Condition>When <code>GenerateUniqueCodes</code>='Y' and Attributes <code>CodeFormat</code> and/or <code>NumberOfCodesToGenerate</code> do not have valid values
								</Condition>
                                <Error
                                    Description="Missing required attribute for coupon code generation" ErrorCode="YPM10166"/>
                            </Validation>
                            <Validation>
                                <Condition>When <code>GenerateUniqueCodes</code>='Y' and no active coupon policy for the organization exists
								</Condition>
                                <Error
                                    Description="No active coupon policy for the organization was found" ErrorCode="YPM10167"/>
                            </Validation>
                            <Validation>
                                <Condition>When <code>GenerateUniqueCodes</code>='Y' and more than one active coupon policy for the organization exists
								</Condition>
                                <Error
                                    Description="Multiple Active coupon policies were found for the organization" ErrorCode="YPM10168"/>
                            </Validation>
