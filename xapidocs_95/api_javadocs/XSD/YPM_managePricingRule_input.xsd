                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TriggerAttributeGroupID"
                type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_GROUP_ID" yfc:tableName="YPM_PRICING_RULE_ATTR_VALUE">
                    <xsd:documentation>The unique identifier of the attribute group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TriggerAttributeID"
                type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_ID" yfc:tableName="YPM_PRICING_RULE_ATTR_VALUE">
                    <xsd:documentation>Unique identifier of the item attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TriggerAttributeIntegerValue" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_INTEGER_VALUE" yfc:tableName="YPM_PRICING_RULE_ATTR_VALUE">
                    <xsd:documentation>The integer value of the item attribute.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Integer"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TriggerAttributeValue"
                type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="ATTRIBUTE_VALUE" yfc:tableName="YPM_PRICING_RULE_ATTR_VALUE">
                    <xsd:documentation>The TEXT value of the item attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleItemList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PricingRuleItem"/>
            </xsd:sequence>
            <xsd:attribute name="DeleteExcludedRecord"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  				If <code>DeleteExcludedRecord</code> is passed as <b>
                                <code>Y</code>
                            </b>, the existing excluded records are removed.
				  			If <code>DeleteExcludedRecord</code> is not passed or <code>DeleteExcludedRecord</code> is passed as <b>
                                <code>N</code>
                            </b>, only the changes specified in the input are applied.
				  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DeleteIncludedRecord"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  				If <code>DeleteIncludedRecord</code> is passed as <b>
                                <code>Y</code>
                            </b>, the existing included records are removed.
				  			If <code>DeleteIncludedRecord</code> is not passed or <code>DeleteIncludedRecord</code> is passed as <b>
                                <code>N</code>
                            </b>, only the changes specified in the input are applied.
				  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IncludeOrExclude"
                type="yfctype:EnumIncludeOrExclude" use="optional">
                <xsd:annotation yfc:columnName="INCLUDE_OR_EXCLUDE" yfc:tableName="YPM_PRICING_RULE_ITEM">
                    <xsd:documentation>
                        <div> Indicates whether certain items should be included or excluded from adjustments. Accepted values <code>INCLUDE</code> and <code>EXCLUDE</code>.
								<Validation>
                                <Condition>Only EXCLUDE Targets are not allowed for Rule. At least one INCLUDE target condition should be there for Combination, Item quantity and Shipping sur-charge rules. 
									</Condition>
                                <Error Description="Invalid ChargeName." ErrorCode="YPM10113"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  				If <code>Reset</code> is passed as <b>
                                <code>Y</code>
                            </b>, the existing quantity tiers are removed and replaced with the new ones passed in the input.
				  			If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                <code>N</code>
                            </b>, only the changes specified in the input are applied.
				  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleItem">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE_ITEM">
            <xsd:documentation>This table maintains the information of combinations of item and quantity conditions that trigger the pricing rule. The data in this table is populated only for combination rule type.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="IncludeOrExclude"
                type="yfctype:EnumIncludeOrExclude" use="optional">
                <xsd:annotation yfc:columnName="INCLUDE_OR_EXCLUDE" yfc:tableName="YPM_PRICING_RULE_ITEM">
                    <xsd:documentation>
                        <div> Indicates whether certain items should be included or excluded from adjustments. Accepted values <code>INCLUDE</code> and <code>EXCLUDE</code>.
								<Validation>
                                <Condition>Only EXCLUDE Targets are not allowed for Rule. At least one INCLUDE target condition should be there for Combination, Item quantity and Shipping sur-charge rules. 
									</Condition>
                                <Error Description="Invalid ChargeName." ErrorCode="YPM10113"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YPM_PRICING_RULE_ITEM">
                    <xsd:documentation>The unique identifier of the target item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Operation to perform
								<br/>Valid Values: 
								<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
								<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record. 
								<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing record.
								<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
								<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingRuleItemKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICING_RULE_ITEM_KEY" yfc:tableName="YPM_PRICING_RULE_ITEM">
                    <xsd:documentation>Primary key attribute of YPM_PRICING_RULE_ITEM table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" use="optional">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YPM_PRICING_RULE_ITEM">
                    <xsd:documentation>
                        <div> Quantity of the item.  
								<Validation>
                                <Condition>Invalid quantity for pricing rule item condition.
									</Condition>
                                <Error
                                    Description="Invalid quantity for pricing rule item condition. Specify a positive non zero value." ErrorCode="YPM10059"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YPM_PRICING_RULE_ITEM">
                    <xsd:documentation>The unit of measure of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleActionList">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE_ACTION">
            <xsd:documentation>
                <div>
                    <Validation>
                        <Condition>Pricing Rule Actions are not allowed for <code>GLOBAL_EXCLUSION</code> rules.
							</Condition>
                        <Error
                            Description="Pricing Rule Actions are not allowed for GLOBAL_EXCLUSION rules." ErrorCode="YPM10124"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:PricingRuleAction"/>
            </xsd:sequence>
            <xsd:attribute name="ApplyAdjustmentToRemainder"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="APPLY_ADJ_TO_REMAINDER" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>Indicates that adjustments should be applied only to the remainder of the transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxAbsoluteAdjustment" use="optional">
                <xsd:annotation yfc:columnName="MAX_ABSOLUTE_ADJUSTMENT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Maximum absolute adjustment that can be provided for this pricing rule action within a transaction. This value will be ignored for Manual rules.
							       <Validation>
                                <Condition>
                                    <code>MaxAbsoluteAdjustment</code> is not supported when <code>AdjustmentType</code> is <code>FIXED_LINE_PRICE</code>
                                </Condition>
                                <Error
                                    Description="MaxAbsoluteAdjustment is not valid when AdjustmentType is FIXED_LINE_PRICE" ErrorCode="YPM10129"/>
                            </Validation>
                            <Validation>
                                <Condition> Passed <code>MaxAbsoluteAdjustment</code> must be set to the same value for all actions.
					       </Condition>
                                <Error
                                    Description="MaxAbsoluteAdjustment must be set to the same values." ErrorCode="YPM10140"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>MaxAbsoluteAdjustment</code> is not valid for Order Total rule with <code>AdjustmentType=ABSOLUTE</code> or for Shipping total rule with any <code>AdjustmentType</code>.
					       </Condition>
                                <Error
                                    Description="MaxAbsoluteAdjustment is only valid when AdjustmentType=PERCENTAGE for Order Total rules. And MaxAbsoluteAdjustment is not valid for Shipping total rules." ErrorCode="YPM10142"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="QualifierAmount" use="optional">
                <xsd:annotation yfc:columnName="QUALIFIER_AMOUNT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>
                            <p>
                                <code>QualifierAmount</code> attribute is used as promotion qualifier. This attribute work along with <code>QualifierAmountType</code>.
					       		</p>
                            <Validation>
                                <Condition>
                                    <code>QualifierAmount</code> is not supported when <code>QualifierAmountType</code> is not defined.
					           </Condition>
                                <Error
                                    Description="QualifierAmountType is mandatory when QualifierAmount is specified and viceversa." ErrorCode="YPM10137"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="QualifierAmountType"
                type="yfctype:EnumQualifierAmountType" use="optional">
                <xsd:annotation yfc:columnName="QUALIFIER_AMOUNT_TYPE" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>
                            <p>
                                <code>QualifierAmountType</code> attribute is used as promotion qualifier. This attribute work along with <code>QualifierAmount</code> as described below.
					       		In case of <code>QualifierAmountType=TRIGGER_MINIMUM_TOTAL_EXTENDED_PRICE</code>, trigger quantity not considered when comparing total extended price with QualifierAmount.</p>
                            <Validation>
                                <Condition>
                                    <code>QualifierAmountType</code> is not supported when <code>QualifierAmount</code> is not defined.
					           </Condition>
                                <Error
                                    Description="QualifierAmountType is mandatory when QualifierAmount is specified and viceversa." ErrorCode="YPM10137"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>QualifierAmountType</code> must be same for all actions with in a pricing rule.
					           </Condition>
                                <Error
                                    Description="QualifierAmountType must be same for all actions with in a pricing rule." ErrorCode="YPM10139"/>
                            </Validation>
                            <Validation>
                                <Condition> Invalid QualifierAmountType. For Quantity and Shipping Sur-charge rules allowed values are <code>TARGET_MINIMUM_UNIT_PRICE and TARGET_MINIMUM_TOTAL_EXTENDED_PRICE</code>. For other types of rules allowed values are  <code> TRIGGER_MINIMUM_TOTAL_EXTENDED_PRICE, TRIGGER_MINIMUM_UNIT_PRICE, TARGET_MINIMUM_TOTAL_EXTENDED_PRICE and TARGET_MINIMUM_UNIT_PRICE</code>. 
					           </Condition>
                                <Error
                                    Description="Invalid QualifierAmountType." ErrorCode="YPM10135"/>
                            </Validation>
                            <Validation>
                                <Condition> Invalid QualifierAmountType for Order total rule with <code>ABSOLUTE DiscountType</code> or for Shipping total rule. For Order total rule with ABSOLUTE DiscountType or for Shipping total rule, allowed values for QualifierAmountType are <code>TRIGGER_MINIMUM_UNIT_PRICE</code> and <code>TRIGGER_MINIMUM_TOTAL_EXTENDED_PRICE.</code>. 
					           </Condition>
                                <Error
                                    Description="Invalid QualifierAmountType for order total rule with ABSOLUTE DiscountType or for Shipping total rule." ErrorCode="YPM10134"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  				If <code>Reset</code> is passed as <b>
                                <code>Y</code>
                            </b>, the existing quantity tiers are removed and replaced with the new ones passed in the input.
				  			If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                <code>N</code>
                            </b>, only the changes specified in the input are applied.
				  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TargetItemDeterminationRule"
                type="yfctype:EnumTargetItemDeterminationRule" use="optional">
                <xsd:annotation yfc:columnName="TARGET_ITEM_DETER_RULE" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Specifies, on which target item the action should be applied. Applicable Values are: <code>LOW_PRICED_ITEM</code>, <code>HIGH_PRICED_ITEM</code>, <code>SAME_LOW_PRICE_ITEM</code> and <code>ALL_ITEMS</code>. used when defining combination rules with target as All items in order, but excludes certain items.
										<Validation>
                                <Condition>If the value for <code>TargetItemDeterminationRule</code> differs across action tiers
											</Condition>
                                <Error
                                    Description="TargetItemDeterminationRule must be set to the same value for all tiers" ErrorCode="YPM10133"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>TargetItemDeterminationRule</code> should be empty or ALL_ITEMS for Order total rule with <code>ABSOLUTE DiscountType</code> or for Shipping total rule.
											</Condition>
                                <Error
                                    Description="Invalid TargetItemDeterminationRule for Order total rule with ABSOLUTE DiscountType or for Shipping total rule" ErrorCode="YPM10152"/>
                            </Validation>
                            <Validation>
                                <Condition>The value <code>SAME_LOW_PRICE_ITEM</code> is supported only for Combination rules.
											</Condition>
                                <Error
                                    Description="The value SAME_LOW_PRICE_ITEM for TargetItemDeterminationRule is supported only for Combination rules" ErrorCode="YPM10155"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleAction">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE_ACTION">
            <xsd:documentation>This table maintains the information about the actions of a pricing rule. One of the actions is fired depending on the condition satisfied. This table also maintains the tiered conditions of order total and item quantity rule.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Adjustment" use="optional">
                <xsd:annotation yfc:columnName="ADJUSTMENT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Adjustment to be applied as part of this action.
								<Validation>
                                <Condition>If <code>Adjustment</code> is void and not a manual adjustment rule.
									</Condition>
                                <Error
                                    Description="Adjustment cannot be void for a pricing rule action." ErrorCode="YPM10039"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule <code>Adjustment</code> and <code>MaximumManualAdjustment</code> are null.
									</Condition>
                                <Error
                                    Description="Both Adjustment and MaximumManualAdjustment cannot be null for a manual adjustment rule." ErrorCode="YPM10083"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule if <code>abs(Adjustment) &gt; abs(MaximumManualAdjustment)</code>.
									</Condition>
                                <Error
                                    Description="Absolute value of Adjustment cannot be greater than absolute value of MaximumManualAdjustment." ErrorCode="YPM10095"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule if <code>Adjustment &lt; 0</code> when <code>MaximumManualAdjustment &gt; 0</code>.
									</Condition>
                                <Error
                                    Description="Adjustment cannot be a discount when MaximumManualAdjustment is an uplift." ErrorCode="YPM10097"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule if <code>Adjustment &gt; 0</code> when <code>MaximumManualAdjustment &lt; 0</code>.
									</Condition>
                                <Error
                                    Description="Adjustment cannot be an uplift when MaximumManualAdjustment is a discount." ErrorCode="YPM10099"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>AdjustmentType</code> is <code>FIXED_LINE_PRICE</code> and the adjustment is a negative number</Condition>
                                <Error
                                    Description="Adjustment value can not be negative when AdjustmentType is FIXED_LINE_PRICE" ErrorCode="YPM10127"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="AdjustmentType"
                type="yfctype:EnumPricingAdjustmentType" use="optional">
                <xsd:annotation yfc:columnName="ADJUSTMENT_TYPE" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Indicates the type of adjustment. Mandatory for every action. Defaulted for <b>
                                <code>SURCHARGE</code>
                            </b> type of rule to <b>
                                <code>ABSOLUTE</code>

                            </b> if not passed in the input.
								<div
                                class="portlet" style="width:90%">
                                <table border="0" cellpadding="0"
                                    cellspacing="0" class="table">
                                    <tr>
                                    <td class="tablecolumnheader"
                                    rowspan="2" width="20%">RuleCategory</td>
                                    <td class="tablecolumnheader"
                                    rowspan="2" width="20%">RuleType</td>
                                    <td align="center"
                                    class="tablecolumnheader" colspan="4">AdjustmentType</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumnheader">
                                    <b>ABSOLUTE</b>
                                    </td>
                                    <td class="tablecolumnheader">
                                    <b>PERCENT</b>
                                    </td>
                                    <td class="tablecolumnheader">
                                    <b>FLAT_FEE</b>
                                    </td>
                                    <td class="tablecolumnheader">
                                    <b>FIXED_LINE_PRICE</b>
                                    </td>
                                    <td class="tablecolumnheader">
                                    <b>ABSOLUTE_TOTAL</b>
                                    </td>
                                    <td class="tablecolumnheader">
                                    <b>FLAT_FEE_TOTAL</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">PRICING</td>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">COMBINATION</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">PRICING</td>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ITEM_QUANTITY</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">PRICING</td>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ORDER_TOTAL</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">SHIPPING</td>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ORDER_TOTAL</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">SHIPPING</td>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">SURCHARGE</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">SHIPPING</td>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ITEM_QUANTITY</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                </table>
                            </div>
                            <Validation>
                                <Condition>Passed <code>AdjustmentType</code> is not valid for the rule.
									</Condition>
                                <Error
                                    Description="Invalid AdjustmentType specified for the rule action." ErrorCode="YPM10047"/>
                            </Validation>
                            <Validation>
                                <Condition>Passed <code>AdjustmentType</code> is not the same for every action.  All actions need to be the same type when <code>FIXED_LINE_PRICE</code>
                                </Condition>
                                <Error
                                    Description="If one action is of type FIXED_LINE_PRICE then all actions for the rule must be of this type" ErrorCode="YPM10125"/>
                            </Validation>
                            <br/>Note: A <code>
                                <b>FLAT_FEE</b>
                            </code> shipping action is applied only in cases where it generates discounts. 
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ApplyAdjustmentToRemainder"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="APPLY_ADJ_TO_REMAINDER" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Indicates that adjustments should be applied only to the remainder of the transaction. Default value is N.
 								<Validation>
                                <Condition>If the value for <code>ApplyAdjustmentToRemainder</code> differs across action tiers
									</Condition>
                                <Error
                                    Description="ApplyAdjustmentToRemainder must be set to the same value for all tiers" ErrorCode="YPM10143"/>
                            </Validation>
                            <Validation>
                                <Condition> The target and trigger criteria should be same if <code>ApplyAdjustmentToRemainder</code> is enabled.
					          		</Condition>
                                <Error
                                    Description="The target and trigger include criteria should be same if ApplyAdjustmentToRemainder is enabled." ErrorCode="YPM10156"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>ApplyAdjustmentToRemainder</code> is not supported when <code>AdjustmentType</code> is <code>FIXED_LINE_PRICE</code>.
					          		</Condition>
                                <Error
                                    Description="ApplyAdjustmentToRemainder is not applicable to Fixed price rules." ErrorCode="YPM10145"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeCategory" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Charge Category associated with the rule action.
								<Validation>
                                <Condition>If <code>ChargeCategory</code> is passed and it is not valid.
									</Condition>
                                <Error
                                    Description="Invalid ChargeCategory." ErrorCode="YPM10034"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ChargeCategory</code> is defined for discount and the rule action generates uplift.
									</Condition>
                                <Error
                                    Description="Charge category defined for discount is not valid for pricing rule action that generate uplift." ErrorCode="YPM10060"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ChargeCategory</code> is defined for uplift and the rule action generates discount.
									</Condition>
                                <Error
                                    Description="Charge category defined for uplift is not valid for pricing rule action that generate discount." ErrorCode="YPM10061"/>
                            </Validation>
                            <Validation>
                                <Condition>If <code>ChargeCategory</code> is not billable and the rule action generates discount or uplift.
									</Condition>
                                <Condition>NOTE:This Rule can be overriden at an enterprise level by enabling the business rule provided "YPM_IS_NONBILLABLE_PRICING_RULE_ENABLED".
									</Condition>
                                <Error
                                    Description="Non billable charge category is not valid for rule action that generate uplift or discount." ErrorCode="YPM10062"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Charge Name associated with the rule action.
								<Validation>
                                <Condition>A valid <code>ChargeCategory</code> is passed and <code>ChargeName</code> is not valid.
									</Condition>
                                <Error Description="Invalid ChargeName." ErrorCode="YPM10035"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxAbsoluteAdjustment" use="optional">
                <xsd:annotation yfc:columnName="MAX_ABSOLUTE_ADJUSTMENT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Maximum absolute adjustment that can be provided for this pricing rule action within a transaction. This value will be ignored for Manual rules.
					       <Validation>
                                <Condition>
                                    <code>MaxAbsoluteAdjustment</code> is not supported when <code>AdjustmentType</code> is <code>FIXED_LINE_PRICE</code>
                                </Condition>
                                <Error
                                    Description="MaxAbsoluteAdjustment is not valid when AdjustmentType is FIXED_LINE_PRICE" ErrorCode="YPM10129"/>
                            </Validation>
                            <Validation>
                                <Condition> Passed <code>MaxAbsoluteAdjustment</code> must be set to the same value for all actions.
                               </Condition>
                                <Error
                                    Description="MaxAbsoluteAdjustment must be set to the same values." ErrorCode="YPM10140"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>MaxAbsoluteAdjustment</code> is not valid for Order Total rules and Shipping total rules when <code>AdjustmentType=ABSOLUTE</code>.
                               </Condition>
                                <Error
                                    Description="MaxAbsoluteAdjustment is only valid when AdjustmentType=PERCENTAGE for Order Total rules and for Shipping total rules." ErrorCode="YPM10142"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>MaxAbsoluteAdjustment</code> and <code>Adjustment</code> both should have positive or both should have negative values.
                               </Condition>
                                <Error
                                    Description="Both Adjustment and MaxAbsoluteAdjustment should have positive or both should have negative values in the Pricing Rule Action" ErrorCode="YPM10147"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MaximumManualAdjustment" use="optional">
                <xsd:annotation yfc:columnName="MAX_MANUAL_ADJUSTMENT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>The maximum adjustment that can be applied as part of this manual rule action.
								<Validation>
                                <Condition>For a manual adjustment rule <code>Adjustment</code> and <code>MaximumManualAdjustment</code> are null.
									</Condition>
                                <Error
                                    Description="Both Adjustment and MaximumManualAdjustment cannot be null for a manual adjustment rule." ErrorCode="YPM10083"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule if <code>abs(MaximumManualAdjustment) &lt; abs(Adjustment)</code>.
									</Condition>
                                <Error
                                    Description="Absolute value of MaximumManualAdjustment cannot be less than absolute value of Adjustment." ErrorCode="YPM10094"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule if <code>MaximumManualAdjustment &gt; 0</code> when <code>Adjustment &lt; 0</code>.
									</Condition>
                                <Error
                                    Description="MaximumManualAdjustment cannot be an uplift when Adjustment is a discount." ErrorCode="YPM10096"/>
                            </Validation>
                            <Validation>
                                <Condition>For a manual adjustment rule if <code>MaximumManualAdjustment &lt; 0</code> when <code>Adjustment &gt; 0</code>.
									</Condition>
                                <Error
                                    Description="MaximumManualAdjustment cannot be a discount when Adjustment is an uplift." ErrorCode="YPM10098"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Operation to perform
								<br/>Valid Values: 
								<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
								<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record. 
								<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing record.
								<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
								<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderTotal" use="optional">
                <xsd:annotation yfc:columnName="ORDER_TOTAL" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>The Order total tier for the order total adjustments. Applicable only for rule type order total.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PricingRuleActionKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICING_RULE_ACTION_KEY" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>Primary key attribute of YPM_PRICING_RULE_ACTION table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QualifierAmount" use="optional">
                <xsd:annotation yfc:columnName="QUALIFIER_AMOUNT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>
                            <p>
                                <code>QualifierAmount</code> attribute is used as promotion qualifier. This attribute work along with <code>QualifierAmountType</code>.
					       		</p>
                            <Validation>
                                <Condition>
                                    <code>QualifierAmount</code> is not supported when <code>QualifierAmountType</code> is not defined.
					           </Condition>
                                <Error
                                    Description="QualifierAmountType is mandatory when QualifierAmount is specified and viceversa." ErrorCode="YPM10137"/>
                            </Validation>
                            <Validation>
                                <Condition>For order total rules either <code>QualifierAmount</code> or <code>OrderTotal</code> must have same values across all action tiers. For other rules either <code>QualifierAmount</code> or <code>Quantity</code> must have same values across all actions. 
					           </Condition>
                                <Error
                                    Description="Incase Order Total rule QualifierAmount and OrderTotal both have different values across action tiers.Or for other rules QualifierAmount and Quantity have different values across action tiers." ErrorCode="YPM10138"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="QualifierAmountType"
                type="yfctype:EnumQualifierAmountType" use="optional">
                <xsd:annotation yfc:columnName="QUALIFIER_AMOUNT_TYPE" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>
                            <p>
                                <code>QualifierAmountType</code> attribute is used as promotion qualifier. This attribute work along with <code>QualifierAmount</code> as described below.
					       		</p>
                            <Validation>
                                <Condition>
                                    <code>QualifierAmountType</code> is not supported when <code>QualifierAmount</code> is not defined.
					           </Condition>
                                <Error
                                    Description="QualifierAmountType is mandatory when QualifierAmount is specified and viceversa." ErrorCode="YPM10137"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>QualifierAmountType</code> must be same for all actions with in a pricing rule.
					           </Condition>
                                <Error
                                    Description="QualifierAmountType must be same for all actions with in a pricing rule." ErrorCode="YPM10139"/>
                            </Validation>
                            <Validation>
                                <Condition> Invalid QualifierAmountType. For Quantity and Shipping Sur-charge rules allowed values are <code>TARGET_MINIMUM_UNIT_PRICE and TARGET_MINIMUM_TOTAL_EXTENDED_PRICE</code>. For other types of rules allowed values are  <code> TRIGGER_MINIMUM_TOTAL_EXTENDED_PRICE, TRIGGER_MINIMUM_UNIT_PRICE, TARGET_MINIMUM_TOTAL_EXTENDED_PRICE and TARGET_MINIMUM_UNIT_PRICE</code>. 
					           </Condition>
                                <Error
                                    Description="Invalid QualifierAmountType." ErrorCode="YPM10135"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" use="optional">
                <xsd:annotation yfc:columnName="QUANTITY" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>Quantity of triggering item for tiered actions. Applicable only for rule type item quantity.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TargetItemDeterminationRule"
                type="yfctype:EnumTargetItemDeterminationRule" use="optional">
                <xsd:annotation yfc:columnName="TARGET_ITEM_DETER_RULE" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Specifies, on which target item the action should be applied. Applicable Values are: <code>LOW_PRICED_ITEM</code>, <code>HIGH_PRICED_ITEM</code> and <code>ALL_ITEMS</code>. used when defining combination rules with target as All items in order, but excludes certain items.
 								<Validation>
                                <Condition>If the value for <code>TargetItemDeterminationRule</code> differs across action tiers
									</Condition>
                                <Error
                                    Description="TargetItemDeterminationRule must be set to the same value for all tiers" ErrorCode="YPM10133"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TargetOccurrenceLimit"
                type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The value will be considered as the maximum number of times a particular discount can be applied in a transaction.
					       <Validation>
                                <Condition> Passed <code>TargetOccurrenceLimit</code> must be set to the same value for all actions when <code>AdjustmentType</code> is set to <code>FIXED_LINE_PRICE</code>
                                </Condition>
                                <Error
                                    Description="WhenToApply and TargetOccurrenceLimit must be set to the same values if AdjustmentType is FIXED_LINE_PRICE" ErrorCode="YPM10126"/>
                            </Validation>
                            <Validation>
                                <Condition> If <code>TargetOccurrenceLimit</code> is not set to the highest <code>Quantity</code> or a multiple of the quantities for each action
                               </Condition>
                                <Error
                                    Description="TargetOccurrenceLimit must be equal to the highest quantity tier or a multiple of quantity tiers when AdjustmentType is FIXED_LINE_PRICE" ErrorCode="YPM10128"/>
                            </Validation>
                            <Validation>
                                <Condition> If <code>WhenToApply</code> is set to <code>SPECIFIC_NUMBER</code> and <code>TargetOccurrenceLimit</code> is not set</Condition>
                                <Error
                                    Description="TargetOccurrenceLimit must be set if WhenToApply is set to SPECIFIC_NUMBER" ErrorCode="YPM10132"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>TargetOccurrenceLimit</code> is not allowed for Order total rule with <code>ABSOLUTE DiscountType</code> or for Shipping total rule.</Condition>
                                <Error
                                    Description="TargetOccurrenceLimit is not applicable for Order total rule with ABSOLUTE DiscountType or for Shipping total rule" ErrorCode="YPM10151"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="WhenToApply"
                type="yfctype:EnumRuleApplicationType" use="optional">
                <xsd:annotation yfc:columnName="WHEN_TO_APPLY" yfc:tableName="YPM_PRICING_RULE_ACTION">
                    <xsd:documentation>
                        <div>Indicates how to apply the action. Mandatory for every action. Defaulted for <b>
                                <code>SURCHARGE</code>
                            </b> type of rule and manual item level rules to <b>
                                <code>ALL</code>
                            </b> and <b>
                                <code>ORDER_TOTAL</code>
                            </b> type of rule to <b>
                                <code>SINGLE_ITEM</code>

                            </b> if not passed in the input.
								<div
                                class="portlet" style="width:90%">
                                <table border="0" cellpadding="0"
                                    cellspacing="0" class="table">
                                    <tr>
                                    <td class="tablecolumnheader"
                                    rowspan="2" width="10%">RuleType</td>
                                    <td class="tablecolumnheader"
                                    rowspan="2" width="10%">IsManualRule</td>
                                    <td align="center"
                                    class="tablecolumnheader" colspan="4">WhenToApply</td>
                                    </tr>
                                    <tr>
                                    <td align="center"
                                    class="tablecolumnheader" nowrap="true">
                                    <b>SINGLE_ITEM</b>
                                    </td>
                                    <td align="center"
                                    class="tablecolumnheader" nowrap="true">
                                    <b>ALL</b>
                                    </td>
                                    <td align="center"
                                    class="tablecolumnheader" nowrap="true">
                                    <b>EACH_COMBINATION</b>
                                    </td>
                                    <td align="center"
                                    class="tablecolumnheader" nowrap="true">
                                    <b>SPECIFIC_NUMBER</b>
                                    </td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">COMBINATION</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">No</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">COMBINATION</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Yes</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ITEM_QUANTITY</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">No</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ITEM_QUANTITY</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Yes</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">ORDER_TOTAL</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Yes or No</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                    <tr>
                                    <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">SURCHARGE</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">No</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Not Allowed</td>
                                    <td align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Allowed</td>
                                    </tr>
                                </table>
                            </div>
                            <Validation>
                                <Condition>Passed <code>WhenToApply</code> is not valid for the type of rule.
									</Condition>
                                <Error
                                    Description="Invalid value for WhenToApply." ErrorCode="YPM10040"/>
                            </Validation>
                            <Validation>
                                <Condition> Passed <code>WhenToApply</code> must be set to the same value for all actions when <code>AdjustmentType</code> is set to <code>FIXED_LINE_PRICE</code>
                                </Condition>
                                <Error
                                    Description="WhenToApply and TargetOccurrenceLimit must be set to the same values if AdjustmentType is FIXED_LINE_PRICE" ErrorCode="YPM10126"/>
                            </Validation>
                            <Validation>
                                <Condition>WhenToApply=SPECIFIC_NUMBER is not allowed for Order total rule with <code>ABSOLUTE DiscountType</code> or for Shipping total rule.
								    </Condition>
                                <Error
                                    Description="Invalid value for WhenToApply for Order total rule with ABSOLUTE DiscountType or for Shipping total rule." ErrorCode="YPM10150"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleLocaleList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PricingRuleLocale"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  				If <code>Reset</code> is passed as <b>
                                <code>Y</code>
                            </b>, the old records are removed and replaced with the new ones.
							If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                <code>N</code>
                            </b>, only the deltas are applied. 
				  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRuleLocale">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE_LOCALE">
            <xsd:documentation>The table is used to store localized information for the related pricing rule in different locales.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YPM_PRICING_RULE_LOCALE">
                    <xsd:documentation>The country associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Description"
                type="yfctype:PricingRuleDescription" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YPM_PRICING_RULE_LOCALE">
                    <xsd:documentation>Description of the rule.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Language" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="LANGUAGE" yfc:tableName="YPM_PRICING_RULE_LOCALE">
                    <xsd:documentation>The language associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								Valid value is <b>
                                <code>Delete</code>
                            </b>. Set <code>Operation</code> = "<b>
                                <code>Delete</code>
                            </b>" to delete the entity.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingRuleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE_LOCALE">
                    <xsd:documentation>The reference key to the YPM_PRICING_RULE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingRuleLocaleKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICING_RULE_LOCALE_KEY" yfc:tableName="YPM_PRICING_RULE_LOCALE">
                    <xsd:documentation>The primary key of the YPM_PRICING_RULE_LOCALE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Variant" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="VARIANT" yfc:tableName="YPM_PRICING_RULE_LOCALE">
                    <xsd:documentation>The variant associated with the locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRulePricelistExclusionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PricingRulePricelistExclusion"/>
            </xsd:sequence>
            <xsd:attribute name="Reset" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  				If <code>Reset</code> is passed as <b>
                                <code>Y</code>
                            </b>, the existing price lists are removed and replaced with the new ones passed in the input.
				  			If <code>Reset</code> is not passed or <code>Reset</code> is passed as <b>
                                <code>N</code>
                            </b>, only the changes specified in the input are applied.
				  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRulePricelistExclusion">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE_PRCLST_EXCL">
            <xsd:documentation>
                <div>
							This table maintains information about price lists that should be excluded by the pricing rule.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Operation to perform
							<br/>Valid Values: 
							<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Adds a new price list to exclude.
							<br/>
                            <b>
                                <code>Create</code>
                            </b> - Adds a new price list to exclude 
							<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Change values in an existing entry.
							<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing price list from the exclusion list.
							<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricelistHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICELIST_HDR_KEY" yfc:tableName="YPM_PRICING_RULE_PRCLST_EXCL">
                    <xsd:documentation>
                        <div> the price list header key of the price list to exclude
							    <p/>
                            <Validation>
                                <Condition> Either <code>PricelistHeaderKey</code> or <code>PricelistName</code> is not defined.
					           		</Condition>
                                <Error
                                    Description="PricelistHeaderKey or PricelistName is required" ErrorCode="YPM10001"/>
                            </Validation>
                            <Validation>
                                <Condition> Price list does not exist for the passed <code>PricelistHeaderKey</code> or <code>PricelistName</code>.
					           		</Condition>
                                <Error
                                    Description="Invalid price list identifiers" ErrorCode="YPM10016"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricelistName" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> the name of the price list to exclude
								<p/>
                            <Validation>
                                <Condition> Either <code>PricelistHeaderKey</code> or <code>PricelistName</code> is not defined.
					           		</Condition>
                                <Error
                                    Description="PricelistHeaderKey or PricelistName is required" ErrorCode="YPM10001"/>
                            </Validation>
                            <Validation>
                                <Condition> Price list does not exist for the passed <code>PricelistHeaderKey</code> or <code>PricelistName</code>.
					           		</Condition>
                                <Error
                                    Description="Invalid price list identifiers" ErrorCode="YPM10016"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingRulePricelistExclusionKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="PRICING_RULE_PRCLST_EXCL_KEY" yfc:tableName="YPM_PRICING_RULE_PRCLST_EXCL">
                    <xsd:documentation>Primary key attribute of YPM_PRICING_RULE_PRCLST_EXCL table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
