<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YIC/instantiateSubModel/output"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YIC/instantiateSubModel/output" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YIC_instantiateSubModel_output.xsd" yfc:rootElement="Model">
        <xsd:appinfo source="yfc:structure">
            <Model hasAttributes="Y">
                <Picks>
                    <Pick hasAttributes="Y">
                        <NestedConfiguration>
                            <ConfiguratorBOM hasAttributes="Y">
                                <message hasAttributes="Y"/>
                                <LineItem hasAttributes="Y">
                                    <property hasAttributes="Y"/>
                                    <LineItem hasAttributes="Y"/>
                                    <AdditionalLinePrices>
                                    <AdditionalLinePrice hasAttributes="Y"/>
                                    </AdditionalLinePrices>
                                </LineItem>
                            </ConfiguratorBOM>
                        </NestedConfiguration>
                        <NestedProperties>
                            <properties>
                                <property hasAttributes="Y"/>
                            </properties>
                        </NestedProperties>
                    </Pick>
                </Picks>
                <ParentConfigurations>
                    <ParentConfiguration hasAttributes="Y">
                        <InputProperties>
                            <Property hasAttributes="Y"/>
                        </InputProperties>
                        <ConfiguratorBOM hasAttributes="Y">
                            <message hasAttributes="Y"/>
                            <LineItem hasAttributes="Y">
                                <property hasAttributes="Y"/>
                                <LineItem hasAttributes="Y"/>
                                <AdditionalLinePrices>
                                    <AdditionalLinePrice hasAttributes="Y"/>
                                </AdditionalLinePrices>
                            </LineItem>
                        </ConfiguratorBOM>
                    </ParentConfiguration>
                </ParentConfigurations>
                <InputProperties>
                    <Property hasAttributes="Y"/>
                </InputProperties>
                <Messages>
                    <Error hasAttributes="Y"/>
                    <Warning hasAttributes="Y"/>
                    <Suggestion hasAttributes="Y"/>
                </Messages>
                <PropertyPool>
                    <Property hasAttributes="Y"/>
                </PropertyPool>
                <TraceLog>
                    <Log hasAttributes="Y"/>
                </TraceLog>
                <Tab hasAttributes="Y">
                    <Member hasAttributes="Y"/>
                </Tab>
                <Summary>
                    <Section hasAttributes="Y">
                        <Item hasAttributes="Y"/>
                    </Section>
                </Summary>
                <Property hasAttributes="Y"/>
                <Optionclass hasAttributes="Y">
                    <Property hasAttributes="Y"/>
                    <Optionitem hasAttributes="Y">
                        <Property hasAttributes="Y"/>
                    </Optionitem>
                </Optionclass>
            </Model>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Model" type="yfc:ModelXSDType">
        <xsd:annotation>
            <xsd:documentation>
                <div>
					The root xml element of the header.  
				</div>
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ModelXSDType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Picks"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ParentConfigurations"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="InputProperties">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					A holder of input properties applicable for the current model. It contains:
					<ol>
                                <li>External properties injected into the current model.</li>
                                <li>Properties derived from parent state, based on the input property list defined in the current model. 
						This information is initially captured while punching into the current model from its parent.</li>
                            </ol>
					These input property values are considered while evaluating the rules in the current model. 
				</div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" minOccurs="0" name="Property">
                            <xsd:annotation yfc:tableName="PLT_PROPERTY">
                                <xsd:documentation>
                                    <div>
						This element holds information about input properties for 
						the current model.
					</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:attribute name="External"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				  			The flag to indicate whether the property is an external input property or not. 
				  			Default value is <b>
                                    <code>N</code>
                                    </b>.
					  	</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Name"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							The name of the property in the property pool.				
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Path"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							The path to a property from the property pool.							
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Type"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							The datatype of the property.							
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Value"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							The value of the property.				
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Messages"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PropertyPool"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TraceLog"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Tab"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Summary"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Property">
                <xsd:annotation yfc:tableName="PLT_PROPERTY">
                    <xsd:documentation>
                        <div>
					This element holds information about properties associated to item.  
				</div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:attribute name="Name" type="yfctype:Text-480" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
						The name of the property.							
					</div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="Path" type="yfctype:Text-120" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
						The path to the item that contains the property.									
					</div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="Type" type="yfctype:Text-120" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
						The datatype of the property.
						<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                </div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="Value" type="yfctype:Text-480" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>
						The value in string form for the property. List values are returned with each value separated by a carriage return.		
					</div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Optionclass"/>
        </xsd:sequence>
        <xsd:attribute name="Country" type="yfctype:Text-120" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						The country identifier for the models.  The default country code is the users default country code.						
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Id" type="yfctype:Integer" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						Configurator item id of the node.  For any given model the item ids are unique within that model.  The id of the root node of a model is always 0.
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Language" type="yfctype:Text-120" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						The language identifier for the models to search for.  The default language is the users default language.				
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ModelDepth" type="yfctype:String" use="optional"/>
        <xsd:attribute name="Name" type="yfctype:Text-480" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						The name of the model.							
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Path" type="yfctype:Text-480" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						The path to the model.							
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="Picks">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Pick"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Pick">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							A pick node contains the internal information about a pick.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NestedConfiguration"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NestedProperties"/>
            </xsd:all>
            <xsd:attribute name="Item" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The configurator item name for a pick.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemId" type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The <code>Id</code> of the configurator item. This is applicable only
								for option items created dynamically through <code>instantiateSubModel</code> API. 
								This value is initially captured while creating child model instances using 
								<code>instantiateSubModel</code> API.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The quantity of the pick.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Type" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The type of action performed on the pick. For option items created dynamically 
								through <code>instantiateSubModel</code> API, the type will be <code>create</code>. 
								This value is initially captured while creating child model instances using 
								<code>instantiateSubModel</code> API.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Value" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The user entered value associated with the item.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NestedConfiguration">
        <xsd:annotation>
            <xsd:documentation>
                <div>
								The configuration of the entire sub-model tree associated with the option item. The nested 
								configuration includes all the configuration information from the descendants. This information 
								is initially captured while returning from the sub-model.
							</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="ConfiguratorBOM">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
					This element has the header information of configurator BOM.  
				</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="message">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							BOMs can contain messages about the status of the configuration and are produced by rules or constraint tables.  
						</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="item"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								Name of the message.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="type"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The type of the message in the configurator BOM.	
								<br/>Valid Values: <b>
                                    <code>error</code>
                                    </b> or <b>
                                    <code>warning</code>
                                    </b> or <b>
                                    <code>suggest</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="value"
                                    type="yfctype:Text-240" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The displayable value for the message.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="LineItem">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
						</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="property">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The holder of properties associated with the <code>LineItem</code>. It contains:
								<ol>
                                    <li>The Configurator Properties that are attached to the items in the model and 
									flagged as "output" properties.</li>
                                    <li>The external input properties injected into the model to influence its
									runtime behavior.</li>
                                    </ol>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="external"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The flag to differentiate externally injected input properties from output properties.
									The attribute value is <b>
                                    <code>Y</code>
                                    </b>, if the property is an externally injected
									input property. Default value is <b>
                                    <code>N</code>
                                    </b>.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="name"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Name of the property.							
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="type"
                                    type="yfctype:Integer" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The type of the property in the configurator BOM.	
									<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="value"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The value of the property in string form.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="LineItem">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
											LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="CommerceItemKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Description"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The description of the <code>LineItem</code>.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemID"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The item configurator ID for the item.							
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemKey"
                                    type="yfctype:Integer" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The visual modeler key for the item. This attribute is not considered in the input.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Name"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Price"
                                    type="yfctype:UnitPrice" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The price of the <code>LineItem</code>.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The quantity of the item ordered for the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RulePicked"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												This attribute is true if the item was picked by a rule otherwise false.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SKU"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The sku associated to the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UOM"
                                    type="yfctype:UOM" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The UOM associated to the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Visible"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="lockprice"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												This attribute is true if the price in the BOM should used in the order, otherwise false. 
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="uevValue"
                                    type="yfctype:Text-240" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The user entered value associated to the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="AdditionalLinePrices">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The holder of additional line prices associated with the <code>LineItem</code>. It contains one or more <code>AdditionalLinePrice</code> elements.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="AdditionalLinePrice">
                                    <xsd:annotation yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
										Contains the details of an additional line price type that is associated to the <code>LineItem</code>.
									</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="IsPriceLocked"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IS_PRICE_LOCKED" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
											This attribute is true if the price type value in the BOM should used in the order, otherwise false. 
											<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Price" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICE" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
											The price value of the price type.
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:UnitPrice"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PriceTypeName"
                                    type="yfctype:Text-240" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICE_TYPE_NAME" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
											The name of the price type						
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute
                                    name="CommerceItemKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Description"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The description of the <code>LineItem</code>.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ItemID"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The item configurator ID for the item.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ItemKey"
                                    type="yfctype:Integer" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The visual modeler key for the item.  This attribute is not considered in the input.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Name"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Price"
                                    type="yfctype:UnitPrice" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The price of the <code>LineItem</code>.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Quantity"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The quantity of the item ordered for the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RulePicked"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This attribute is true if the item was picked by a rule otherwise false.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SKU"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The sku associated to the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="UOM"
                                    type="yfctype:UOM" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The UOM associated to the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Visible"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="lockprice"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This attribute is true if the price in the BOM should used in the order, otherwise false. 
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="uevValue"
                                    type="yfctype:Text-240" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The user entered value associated to the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="ModelName"
                            type="yfctype:Text-254" use="required">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The full configurator name for the model.
						<Validation>
                                    <Condition>
                                    <code>ModelName</code> is not passed. </Condition>
                                    <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Version"
                            type="yfctype:PositiveInteger" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The current BOM version.					
                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="containerOnly"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						This attribute is true if the model root has the container only flag set otherwise false.
						<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="errors" type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						This attribute is true if there are errors in the BOM, otherwise false.
						<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NestedProperties">
        <xsd:annotation>
            <xsd:documentation>
                <div>
								The output properties from the sub-model and its descendants associated with the option item. This information 
								is initially captured while returning from the sub-model.
							</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:properties"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="properties">
        <xsd:annotation>
            <xsd:documentation>
                <div>
									The holder of output properties.
								</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="property">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
					This element holds information about properties associated to item.  
				</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="name"
                            type="yfctype:Text-480" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The name of the property.							
					</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="path"
                            type="yfctype:Text-120" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The path to the item that contains the property.									
					</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="type"
                            type="yfctype:Text-120" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The datatype of the property.
						<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="value"
                            type="yfctype:Text-480" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The value in string form for the property. List values are returned with each value separated by a carriage return.		
					</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ParentConfigurations">
        <xsd:annotation>
            <xsd:documentation>
                <div>
                    <div>
                        <p>The configuration of parent model(s) in the configuration path. The parent configuration
							at each level is captured separately under <code>&lt;parentConfiguration&gt;</code> element. 
							This information is initially captured while punching into the sub-model from the parent model.
							This configuration is applicable only for sub-models.</p>
                        <p>The <code>parentConfigurations</code> element should be resent by the caller as-is without 
							modification.</p>
                    </div>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ParentConfiguration"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ParentConfiguration">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							The node contains the configuration details of a parent model. It also captures input properties 
							for the model in this configuration.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="InputProperties">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
                                <p>A holder of input properties applicable for this particular parent configuration. It contains the properties 
								derived from its parent state in the preceding <code>&lt;ParentConfiguration&gt;</code>, based on the input 
								property list defined in the model in this configuration and/or external properties to this configuration when 
								it was configured.</p>
                            </div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="Property">
                                <xsd:annotation yfc:tableName="PLT_PROPERTY">
                                    <xsd:documentation>
                                    <div>
									This element holds information about input properties for the model in this configuration.
								</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="External"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							  			The flag to indicate whether the property is an external input property or not. 
							  			Default value is <b>
                                    <code>N</code>
                                    </b>.
								  	</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Name"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
										The name of the property in the property pool.				
									</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Path"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
										The path to a property from the property pool.							
									</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Type"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
										The datatype of the property.							
									</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Value"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
										The value of the property.	
									</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ConfiguratorBOM">
                    <xsd:annotation>
                        <xsd:documentation>
                            <div>
					This element has the header information of configurator BOM.  
				</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="message">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							BOMs can contain messages about the status of the configuration and are produced by rules or constraint tables.  
						</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="item"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								Name of the message.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="type"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The type of the message in the configurator BOM.	
								<br/>Valid Values: <b>
                                    <code>error</code>
                                    </b> or <b>
                                    <code>warning</code>
                                    </b> or <b>
                                    <code>suggest</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="value"
                                    type="yfctype:Text-240" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The displayable value for the message.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element maxOccurs="1" minOccurs="0" name="LineItem">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
						</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="property">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The holder of properties associated with the <code>LineItem</code>. It contains:
								<ol>
                                    <li>The Configurator Properties that are attached to the items in the model and 
									flagged as "output" properties.</li>
                                    <li>The external input properties injected into the model to influence its
									runtime behavior.</li>
                                    </ol>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="external"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The flag to differentiate externally injected input properties from output properties.
									The attribute value is <b>
                                    <code>Y</code>
                                    </b>, if the property is an externally injected
									input property. Default value is <b>
                                    <code>N</code>
                                    </b>.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="name"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									Name of the property.							
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="type"
                                    type="yfctype:Integer" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The type of the property in the configurator BOM.	
									<br/>Valid Values: <b>
                                    <code>bigdecimal</code>
                                    </b> or <b>
                                    <code>int</code>
                                    </b> or <b>
                                    <code>List</code>
                                    </b> or <b>
                                    <code>long</code>
                                    </b> or <b>
                                    <code>double</code>
                                    </b> or <b>
                                    <code>String</code>
                                    </b> or <b>
                                    <code>date</code>
                                    </b> or <b>
                                    <code>unknown</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="value"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The value of the property in string form.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="LineItem">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
											LineItems form a tree structure in that they can contain other line items.  The top most LineItem is the root of the model.  
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="CommerceItemKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Description"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The description of the <code>LineItem</code>.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemID"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The item configurator ID for the item.							
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemKey"
                                    type="yfctype:Integer" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The visual modeler key for the item. This attribute is not considered in the input.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Name"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Price"
                                    type="yfctype:UnitPrice" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The price of the <code>LineItem</code>.
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The quantity of the item ordered for the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="RulePicked"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												This attribute is true if the item was picked by a rule otherwise false.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="SKU"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The sku associated to the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="UOM"
                                    type="yfctype:UOM" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The UOM associated to the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="Visible"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="lockprice"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												This attribute is true if the price in the BOM should used in the order, otherwise false. 
												<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="uevValue"
                                    type="yfctype:Text-240" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
												The user entered value associated to the <code>LineItem</code>.								
											</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="AdditionalLinePrices">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
									The holder of additional line prices associated with the <code>LineItem</code>. It contains one or more <code>AdditionalLinePrice</code> elements.
								</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="AdditionalLinePrice">
                                    <xsd:annotation yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
										Contains the details of an additional line price type that is associated to the <code>LineItem</code>.
									</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="IsPriceLocked"
                                    type="yfctype:Flag" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="IS_PRICE_LOCKED" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
											This attribute is true if the price type value in the BOM should used in the order, otherwise false. 
											<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Price" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICE" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
											The price value of the price type.
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:UnitPrice"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="PriceTypeName"
                                    type="yfctype:Text-240" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="PRICE_TYPE_NAME" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                                    <xsd:documentation>
                                    <div>
											The name of the price type						
										</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute
                                    name="CommerceItemKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								An optional key for the item from an order.  These keys are preserved if it is passed in the input. It will be returned in the output.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Description"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The description of the <code>LineItem</code>.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ItemID"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The item configurator ID for the item.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ItemKey"
                                    type="yfctype:Integer" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The visual modeler key for the item.  This attribute is not considered in the input.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Name"
                                    type="yfctype:Text-480" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The unique name for the <code>LineItem</code> in a particular depth of the hierarchy.							
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Price"
                                    type="yfctype:UnitPrice" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The price of the <code>LineItem</code>.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Quantity"
                                    type="yfctype:Integer" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The quantity of the item ordered for the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RulePicked"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This attribute is true if the item was picked by a rule otherwise false.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="SKU"
                                    type="yfctype:Text-120" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The sku associated to the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="UOM"
                                    type="yfctype:UOM" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The UOM associated to the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Visible"
                                    type="yfctype:Flag" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This attribute is true if the item should be visible to the user in the order, otherwise false.  Very often models contain information needed for reconfiguration that should not be visible in the order.
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="lockprice"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								This attribute is true if the price in the BOM should used in the order, otherwise false. 
								<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="uevValue"
                                    type="yfctype:Text-240" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
								The user entered value associated to the <code>LineItem</code>.								
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                        <xsd:attribute name="ModelName"
                            type="yfctype:Text-254" use="required">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The full configurator name for the model.
						<Validation>
                                    <Condition>
                                    <code>ModelName</code> is not passed. </Condition>
                                    <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YIC10000"/>
                                    </Validation>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Version"
                            type="yfctype:PositiveInteger" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						The current BOM version.					
                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="containerOnly"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						This attribute is true if the model root has the container only flag set otherwise false.
						<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="errors" type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
						This attribute is true if there are errors in the BOM, otherwise false.
						<br/>Valid Values: <b>
                                    <code>true</code>
                                    </b> or <b>
                                    <code>false</code>
                                    </b>
                                    </div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="ModelDepth" type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The depth of the model with respect to the root model.					
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PunchInItem" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The path of the option item on which the punch was invoked. On return from sub model, the child configuration will be attached 
								to this item.						
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubModelReturn" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
				  			The <code>SubModelReturn</code> flag indicates whether end-users return from sub model to the model in this configuration.
					  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubModelValidate" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					  			The <code>SubModelValidate</code> flag indicating whether the child configuration is validated 
					  			on return from sub model.
						  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Messages">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Error"/>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Warning"/>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Suggestion"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Error">
        <xsd:annotation yfc:tableName="YFS_REPROCESS_ERROR">
            <xsd:documentation>
                <div>
							The xml element contains information of error messages.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Message" type="yfctype:Text-480" use="optional">
                <xsd:annotation yfc:columnName="MESSAGE" yfc:tableName="YFS_REPROCESS_ERROR">
                    <xsd:documentation>
                        <div>
								The message.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Path" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The path to the message.				
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Warning">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							The xml element contains information of warning messages.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Message" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The message.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Path" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The path to the message.				
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Suggestion">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							The xml element containes information of suggestion messages.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Message" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The message.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Path" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The path to the message.				
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PropertyPool">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							A holder of properties from the configurator state.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Property">
                    <xsd:annotation yfc:tableName="PLT_PROPERTY">
                        <xsd:documentation>
                            <div>
							The xml element containes information of suggestion messages.  
						</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="Name"
                            type="yfctype:Text-120" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								The name of the property in the property pool.				
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Path"
                            type="yfctype:Text-480" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								The path to a property from the property pool.							
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Type"
                            type="yfctype:Text-120" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								The datatype of the property.							
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Value"
                            type="yfctype:Text-480" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>
								The value of the property.				
							</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TraceLog">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Log"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Log">
        <xsd:complexType>
            <xsd:attribute name="Message" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The message of the rule trace.				
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Time" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The time of the rule trace.							
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Tab">
        <xsd:annotation>
            <xsd:documentation>
                <div>
						This element holds information about tabs.  
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Member"/>
            </xsd:sequence>
            <xsd:attribute name="Errors" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Flag indicating if the tab returned has errors or not.							
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GuidingText" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							The guiding text for the tab.							
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							The name of the tab.									
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suggestions" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Flag indicating if the tab returned has suggestions or not.							
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Warnings" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
							Flag indicating if the tab returned has warnings or not.							
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Member">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							This element holds information about the member option classes.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Class" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The name of a member option class.									
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Summary">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Section"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Section">
        <xsd:annotation>
            <xsd:documentation>
                <div>
							This element holds information about the section.  
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Item"/>
            </xsd:sequence>
            <xsd:attribute name="Name" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The name of the section.									
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Item">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>
                <div>
								This element holds information about the section item.  
							</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Description" type="yfctype:Text-480" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>
                        <div>
									The description of the section item.									
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Fullname" type="yfctype:Text-480" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
									The full name of the section item.								
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Id" type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
									Configurator item id of the node.  For any given model the item ids are unique within that model.  The id of the root node of a model is always 0.								
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
									The name of the section item.									
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Price" type="yfctype:Number" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
									The price of the section item.									
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
									The quantity of the section item.									
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Sku" type="yfctype:Text-120" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
									The sku of the section item.									
								</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Optionclass">
        <xsd:annotation>
            <xsd:documentation>
                <div>
						This element holds information about the option classes.  
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Property">
                    <xsd:annotation yfc:tableName="PLT_PROPERTY">
                        <xsd:documentation>
                            <div>
					This element holds information about properties associated to item.  
				</div>
                        </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="Name"
                            type="yfctype:Text-480" use="optional">
                            <xsd:annotation>
