<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YCM/ItemList/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YCM/ItemList/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YCM_ItemList_input.xsd" yfc:rootElement="Item">
        <xsd:appinfo source="yfc:structure">
            <Item hasAttributes="Y">
                <ItemServiceAssoc hasAttributes="Y"/>
                <CustomerInformation hasAttributes="Y"/>
                <PrimaryInformation hasAttributes="Y"/>
                <InventoryParameters hasAttributes="Y"/>
                <ClassificationCodes hasAttributes="Y"/>
                <ItemAliasList>
                    <ItemAlias hasAttributes="Y"/>
                </ItemAliasList>
                <ItemExclusionList>
                    <ItemExclusion hasAttributes="Y"/>
                </ItemExclusionList>
                <AdditionalAttributeList>
                    <AdditionalAttribute hasAttributes="Y"/>
                </AdditionalAttributeList>
                <CategoryFilter hasAttributes="Y"/>
                <Components>
                    <Component hasAttributes="Y"/>
                </Components>
                <LanguageDescriptionList>
                    <LanguageDescription hasAttributes="Y"/>
                </LanguageDescriptionList>
                <InventoryTagAttributes hasAttributes="Y"/>
                <ItemAttributeGroupTypeList>
                    <ItemAttributeGroupType hasAttributes="Y"/>
                </ItemAttributeGroupTypeList>
                <OrderBy>
                    <Attribute hasAttributes="Y"/>
                </OrderBy>
            </Item>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Item" type="yfc:ItemXSDType">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="ItemXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ComplexQuery"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemServiceAssoc"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CustomerInformation"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PrimaryInformation"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:InventoryParameters"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ClassificationCodes"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemAliasList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemExclusionList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AdditionalAttributeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CategoryFilter"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Components"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LanguageDescriptionList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:InventoryTagAttributes"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ItemAttributeGroupTypeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrderBy"/>
        </xsd:all>
        <xsd:attribute name="AuthenticationKey" type="yfctype:Key" use="optional">
            <xsd:annotation>
                <xsd:documentation>Not used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CallingOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>Calling Organization used to determine catalog organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CanUseAsServiceTool" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="CAN_USE_AS_SERVICE_TOOL" yfc:tableName="YFS_ITEM">
                <xsd:documentation>This flag indicates whether this item is configured to be a service tool.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DisplayLocalizedFieldInLocale"
            type="yfctype:Locale" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						The locale for which output should be returned in. Locale should be passed in the standard format of "language_country_variant" (e.g., "fr_FR", "en_US_CA", etc.).
					</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExcludeChildItemsOfModelItems"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			This flag indicates whether child items of an item with variations should be returned in the search results.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ExcludeItemsInPriceList"
            type="yfctype:String" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Items within this Pricelist will be excluded from the search results.</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FromGlobalItemID"
            type="yfctype:GlobalItemID" use="optional"/>
        <xsd:attribute name="FromItemGroupCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="FromItemID" type="yfctype:ItemID" use="optional"/>
        <xsd:attribute name="FromUnitOfMeasure" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="GetUnpublishedItems" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>Indicates if unpublished items needs to be listed.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GlobalItemID" type="yfctype:GlobalItemID" use="optional">
            <xsd:annotation yfc:columnName="GLOBAL_ITEM_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>Unique Global identifier used to cross reference an item with another catalog organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="GlobalItemIDQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="IgnoreInvalidItems" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			This flag indicates whether invalid items should be returned in the search results.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IgnoreOrdering" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
				By default, the output is ordered by <code>ORGANIZATION_CODE</code>, <code>ITEM_ID</code>, and <code>UNIT_OF_MEASURE</code>. Pass <code>IgnoreOrdering</code> as <b>
                            <code>Y</code>
                        </b> to omit ordering.
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InheritAttributesFromClassification"
            type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>If the InheritAttributesFromClassification flag is passed as Y and the value of the item attribute is not defined at item level, the system will try to find the value from the classification definition for the ClassificationPurposeCode - 'Inherited Item Attributes'.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsForOrdering" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
			This flag indicates whether the item search is for ordering purpose.
			Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
			Default value: <b>
                            <code>N</code>
                        </b>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemGroupCode" type="yfctype:Text-20" use="optional">
            <xsd:annotation yfc:columnName="ITEM_GROUP_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>
                    <div>
					Valid values: <b>
                            <code>PS</code>
                        </b> - Provided Service, <b>
                            <code>DS</code>
                        </b> - Delivery Service, <b>
                            <code>PROD</code>
                        </b> - Product Item
				</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemGroupCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
            <xsd:annotation yfc:columnName="ITEM_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>The unique identifier for the item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemIDQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="MaximumRecords" type="yfctype:Count" use="optional">
            <xsd:annotation>
                <xsd:documentation>Maximum records requested for output. Defaults to 5000.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
			If passed, items maintained by the <code>Organization</code> identified by the <code>OrganizationCode</code> are returned. This organization is the organization that maintains the catalog.
			</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequiresDGOverride" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="REQUIRES_DG_OVERRIDE" yfc:tableName="YFS_ITEM">
                <xsd:documentation>This flag indicates if the Default Distribution Rule has to be overridden for real time availability monitoring.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="StartItemKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ITEM_KEY" yfc:tableName="YFS_ITEM">
                <xsd:documentation>To start from a particular Item key.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SubCatalogOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="SUB_CAT_ORG_CODE" yfc:tableName="YFS_ITEM">
                <xsd:documentation>The sub-catalog organization that maintains the item definition.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ToGlobalItemID" type="yfctype:GlobalItemID" use="optional"/>
        <xsd:attribute name="ToItemGroupCode" type="yfctype:Text-20" use="optional"/>
        <xsd:attribute name="ToItemID" type="yfctype:ItemID" use="optional"/>
        <xsd:attribute name="ToUnitOfMeasure" type="yfctype:UOM" use="optional"/>
        <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
            <xsd:annotation yfc:columnName="UOM"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                <xsd:documentation>The unit of measure for item quantity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="UnitOfMeasureQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="UserOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>If passed, items are returned for the catalog organization of the passed <code>UserOrganizationCode</code>. This attribute is considered only if <code>OrganizationCode</code> is not passed.
				<Validation>
                            <Condition>No valid organization identified.
					</Condition>
                            <Error
                                Description="Invalid Organization Code." ErrorCode="YCM0019"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="ItemServiceAssoc">
        <xsd:annotation yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
            <xsd:documentation>All valid provided service items and delivery service associated to an item are maintained in this table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ServiceItemGroupCode"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_ITEM_GROUP_CODE" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>ItemGroupCode for the service or delivery item.  Valid values are PS - Provided Service, DS - Delivery Service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceItemId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_ITEM_ID" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>Provided service or delivery service item associated as option when selling the parent item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation
                    yfc:columnName="SERVICE_ORGANIZATION_CODE" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>Organization code of Service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_UOM" yfc:tableName="YFS_ITEM_SERVICE_ASSOC">
                    <xsd:documentation>UOM of the provided service or delivery service item associated to the parent item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CustomerInformation">
        <xsd:annotation yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
            <xsd:documentation>This is used to provide information about the customer for which items are being retrieved when entitlements need to be applied. CustomerID is used to get 
				customer information from the system. If both CustomerID and CustomerType are not populated, CustomerType is
				defaulted to <code>03 - Anonymous</code>. All other attributes are used as an override to apply corresponding entitlements. </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CustomerID" type="yfctype:CustomerId" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ID" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>The identifier of the customer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerLevel" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_LEVEL" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>Level of customer membership (Gold, Tin, Platinum, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerType" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_TYPE" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>
                        <div> The type of customer.
							<br/>Valid Values:
							<br/>01 - for B2B customer
							<br/>02 - for B2C customer
							<br/>03 - for anonymous customer 
							<br/>04 - for everyone
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Region" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="REGION_KEY" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>
                        <div> Region to which entitlement rule is assigned. </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RelationshipType"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>The type of the relationship that exists between an organization and an enterprise (distributor, retailer, reseller, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Vertical" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="VERTICAL" yfc:tableName="YFS_ENTITLE_RULE_ASSIGNMENT">
                    <xsd:documentation>The vertical customer belongs to.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PrimaryInformation">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AssumeInfiniteInventory"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="ASSUME_INFINITE_INVENTORY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is assumed to be always available. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ColorCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="COLOR_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes color code for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ConfiguredModelKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONFIGURED_MODEL_KEY" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The key of the configured model for a configurable item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CostCurrency" type="yfctype:Currency" use="optional">
                <xsd:annotation yfc:columnName="COST_CURRENCY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The currency in which the item's cost is defined.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CostCurrencyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The item's country of origin/manufacture.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CountryOfOriginQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DefaultProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_PRODUCT_CLASS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The default product class of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultProductClassQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Description" type="yfctype:Text-500" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DescriptionQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromCostCurrency"
                type="yfctype:Currency" use="optional"/>
            <xsd:attribute name="FromCountryOfOrigin"
                type="yfctype:Country" use="optional"/>
            <xsd:attribute name="FromDefaultProductClass"
                type="yfctype:ProductClass" use="optional"/>
            <xsd:attribute name="FromDescription"
                type="yfctype:Text-500" use="optional"/>
            <xsd:attribute name="FromIsAirShippingAllowed"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromIsForwardingAllowed"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromIsFreezerRequired"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromIsHazmat" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromIsProcurementAllowed"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FromItemType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromKitCode" type="yfctype:EnumKitCode" use="optional"/>
            <xsd:attribute name="FromManufacturerItem"
                type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="FromManufacturerItemDesc"
                type="yfctype:Text-255" use="optional"/>
            <xsd:attribute name="FromManufacturerName"
                type="yfctype:Text-50" use="optional"/>
            <xsd:attribute name="FromMasterCatalogID"
                type="yfctype:ID-40" use="optional"/>
            <xsd:attribute name="FromMaxOrderQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="FromMinOrderQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="FromMinimumCapacityQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="FromPricingQuantityConvFactor"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="FromPricingQuantityStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="FromPricingUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="FromPricingUOMStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="FromProductLine"
                type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="FromServiceTypeID"
                type="yfctype:Text-24" use="optional"/>
            <xsd:attribute name="FromShipmentConsolidationWindow"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromShortDescription"
                type="yfctype:Text-200" use="optional"/>
            <xsd:attribute name="FromStatus"
                type="yfctype:EnumShortStatus" use="optional"/>
            <xsd:attribute name="FromUnitCost" type="yfctype:UnitPrice" use="optional"/>
            <xsd:attribute name="FromUnitHeight"
                type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="FromUnitHeightUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="FromUnitLength"
                type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="FromUnitLengthUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="FromUnitWeight"
                type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="FromUnitWeightUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="FromUnitWidth" type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="FromUnitWidthUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="IsAirShippingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_AIR_SHIPPING_ALLOWED"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if item can be shipped by air. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsAirShippingAllowedQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsConfigurable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_CONFIGURABLE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the item is a configurable item. A configurable item can either be a bundle or a physical kit. A configurable bundle item offers a number of different options from which a customer can select before buying the product. Components for a bundle are not specified in the catalog as the actual components will be determined by the user when an order is placed. A physical kit must always be pre-configured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsDeliveryAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_DELIVERY_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the delivery method DEL is allowed for item. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsForwardingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FORWARDING_ALLOWED"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates if the item can be forwarded for zone skipping. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsForwardingAllowedQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsFreezerRequired" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FREEZER_REQUIRED"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if item requires freezer for transportation. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsFreezerRequiredQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsHazmat" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_HAZMAT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if item is hazardous material.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsHazmatQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsModelItem" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_MODEL_ITEM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the item is an item with variations (formerly known as a model item). An item with variations represents a group of items with similar set of attributes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsParcelShippingAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_PARCEL_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the item can be shipped through parcel service. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPickupAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PICKUP_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the delivery method PICK is allowed for item. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPreConfigured" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRE_CONFIGURED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates that the configurable item is pre-configured with selected options. If a bundle is pre-configured, the bundle or physical components that represent the selected option will be available in the catalog. For a pre-configured physical kit, only the physical kit will be available as the item in the catalog.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsProcurementAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PROCUREMENT_ALLOWED"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates if the item can be procured for zone skipping. Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsProcurementAllowedQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsReturnService" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_RETURN_SERVICE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether this provided or delivery service item is to be associated only to a return document.  Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsReturnable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_RETURNABLE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether this product item is returnable. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsShippingAllowed" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SHIPPING_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the delivery method SHP is allowed for item. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsStandaloneService"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_STANDALONE_SERVICE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether a PS Item is standalone and does not require associations with product lines for it to be included on an order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsSubOnOrderAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SUB_ON_ORDER_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates if during scheduling an order, substitution is allowed or not. If Y, schedule will automatically attempt to substitute an item with preferred or regular substitute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ITEM_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The generic type of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="KitCode" type="yfctype:EnumKitCode" use="optional">
                <xsd:annotation yfc:columnName="KIT_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates the kit code for this item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="KitCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ManufacturerItem"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The identifier used by a manufacturer to identify an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItemDesc"
                type="yfctype:Text-255" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_ITEM_DESC"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The description of an item, from a manufacturer's view point.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerItemDescQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ManufacturerItemQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ManufacturerName"
                type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="MANUFACTURER_NAME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The manufacturer of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturerNameQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="MasterCatalogID" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="MASTER_CATALOG_ID"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The master catalog to which an item belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MasterCatalogIDQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="MaxOrderQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="MAX_ORDER_QUANTITY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The maximum order quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxOrderQuantityQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="MinOrderQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation yfc:columnName="MIN_ORDER_QUANTITY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The minimum order quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinOrderQuantityQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="MinimumCapacityQuantity"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="MINIMUM_CAPACITY_QUANTITY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This is the minimum capacity quantity which will be assumed when  the inventory quantity of the line to arrive at the capacity quantity when the strategy is to arrive at the capacity quantity by association with product line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinimumCapacityQuantityQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ModelItemUnitOfMeasure"
                type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="MODEL_ITEM_UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes the unit of measure of the item with variations.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NumSecondarySerials"
                type="yfctype:Digit" use="optional">
                <xsd:annotation yfc:columnName="NUM_SECONDARY_SERIALS" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The number of secondary serial numbers applicable for a serialized item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQuantityConvFactor"
                type="yfctype:Quantity" use="optional">
                <xsd:annotation
                    yfc:columnName="PRICING_QUANTITY_CONV_FACTOR"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The pricing quantity conversion factor. This conversion factor is multiplied with the inventory quantity of the line to arrive at this pricing quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQuantityConvFactorQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PricingQuantityStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="PRICING_QUANTITY_STRATEGY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the pricing quantity is arrived at when different from the inventory quantity.   Valid values: IQTY - Based on the inventory quantity.  Default value: IQTY.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingQuantityStrategyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PricingUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The pricing unit of measure, if different from the inventory unit of measure.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOMQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PricingUOMStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PRICING_UOM_STRATEGY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the pricing unit of measure is defaulted for the item. It could be same as the inventory UOM or could be different.  Valid values: INV - Same as the inventory UOM; ALT - Different from the inventory UOM.  Default value: INV.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingUOMStrategyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ProductLine" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_LINE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The product line associated with the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductLineQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="RequiresProdAssociation"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="REQUIRES_PROD_ASSOCIATION" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>IThis flag indicates whether the item must be associated to an order line having a group code of PROD. Applicable for service items which must be associated to a Product order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceTypeID" type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_TYPE_ID"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The service type associated to this service item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceTypeIDQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ShipmentConsolidationWindow" use="optional">
                <xsd:annotation yfc:columnName="SHP_CONSOL_WINDOW"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Indicates the number of days an item can be delayed after it becomes available to be consolidated to a single shipment. Used by scheduling when shipment/cost optimization is considered.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ShipmentConsolidationWindowQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="ShortDescription"
                type="yfctype:Text-200" use="optional">
                <xsd:annotation yfc:columnName="SHORT_DESCRIPTION"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The short description of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShortDescriptionQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="SizeCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SIZE_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes the size code of the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Status" type="yfctype:EnumShortStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates the status of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is taxable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToCostCurrency" type="yfctype:Currency" use="optional"/>
            <xsd:attribute name="ToCountryOfOrigin"
                type="yfctype:Country" use="optional"/>
            <xsd:attribute name="ToDefaultProductClass"
                type="yfctype:ProductClass" use="optional"/>
            <xsd:attribute name="ToDescription" type="yfctype:Text-500" use="optional"/>
            <xsd:attribute name="ToIsAirShippingAllowed"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToIsForwardingAllowed"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToIsFreezerRequired"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToIsHazmat" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToIsProcurementAllowed"
                type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToItemType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToKitCode" type="yfctype:EnumKitCode" use="optional"/>
            <xsd:attribute name="ToManufacturerItem"
                type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="ToManufacturerItemDesc"
                type="yfctype:Text-255" use="optional"/>
            <xsd:attribute name="ToManufacturerName"
                type="yfctype:Text-50" use="optional"/>
            <xsd:attribute name="ToMasterCatalogID" type="yfctype:ID-40" use="optional"/>
            <xsd:attribute name="ToMaxOrderQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="ToMinOrderQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="ToMinimumCapacityQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="ToPricingQuantityConvFactor"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="ToPricingQuantityStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="ToPricingUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="ToPricingUOMStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="ToProductLine" type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="ToServiceTypeID" type="yfctype:Text-24" use="optional"/>
            <xsd:attribute name="ToShipmentConsolidationWindow"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToShortDescription"
                type="yfctype:Text-200" use="optional"/>
            <xsd:attribute name="ToStatus"
                type="yfctype:EnumShortStatus" use="optional"/>
            <xsd:attribute name="ToUnitCost" type="yfctype:UnitPrice" use="optional"/>
            <xsd:attribute name="ToUnitHeight" type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="ToUnitHeightUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="ToUnitLength" type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="ToUnitLengthUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="ToUnitWeight" type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="ToUnitWeightUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="ToUnitWidth" type="yfctype:Dimension" use="optional"/>
            <xsd:attribute name="ToUnitWidthUOM" type="yfctype:UOM" use="optional"/>
            <xsd:attribute name="UnitCost" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation yfc:columnName="UNIT_COST"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The cost of one unit of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitCostQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="UnitHeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_HEIGHT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The height of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitHeightQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="UnitHeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_HEIGHT_UOM"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which item height is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitHeightUOMQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="UnitLength" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_LENGTH"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The length of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitLengthQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="UnitLengthUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_LENGTH_UOM"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which an item length is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitLengthUOMQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="UnitWeight" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WEIGHT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The weight of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWeightQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="UnitWeightUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WEIGHT_UOM"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which the item weight is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWeightUOMQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="UnitWidth" type="yfctype:Dimension" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WIDTH"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The width of one unit of item quantity.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWidthQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="UnitWidthUOM" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UNIT_WIDTH_UOM"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The unit of measure in which an item's width is measured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitWidthUOMQryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InventoryParameters">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ATPRule" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ATP_RULE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The available to promise rule to be used for the item to determine availability.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ATPRuleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="AdvanceNotificationTime" use="optional">
                <xsd:annotation
                    yfc:columnName="ADVANCE_NOTIFICATION_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The advance notification time for this item. Communication to ship the order to shipping node/supplier is made at most this many days before the expected shipment date. Used during the release process. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="AdvanceNotificationTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="CapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="CAPACITY_QUANTITY_STRATEGY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field determines how the capacity quantity is arrived at for provided and delivery services from the inventory quantity.   Valid values: ORD - Based on the ordered service quantity; ASSOC - Based on the service and product association.  Default value: ORD.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CapacityQuantityStrategyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DefaultExpirationDays"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_EXPIRATION_DAYS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The default for number of days after which an item would expire.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultExpirationDaysQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="FromATPRule" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromAdvanceNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromCapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="FromDefaultExpirationDays"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromFutureSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="FromInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromLeadTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromMaximumNotificationTime"
                type="yfctype:NumberOfHours" use="optional"/>
            <xsd:attribute name="FromMinNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromNodeLevelInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromOnhandSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="FromOnhandSafetyFactorQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="FromProcessingTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="FromTimeSensitive" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="FutureSafetyFactorPercentage" use="optional">
                <xsd:annotation
                    yfc:columnName="FUTURE_SAFETY_FACTOR_PCT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The future safety factor percentage of this item.  The FutureSafetyFactorPercentage will be excluded from the actual future availability of the item if the scheduling rule, node type, and supply type are configured to apply future safety factor.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Percentage"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FutureSafetyFactorPercentageQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="InventoryMonitorRule"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="INVENTORY_MONITOR_RULE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The monitoring rule that will be used by the availability monitor.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InventoryMonitorRuleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="IsFifoTracked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FIFO_TRACKED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is FIFO tracked. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsItemBasedAllocationAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_IBA_ALLOWED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the item is allowed for Item Based Allocation process.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsSerialTracked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_SERIAL_TRACKED" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is to be serial tracked in inventory.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LeadTime" use="optional">
                <xsd:annotation yfc:columnName="LEAD_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The lead time for getting the product delivered. Purchase order placed today will arrive after this many days at the warehouse. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LeadTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="MaximumNotificationTime" use="optional">
                <xsd:annotation yfc:columnName="MAX_NOTIFICATION_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The maximum notification time for this item. Communication to ship the order to shipping node/supplier is made at most this many shipping calendar hours before the expected shipment date. Used during the release process. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfHours"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MaximumNotificationTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="MinNotificationTime" use="optional">
                <xsd:annotation yfc:columnName="MIN_NOTIFICATION_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The minimum notification time for this item. Communication to ship the order should be made at least this many days before the expected shipment date. Used to determine the earliest ship date on the line. Default value: 0.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfDays"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MinNotificationTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="NodeLevelInventoryMonitorRule"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="NODE_LEVEL_INV_MONITOR_RULE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The monitoring rule that will be used by the real-time availability monitor for node-level monitoring.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NodeLevelInventoryMonitorRuleQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OnhandSafetyFactorPercentage" use="optional">
                <xsd:annotation
                    yfc:columnName="ONHAND_SAFETY_FACTOR_PCT"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The on-hand safety factor percentage of this item.  The OnhandSafetyFactorPercentage will be excluded from the actual on-hand availability of the item if the scheduling rule, node type, and supply type are configured to apply on-hand safety factor.   OnhandSafetyFactorPercentage will be used only when OnhandSafetyFactorQuantity is not specified.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Percentage"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OnhandSafetyFactorPercentageQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="OnhandSafetyFactorQuantity" use="optional">
                <xsd:annotation
                    yfc:columnName="ONHAND_SAFETY_FACTOR_QTY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The on-hand safety factor quantity of this item.  The OnhandSafetyFactorQuantity will be excluded from the actual on-hand availability of the item if the scheduling rule, node type, and supply type are configured to apply on-hand safety factor.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Quantity"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="OnhandSafetyFactorQuantityQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ProcessingTime"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation yfc:columnName="PROCESSING_TIME"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Define processing time for items in days such as special services.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProcessingTimeQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="SerializedFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SERIALIZED_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether an item is serialized.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TagControlFlag"
                type="yfctype:EnumTagControlFlag" use="optional">
                <xsd:annotation yfc:columnName="TAG_CONTROL_FLAG" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field indicates whether an item is tag controlled. Inventory module uses this flag to determine if additional inventory attributes need to be stored for the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TimeSensitive" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TIME_SENSITIVE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indiactes whether the item is time-sensitive. Inventory module uses this to determine if inventory is dated. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TimeSensitiveQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToATPRule" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToAdvanceNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToCapacityQuantityStrategy"
                type="yfctype:Text-20" use="optional"/>
            <xsd:attribute name="ToDefaultExpirationDays"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToFutureSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="ToInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToLeadTime" type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToMaximumNotificationTime"
                type="yfctype:NumberOfHours" use="optional"/>
            <xsd:attribute name="ToMinNotificationTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToNodeLevelInventoryMonitorRule"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOnhandSafetyFactorPercentage"
                type="yfctype:Percentage" use="optional"/>
            <xsd:attribute name="ToOnhandSafetyFactorQuantity"
                type="yfctype:Quantity" use="optional"/>
            <xsd:attribute name="ToProcessingTime"
                type="yfctype:NumberOfDays" use="optional"/>
            <xsd:attribute name="ToTimeSensitive" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="UseUnplannedInventory"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="USE_UNPLANNED_INV" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the item is allowed to use unplanned inventory. Default value: N.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ClassificationCodes">
        <xsd:annotation yfc:tableName="YFS_ITEM">
            <xsd:documentation>The table stores product information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CommodityCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="COMMODITY_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The commodity code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommodityCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ECCNNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ECCN_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The ECCN number of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ECCNNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromCommodityCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromECCNNo" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromHarmonizedCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromHazmatClass" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromNAICSCode" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromNMFCClass" type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="FromNMFCCode" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromPickingType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromPostingClassification"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromSchedule_B_Code"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromStorageType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromTaxProductCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromUNSPSC" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="HarmonizedCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="HARMONIZED_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The harmonized code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HarmonizedCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="HazmatClass" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="HAZMAT_CLASS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The hazardous material classification code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HazmatClassQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Model" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="MODEL" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This field describes the identifier of the item with variations to which this item belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NAICSCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NAICS_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The NAICS code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NAICSCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="NMFCClass" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CLASS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The NMFC class of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCClassQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="NMFCCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NMFC_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The NMFC code for an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NMFCCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OperationalConfigurationComplete"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="OPERATIONAL_CONFIG_COMPLETE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>This flag indicates whether the operational configuration is complete.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickingType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="PICKING_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The picking type code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickingTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PostingClassification"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="POSTING_CLASSIFICATION"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The posting classification. This will have the item grouping value for the Financial Ledger groups.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PostingClassificationQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Schedule_B_Code" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCHEDULE_B_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The schedule B code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Schedule_B_CodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="StorageType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="STORAGE_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The storage type code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StorageTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="TaxProductCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_PRODUCT_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The tax product code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxProductCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToCommodityCode" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToECCNNo" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToHarmonizedCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToHazmatClass" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToNAICSCode" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToNMFCClass" type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="ToNMFCCode" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToPickingType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToPostingClassification"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToSchedule_B_Code"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToStorageType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToTaxProductCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToUNSPSC" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="UNSPSC" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UNSPSC"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>The UNSPSC code of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UNSPSCQryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAliasList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemAlias"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAlias">
        <xsd:annotation yfc:tableName="YFS_ITEM_ALIAS">
            <xsd:documentation>This table is used to cross-reference item id of one organization to another. It also stores all aliases for an item. One of the aliases can be designated as a "cross-referencing" alias.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AliasName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ALIAS_NAME" yfc:tableName="YFS_ITEM_ALIAS">
                    <xsd:documentation>Alias type. For example, UPC, EAN, MFG. CODE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AliasValue" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="ALIAS_VALUE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_ALIAS">
                    <xsd:documentation>Alias value for the corresponding alias name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AliasValueQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromAliasValue" type="yfctype:Text-100" use="optional"/>
            <xsd:attribute name="ToAliasValue" type="yfctype:Text-100" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemExclusionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemExclusion"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemExclusion">
        <xsd:annotation yfc:tableName="YFS_ITEM_EXCLUSION">
            <xsd:documentation>Used to store exclusion codes associated with an item. Can be used to store list of countries, customers etc that are excluded from ordering an item. This element is not enabled for complex query.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ExclusionCode" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="EXCLUSION_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_EXCLUSION">
                    <xsd:documentation>Exclusion code for an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExclusionCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromExclusionCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToExclusionCode" type="yfctype:Text-40" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalAttributeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:AdditionalAttribute"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalAttribute">
        <xsd:annotation yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
            <xsd:documentation>Stores dynamic attributes attached with an entity. Currently used to store additional attributes for item and category entities.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="DoubleValue" use="optional">
                <xsd:annotation yfc:columnName="DOUBLE_VALUE" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>The value of the assigned attribute whose data type is 'DECIMAL'.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FromValue" type="yfctype:Text-2000" use="optional"/>
            <xsd:attribute name="IntegerValue" use="optional">
                <xsd:annotation yfc:columnName="INTEGER_VALUE" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>The value of the assigned attribute whose data type is 'INTEGER'.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Number20"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="IsInherited" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_INHERITED" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>This field indicates whether the assignment of attribute value as additional attribute was based on inheritance. For ParentTable='YFS_CATEGORY', a value of 'Y' indicates that the assignment is based on inheritance of the value assigned to the upper level category. For ParentTable='YFS_ITEM', a value of 'Y' indicates that the assignment is based on inheritance of the value assigned to the category to which the item belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="NAME" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>Name of the dynamic attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToValue" type="yfctype:Text-2000" use="optional"/>
            <xsd:attribute name="Value" type="yfctype:Text-2000" use="optional">
                <xsd:annotation yfc:columnName="VALUE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ADDITIONAL_ATTRIBUTE">
                    <xsd:documentation>Value of the attribute.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValueQryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CategoryFilter">
        <xsd:annotation yfc:tableName="YFS_CATEGORY">
            <xsd:documentation>Used to filter items based on category. This element is not enabled for complex query.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CategoryPath" type="yfctype:Text-800" use="optional">
                <xsd:annotation yfc:columnName="CATEGORY_PATH"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_CATEGORY">
                    <xsd:documentation>The path for the category.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CategoryPathQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromCategoryPath"
                type="yfctype:Text-800" use="optional"/>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_CATEGORY">
                    <xsd:documentation>The code for the organization this category is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ToCategoryPath" type="yfctype:Text-800" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Components">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Component"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Component">
        <xsd:annotation yfc:tableName="YFS_KIT_ITEM">
            <xsd:documentation>A Component Definition.  In order to correctly identify a component item, either <code>ComponentOrganizationCode</code>, <code>ComponentItemID</code> and <code>ComponentUnitOfMeasure</code> (if not blank) must be passed, or <code>ComponentItemKey</code> must be passed.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ComponentItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Along with ComponentOrganizationCode and ComponentUnitOfMeasure, it identifies a component item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComponentItemKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ITEM_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Component item's primary key</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComponentItemKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ComponentOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Organization that maintains catalog.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ComponentUnitOfMeasure"
                type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ITEM">
                    <xsd:documentation>Unit of measure for the Component Item</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FromComponentItemKey"
                type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToComponentItemKey" type="yfctype:Key" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LanguageDescriptionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LanguageDescription"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LanguageDescription">
        <xsd:annotation yfc:tableName="YFS_LANGUAGE_DESCRIPTION">
            <xsd:documentation>This table stores description in different languages.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="FromLocaleCode" type="yfctype:Locale" use="optional"/>
            <xsd:attribute name="LocaleCode" type="yfctype:Locale" use="optional">
                <xsd:annotation yfc:columnName="LOCALE_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_LANGUAGE_DESCRIPTION">
                    <xsd:documentation>Locale for which the extended description is stored.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LocaleCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToLocaleCode" type="yfctype:Locale" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InventoryTagAttributes">
        <xsd:annotation yfc:tableName="YFS_ITEM_TAG">
            <xsd:documentation>This table stores and defines the Tag Identifier and Tag Descriptor attributes for Tag Controlled Items.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BatchNo" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="BATCH_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Identifier or a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 02 - Use as Tag Identifier, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BatchNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromBatchNo" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromItemTagKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="FromLotAttribute1"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotAttribute2"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotAttribute3"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotKeyReference"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromLotNumber" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromManufacturingDate"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="FromRevisionNo" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ItemTagKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ITEM_TAG_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Primary key attribute of YFS_ITEM_TAG.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemTagKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotAttribute1" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE1"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute1QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotAttribute2" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE2"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute2QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotAttribute3" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_ATTRIBUTE3"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotAttribute3QryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotKeyReference" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_KEY_REFERENCE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotKeyReferenceQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="LotNumber" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_NUMBER"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Identifier or a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 02 - Use as Tag Identifier, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LotNumberQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ManufacturingDate"
                type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="LOT_MANUFACTURE_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ManufacturingDateQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="RevisionNo" type="yfctype:Text-2" use="optional">
                <xsd:annotation yfc:columnName="REVISION_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_ITEM_TAG">
                    <xsd:documentation>Specify whether this attibute can be used as a Tag Identifier or a Tag Descriptor.  Valid values:  01 - Use as Tag Descriptor, 02 - Use as Tag Identifier, 03 - Not Use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RevisionNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToBatchNo" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToItemTagKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToLotAttribute1" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotAttribute2" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotAttribute3" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotKeyReference"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToLotNumber" type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToManufacturingDate"
                type="yfctype:Text-2" use="optional"/>
            <xsd:attribute name="ToRevisionNo" type="yfctype:Text-2" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAttributeGroupTypeList">
        <xsd:annotation>
            <xsd:documentation>
                <div>
				A list of ItemAttributeGroupType can be passed to be used as filter criteria to control the item attribute group types to return in the output XML.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ItemAttributeGroupType"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ItemAttributeGroupType">
        <xsd:annotation yfc:tableName="YFS_ITEM_ATTR_GROUP_TYPE">
            <xsd:documentation>This table stores item attribute group types.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ClassificationPurposeCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation
                    yfc:columnName="CLASSIFICATION_PURPOSE_CODE" yfc:tableName="YFS_ITEM_ATTR_GROUP_TYPE">
                    <xsd:documentation>The classification purpose this group type is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemAttributeGroupType"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ATTR_GROUP_TYPE" yfc:tableName="YFS_ITEM_ATTR_GROUP_TYPE">
                    <xsd:documentation>The group type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderBy">
        <xsd:annotation>
            <xsd:documentation>
                <div>
				If OrderBy element is passed, items returned by the API are sorted in the same order as Attribute elements passed.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:Attribute"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Attribute">
        <xsd:annotation yfc:tableName="YFS_ATTRIBUTE">
            <xsd:documentation>This table stores attribute definitions. Attributes represent characteristics of entities and can be assigned to an attribute group.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Desc" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Sort in decending order.
						Valid values: <b>
                            <code>Y</code>
                        </b> or <b>
                            <code>N</code>
                        </b>
						Default value: <b>
                            <code>N</code>
                        </b>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Name" type="yfctype:Text-32" use="required">
                <xsd:annotation>
                    <xsd:documentation>Name of the attribute to use for sorting the results.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ComplexQuery">
        <xsd:complexType>
            <xsd:sequence maxOccurs="unbounded" minOccurs="1">
                <xsd:choice>
                    <xsd:element maxOccurs="1" minOccurs="1" name="And" type="yfc:ComplexQueryAnd"/>
                    <xsd:element maxOccurs="1" minOccurs="1" name="Or" type="yfc:ComplexQueryOr"/>
                    <xsd:element maxOccurs="1" minOccurs="1" name="Exp" type="yfc:ComplexQueryExp"/>
                </xsd:choice>
            </xsd:sequence>
            <xsd:attribute name="Operator" use="optional">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:enumeration value=""/>
                        <xsd:enumeration value="AND"/>
                        <xsd:enumeration value="And"/>
                        <xsd:enumeration value="and"/>
                        <xsd:enumeration value="OR"/>
                        <xsd:enumeration value="Or"/>
                        <xsd:enumeration value="or"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:complexType name="ComplexQueryAnd">
        <xsd:sequence maxOccurs="unbounded" minOccurs="1">
            <xsd:choice>
                <xsd:element maxOccurs="1" minOccurs="1" name="And" type="yfc:ComplexQueryAnd"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Or" type="yfc:ComplexQueryOr"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Exp" type="yfc:ComplexQueryExp"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ComplexQueryOr">
        <xsd:sequence maxOccurs="unbounded" minOccurs="1">
            <xsd:choice>
                <xsd:element maxOccurs="1" minOccurs="1" name="And" type="yfc:ComplexQueryAnd"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Or" type="yfc:ComplexQueryOr"/>
                <xsd:element maxOccurs="1" minOccurs="1" name="Exp" type="yfc:ComplexQueryExp"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ComplexQueryExp">
        <xsd:attribute name="Name" type="xsd:string" use="required"/>
        <xsd:attribute name="Value" type="xsd:string" use="optional"/>
        <xsd:attribute name="QryType" use="optional">
            <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value=""/>
                    <xsd:enumeration value="EQ"/>
                    <xsd:enumeration value="LIKE"/>
                    <xsd:enumeration value="FLIKE"/>
                    <xsd:enumeration value="NE"/>
                    <xsd:enumeration value="GE"/>
                    <xsd:enumeration value="LE"/>
                    <xsd:enumeration value="GT"/>
                    <xsd:enumeration value="LT"/>
                    <xsd:enumeration value="ISNULL"/>
                    <xsd:enumeration value="NOTNULL"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
    </xsd:complexType>
</xsd:schema>
