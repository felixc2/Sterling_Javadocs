    <xsd:annotation yfc:fileName="YPM_getPricelistLineList_output.xsd" yfc:rootElement="PricelistLineList">
        <xsd:appinfo source="yfc:structure">
            <PricelistLineList hasAttributes="Y">
                <PricelistLine hasAttributes="Y">
                    <PricelistHeader hasAttributes="Y"/>
                    <PricelistLineQuantityTierList hasAttributes="Y">
                        <PricelistLineQuantityTier hasAttributes="Y"/>
                    </PricelistLineQuantityTierList>
                </PricelistLine>
            </PricelistLineList>
        </xsd:appinfo>
