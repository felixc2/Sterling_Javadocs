<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/WMS/getLocationInventoryAuditList/input"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/WMS/getLocationInventoryAuditList/input" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation
        yfc:fileName="WMS_getLocationInventoryAuditList_input.xsd" yfc:rootElement="LocationInventoryAudit">
        <xsd:appinfo source="yfc:structure">
            <LocationInventoryAudit hasAttributes="Y">
                <TagDetail hasAttributes="Y"/>
                <InventoryItem hasAttributes="Y"/>
                <Receipt hasAttributes="Y"/>
                <CountRequest hasAttributes="Y"/>
            </LocationInventoryAudit>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="LocationInventoryAudit" type="yfc:LocationInventoryAuditXSDType">
        <xsd:annotation yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
            <xsd:documentation>Stores audit information for every single location inventory transaction.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="LocationInventoryAuditXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TagDetail"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:InventoryItem"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Receipt"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CountRequest"/>
        </xsd:all>
        <xsd:attribute name="AdjustmentType"
            type="yfctype:EnumAdjustmentType" use="optional">
            <xsd:annotation yfc:columnName="ADJUSTMENT_TYPE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The type of adjustment done for the inventory processed. Some of the possible values are like RECEIPT, RETURN, SHIPMENT and ADJUSTMENT.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AdjustmentTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AuditOperation" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="OPERATION"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>This flag indicates whether it was a an addition to or a substraction of inventory.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AuditOperationQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="BolNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="BOL_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The Bill of Lading number. An audit reference indicating that this inventory transaction was for a BOL is detailed here.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BolNoQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CaseId" type="yfctype:LPNNo" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						A CaseId
							<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CountRequestKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="COUNT_REQUEST_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The count request key that is associated with this location inventory audit. This specifies the count request against which the audit is written.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CountRequestKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="CountryOfOrigin" type="yfctype:Country" use="optional">
            <xsd:annotation yfc:columnName="COUNTRY_OF_ORIGIN"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The country in which the item was manufactured.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CountryOfOriginQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Createprogid" type="yfctype:Createprogid" use="optional">
            <xsd:annotation yfc:columnName="CREATEPROGID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The program that created this record.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CreateprogidQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Createts" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="CREATETS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The timestamp for the creation of this record. 
					If createts is not passed, it will take today as the default date and returns audits written today.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CreatetsQryType" type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="Createuserid" type="yfctype:Createuserid" use="optional">
            <xsd:annotation yfc:columnName="CREATEUSERID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The user who created this record.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CreateuseridQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="DocumentType" type="yfctype:Text-24" use="optional">
            <xsd:annotation yfc:columnName="DOCUMENT_TYPE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The document type for the inventory transaction.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DocumentTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ENTERPRISE_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The inventory organization code.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="EnterpriseCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="FifoNo" type="yfctype:Integer" use="optional">
            <xsd:annotation yfc:columnName="FIFO_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>A date based inventory attribute that helps understand the order in which stock arrived at the node. This is used to send out items that arrived earlier than the ones that arrived later.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="FifoNoQryType" type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="FromAdjustmentType"
            type="yfctype:EnumAdjustmentType" use="optional"/>
        <xsd:attribute name="FromAuditOperation" type="yfctype:Flag" use="optional"/>
        <xsd:attribute name="FromBolNo" type="yfctype:VarcharDocumentNo" use="optional"/>
        <xsd:attribute name="FromCountRequestKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromCountryOfOrigin" type="yfctype:Country" use="optional"/>
        <xsd:attribute name="FromCreateprogid"
            type="yfctype:Createprogid" use="optional"/>
        <xsd:attribute name="FromCreatets" type="yfctype:TimeStamp" use="optional"/>
        <xsd:attribute name="FromCreateuserid"
            type="yfctype:Createuserid" use="optional"/>
        <xsd:attribute name="FromDocumentType" type="yfctype:Text-24" use="optional"/>
        <xsd:attribute name="FromEnterpriseCode" type="yfctype:OrgCode" use="optional"/>
        <xsd:attribute name="FromFifoNo" type="yfctype:Integer" use="optional"/>
        <xsd:attribute name="FromInventoryItemKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromInventoryStatus"
            type="yfctype:InventoryStatus" use="optional"/>
        <xsd:attribute name="FromInventoryTagKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromLocationId" type="yfctype:LocationId" use="optional"/>
        <xsd:attribute name="FromLockid" type="yfctype:Lockid" use="optional"/>
        <xsd:attribute name="FromLocnInventoryAuditKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromModifyprogid"
            type="yfctype:Modifyprogid" use="optional"/>
        <xsd:attribute name="FromModifyts" type="yfctype:TimeStamp" use="optional"/>
        <xsd:attribute name="FromModifyuserid"
            type="yfctype:Modifyuserid" use="optional"/>
        <xsd:attribute name="FromNode" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromOrderNo"
            type="yfctype:VarcharDocumentNo" use="optional"/>
        <xsd:attribute name="FromPrimeLineNo" type="yfctype:LineNo" use="optional"/>
        <xsd:attribute name="FromQuantity" type="yfctype:Quantity" use="optional"/>
        <xsd:attribute name="FromReasonCode" type="yfctype:ReasonCode" use="optional"/>
        <xsd:attribute name="FromReasonText" type="yfctype:ReasonText" use="optional"/>
        <xsd:attribute name="FromReceiptHeaderKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromReference1" type="yfctype:Text-50" use="optional"/>
        <xsd:attribute name="FromReference2" type="yfctype:Text-50" use="optional"/>
        <xsd:attribute name="FromReference3" type="yfctype:Text-50" use="optional"/>
        <xsd:attribute name="FromReference4" type="yfctype:Text-254" use="optional"/>
        <xsd:attribute name="FromReference5" type="yfctype:Text-254" use="optional"/>
        <xsd:attribute name="FromReleaseNo" type="yfctype:Count" use="optional"/>
        <xsd:attribute name="FromSegment" type="yfctype:Segment" use="optional"/>
        <xsd:attribute name="FromSegmentType" type="yfctype:SegmentType" use="optional"/>
        <xsd:attribute name="FromSequenceNo" type="yfctype:LongSequence" use="optional"/>
        <xsd:attribute name="FromSerialNo" type="yfctype:SerialNo" use="optional"/>
        <xsd:attribute name="FromShipByDate" type="yfctype:Date" use="optional"/>
        <xsd:attribute name="FromShipmentNo" type="yfctype:DocumentNo" use="optional"/>
        <xsd:attribute name="FromTaskType" type="yfctype:TaskType" use="optional"/>
        <xsd:attribute name="InventoryItemKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_ITEM_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The inventory item key that is associated with this location inventory audit. This points to a SKU that comprises the Inventory Organization, Item ID, Product Class, and Unit of Measure.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryItemKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="InventoryStatus"
            type="yfctype:InventoryStatus" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_STATUS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The status in which inventory is sub-classified once it is received in the warehouse. For example, product class is a classification of inventory that the product comes with when it arrives at the warehouse. Typical values of product class are First Quality and Second Quality. Due to additional inventory processes within the warehouse, it is required to further classify inventory. Typical inventory status values are Normal, Awaiting Inventory Check, Inventory Held, etc.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryStatusQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="InventoryTagKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_TAG_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The inventory tag key that is associated with this location inventory audit. This points to the lot and related attributes (revision, etc.) for the item whose inventory is tracked here.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryTagKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="LocationId" type="yfctype:LocationId" use="optional">
            <xsd:annotation yfc:columnName="LOCATION_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The identifier for the location. This, in conjunction with NODE_KEY, identifies a unique location in the node.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LocationIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Lockid" type="yfctype:Lockid" use="optional">
            <xsd:annotation yfc:columnName="LOCKID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>An integer used to track and prevent concurrent modifications.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LockidQryType" type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="LocnInventoryAuditKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="LOCN_INVENTORY_AUDIT_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The primary key for the YFS_LOCN_INVENTORY_AUDIT table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LocnInventoryAuditKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="MaximumRecords" type="yfctype:Count" use="optional">
            <xsd:annotation>
                <xsd:documentation>Specifies maximum number of records that must be fetched.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Modifyprogid" type="yfctype:Modifyprogid" use="optional">
            <xsd:annotation yfc:columnName="MODIFYPROGID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The program that modified this record.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ModifyprogidQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Modifyts" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="MODIFYTS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The timestamp for the last modification of this record.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ModifytsQryType" type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="Modifyuserid" type="yfctype:Modifyuserid" use="optional">
            <xsd:annotation yfc:columnName="MODIFYUSERID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The user who last modified this record.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ModifyuseridQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Node" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="NODE_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The node key that is associated with this location inventory audit.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NodeQryType" type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OrderNo" type="yfctype:VarcharDocumentNo" use="optional">
            <xsd:annotation yfc:columnName="ORDER_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The order # against which inventory transaction was made. This is a part of the unique key for the order.The Enterprise code and the order # together uniquely identify an order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrderNoQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OuterMostCaseId" type="yfctype:LPNNo" use="optional">
            <xsd:annotation>
                <xsd:documentation>Outer most case in the containment hierarchy</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OuterMostPalletId" type="yfctype:LPNNo" use="optional">
            <xsd:annotation>
                <xsd:documentation>Outer most Pallet in the containment hierarchy</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PalletId" type="yfctype:LPNNo" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>
						A PalletId
							<Validation>
                            <Condition>Both <code>CaseId</code> and <code>PalletId</code> cannot be passed. Error if both <code>CaseId</code> and <code>PalletId</code> are given.
								</Condition>
                            <Error
                                Description="Only One of Case/Pallet Can Be Used." ErrorCode="WMS00028"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
            <xsd:annotation yfc:columnName="PRIME_LINE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The order line number. This is part of an order line's unique key.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimeLineNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="Quantity" type="yfctype:Quantity" use="optional">
            <xsd:annotation yfc:columnName="QUANTITY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The number of units of the item available at this location.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="QuantityQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
            <xsd:annotation yfc:columnName="REASON_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The reason for the inventory transaction. This field is typically populated when the user does an inventory adjustment. Inventory bins, which will be used to adjust inventory (for inventory synchronization) on the host systems, will be tied to this reason code.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReasonCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReasonText" type="yfctype:ReasonText" use="optional">
            <xsd:annotation yfc:columnName="REASON_TEXT"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The description of the reason for the inventory adjustment.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReasonTextQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReceiptHeaderKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="RECEIPT_HEADER_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The receipt header key that is associated with this location inventory audit. This helps identify all stocks attached to a receipt. This number is of significance until the sample from the receipt has been certified as having passed or failed QC. This number is used to pull out all stocks based on the receipt. This is usually flled in situations where the LPN does not have contents.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReceiptHeaderKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Reference1" type="yfctype:Text-50" use="optional">
            <xsd:annotation yfc:columnName="REFERENCE_1"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>Generic references.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Reference1QryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Reference2" type="yfctype:Text-50" use="optional">
            <xsd:annotation yfc:columnName="REFERENCE_2"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>Generic references.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Reference2QryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Reference3" type="yfctype:Text-50" use="optional">
            <xsd:annotation yfc:columnName="REFERENCE_3"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>Generic references.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Reference3QryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Reference4" type="yfctype:Text-254" use="optional">
            <xsd:annotation yfc:columnName="REFERENCE_4"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>Generic references.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Reference4QryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Reference5" type="yfctype:Text-254" use="optional">
            <xsd:annotation yfc:columnName="REFERENCE_5"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>Generic references.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Reference5QryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReleaseNo" type="yfctype:Count" use="optional">
            <xsd:annotation yfc:columnName="RELEASE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The release number. This is generated as a one up number against that release.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReleaseNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="Segment" type="yfctype:Segment" use="optional">
            <xsd:annotation yfc:columnName="SEGMENT"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The segment to which the inventory belongs. This helps to track inventory separately when the inventory is customized for a specific order so that the inventory can be allocated to that order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SegmentQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="SegmentType" type="yfctype:SegmentType" use="optional">
            <xsd:annotation yfc:columnName="SEGMENT_TYPE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The segment type of the inventory. This field indicates the buyer or buyer category for whom the inventory is customized.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SegmentTypeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="SequenceNo" type="yfctype:LongSequence" use="optional">
            <xsd:annotation yfc:columnName="SEQUENCE_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The sequence number of the program creating the audit.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SequenceNoQryType"
            type="yfctype:NumberQryType" use="optional"/>
        <xsd:attribute name="SerialNo" type="yfctype:SerialNo" use="optional">
            <xsd:annotation yfc:columnName="SERIAL_NO"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_LOCN_INVENTORY_AUDIT">
                <xsd:documentation>The serial of the inventory being modified.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SerialNoQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ShipByDate" type="yfctype:Date" use="optional">
            <xsd:annotation yfc:columnName="SHIP_BY_DATE"
