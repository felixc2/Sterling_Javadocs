                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NumOfCartonsQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="NumOfPallets" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="NUM_OF_PALLETS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The number of pallets in the receipt.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NumOfPalletsQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="OpenReceiptFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="OPEN_RECEIPT_FLAG"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>This flag indicates whether the receiving can be done against the receipt. Receiving can be done only for open receipts.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OpenReceiptFlagQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Link to YFS_ORDER_HEADER table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderHeaderKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OrderNo"
                type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="ORDER_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Order number of the order for the Shipment while creating with SingleOrder.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="OrderReleaseKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_RELEASE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Link to YFS_ORDER_RELEASE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderReleaseKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PickticketNo"
                type="yfctype:VarcharID-24" use="optional">
                <xsd:annotation yfc:columnName="PICKTICKET_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>User input. A number assigned to group related shipments under one head.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PickticketNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PipelineKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PIPELINE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The pipeline to which this receipt belongs. This is determined during receipt creation based on the document type and several other key attributes of the shipment and order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PipelineKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PodNo" type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="POD_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Proof of Delivery #.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PodNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="PrimeLineNo" type="yfctype:LineNo" use="optional">
                <xsd:annotation yfc:columnName="PRIME_LINE_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT_LINE">
                    <xsd:documentation>PrimeLineNo of the corresponding OrderLine.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PrimeLineNoQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="ProNo" type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Progessive number assigned by a carrier to track a shipment (usually for LTL shipments).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReceiptDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="RECEIPT_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The date when the receipt was started.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceiptDateQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="ReceiptHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="RECEIPT_HEADER_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The primary key for the YFS_RECEIPT_HEADER table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceiptHeaderKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReceiptNo" type="yfctype:ReceiptNo" use="optional">
                <xsd:annotation yfc:columnName="RECEIPT_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The unique key or business key, which by itself identifies a receipt along with ReceivingNode.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceiptNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReceivingDock" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RECEIVING_DOCK_ID"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The location where the receiving is done.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingDockQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReceivingNode" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="RECEIVINGNODE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The Receiving Node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingNodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ReleaseNo" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="RELEASE_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>ReleaseNo of the OrderRelease for the Shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReleaseNoQryType"
                type="yfctype:NumberQryType" use="optional"/>
            <xsd:attribute name="Scac" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="SCAC"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Carrier by which this shipment will be transported.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="SealNo"
                type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="SEAL_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Seal number on the load.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SealNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="SellerOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation
                    yfc:columnName="SELLER_ORGANIZATION_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Seller organization to which this shipment belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SellerOrganizationCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Ship node from where shipment was made.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ShipmentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The shipment key that is associated with this receipt header.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentKeyQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ShipmentNo" type="yfctype:DocumentNo" use="optional">
                <xsd:annotation yfc:columnName="SHIPMENT_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Part of Unique key or business key.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipmentNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Status" type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="STATUS"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The status of the receipt. As various transactions in the receipts pipeline act upon Receipt, the Receipt Status changes. Typical values are 1200-Receipt Started, 1300-Receipt In Progress, 1400-Inspection In Progress, etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="STATUS_DATE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The date and time when the last status change took place.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="StatusDateQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="StatusQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToActualShipmentDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToArrivalDateTime"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToBolNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="ToBuyerOrganizationCode"
                type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="ToCarrierServiceCode"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToDriverName" type="yfctype:Text-50" use="optional"/>
            <xsd:attribute name="ToEnterpriseCode"
                type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="ToExpectedDeliveryDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToExpectedShipmentDate"
                type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToNextAlertTs" type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToNumOfCartons" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="ToNumOfPallets" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="ToOpenReceiptFlag" type="yfctype:Flag" use="optional"/>
            <xsd:attribute name="ToOrderHeaderKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToOrderNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="ToOrderReleaseKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToPickticketNo"
                type="yfctype:VarcharID-24" use="optional"/>
            <xsd:attribute name="ToPipelineKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToPodNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="ToPrimeLineNo" type="yfctype:LineNo" use="optional"/>
            <xsd:attribute name="ToProNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="ToReceiptDate" type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToReceiptHeaderKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToReceiptNo" type="yfctype:ReceiptNo" use="optional"/>
            <xsd:attribute name="ToReceivingDock" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="ToReceivingNode" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToReleaseNo" type="yfctype:Count" use="optional"/>
            <xsd:attribute name="ToScac" type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="ToSealNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="ToSellerOrganizationCode"
                type="yfctype:OrgCode" use="optional"/>
            <xsd:attribute name="ToShipNode" type="yfctype:ShipNode" use="optional"/>
            <xsd:attribute name="ToShipmentKey" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="ToShipmentNo" type="yfctype:DocumentNo" use="optional"/>
            <xsd:attribute name="ToStatus" type="yfctype:ShortStatus" use="optional"/>
            <xsd:attribute name="ToStatusDate" type="yfctype:TimeStamp" use="optional"/>
            <xsd:attribute name="ToTrackingNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="ToTrailerLPNNo" type="yfctype:ID-40" use="optional"/>
            <xsd:attribute name="ToTrailerNo"
                type="yfctype:VarcharDocumentNo" use="optional"/>
            <xsd:attribute name="TrackingNo"
                type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="TRACKING_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TrackingNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="TrailerLPNNo" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="TRAILER_LPN_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_RECEIPT_HEADER">
                    <xsd:documentation>The License Plate Number of the Trailer.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TrailerLPNNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="TrailerNo"
                type="yfctype:VarcharDocumentNo" use="optional">
                <xsd:annotation yfc:columnName="TRAILER_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_SHIPMENT">
                    <xsd:documentation>Trailer number by which the shipment leaves the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TrailerNoQryType"
                type="yfctype:StringQryType" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CountRequest">
        <xsd:annotation yfc:tableName="YFS_COUNT_REQUEST">
            <xsd:documentation>This transaction table stores count requests to perform inventory count in a warehouse. The count process in a warehouse begins with the count request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CountRequestNo"
                type="yfctype:DocumentNo" use="optional">
                <xsd:annotation yfc:columnName="COUNT_REQUEST_NO"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_COUNT_REQUEST">
                    <xsd:documentation>Identifier of the request within the context of a document type and node</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CountRequestNoQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DocumentType" type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_COUNT_REQUEST">
                    <xsd:documentation>Purpose for the request.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FromCountRequestNo"
                type="yfctype:DocumentNo" use="optional"/>
            <xsd:attribute name="FromDocumentType"
                type="yfctype:Text-24" use="optional"/>
            <xsd:attribute name="FromNode" type="yfctype:Key" use="optional"/>
            <xsd:attribute name="Node" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NODE_KEY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_COUNT_REQUEST">
                    <xsd:documentation>Node where the request was processed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="ToCountRequestNo"
                type="yfctype:DocumentNo" use="optional"/>
            <xsd:attribute name="ToDocumentType" type="yfctype:Text-24" use="optional"/>
            <xsd:attribute name="ToNode" type="yfctype:Key" use="optional"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
