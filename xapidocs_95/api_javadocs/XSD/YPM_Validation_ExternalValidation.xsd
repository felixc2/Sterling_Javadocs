<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.sterlingcommerce.com/documentation/YPM/Validation_ExternalValidation"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:yfc="http://www.sterlingcommerce.com/documentation/YPM/Validation_ExternalValidation" xmlns:yfctype="http://www.sterlingcommerce.com/documentation/types">
    <xsd:annotation yfc:fileName="YPM_Validation_ExternalValidation.xsd" yfc:rootElement="Coupon">
        <xsd:appinfo source="yfc:structure">
            <Coupon hasAttributes="Y">
                <Order hasAttributes="Y">
                    <SelectionCriteria hasAttributes="Y">
                        <PersonInfo hasAttributes="Y"/>
                    </SelectionCriteria>
                    <HeaderTaxes>
                        <HeaderTax hasAttributes="Y"/>
                        <TaxSummary>
                            <TaxSummaryDetail hasAttributes="Y"/>
                        </TaxSummary>
                    </HeaderTaxes>
                    <ManualRuleAdjustments>
                        <ManualRuleAdjustment hasAttributes="Y"/>
                    </ManualRuleAdjustments>
                    <ManualAdjustments>
                        <ManualAdjustment hasAttributes="Y"/>
                    </ManualAdjustments>
                    <Shipping hasAttributes="Y"/>
                    <ExternalCouponAdjustments>
                        <ExternalCouponAdjustment hasAttributes="Y"/>
                    </ExternalCouponAdjustments>
                    <OrderLines>
                        <OrderLine hasAttributes="Y">
                            <LineTaxes>
                                <LineTax hasAttributes="Y"/>
                                <TaxSummary>
                                    <TaxSummaryDetail hasAttributes="Y"/>
                                </TaxSummary>
                            </LineTaxes>
                            <ManualRuleAdjustments>
                                <ManualRuleAdjustment hasAttributes="Y"/>
                            </ManualRuleAdjustments>
                            <ManualAdjustments>
                                <ManualAdjustment hasAttributes="Y"/>
                            </ManualAdjustments>
                            <ExternalCouponAdjustments>
                                <ExternalCouponAdjustment hasAttributes="Y"/>
                            </ExternalCouponAdjustments>
                            <ModificationTypes>
                                <ModificationType hasAttributes="Y"/>
                            </ModificationTypes>
                            <Shipping hasAttributes="Y"/>
                            <AdditionalLinePrices>
                                <AdditionalLinePrice hasAttributes="Y">
                                    <ManualRuleAdjustments>
                                    <ManualRuleAdjustment hasAttributes="Y"/>
                                    </ManualRuleAdjustments>
                                    <ManualAdjustments>
                                    <ManualAdjustment hasAttributes="Y"/>
                                    </ManualAdjustments>
                                </AdditionalLinePrice>
                            </AdditionalLinePrices>
                        </OrderLine>
                    </OrderLines>
                    <Coupons>
                        <Coupon hasAttributes="Y">
                            <References>
                                <Reference hasAttributes="Y"/>
                            </References>
                        </Coupon>
                    </Coupons>
                    <ModificationTypes>
                        <ModificationType hasAttributes="Y"/>
                    </ModificationTypes>
                    <PaymentTypeList>
                        <PaymentType hasAttributes="Y"/>
                    </PaymentTypeList>
                    <TransactionViolationList>
                        <TransactionViolation hasAttributes="Y">
                            <TransactionApprovalStatusList hasAttributes="Y">
                                <TransactionApprovalStatus hasAttributes="Y">
                                    <TransactionApprover hasAttributes="Y"/>
                                </TransactionApprovalStatus>
                            </TransactionApprovalStatusList>
                            <TransactionViolationReferenceList hasAttributes="Y">
                                <TransactionViolationReference hasAttributes="Y"/>
                            </TransactionViolationReferenceList>
                            <TransactionInfo hasAttributes="Y"/>
                        </TransactionViolation>
                    </TransactionViolationList>
                </Order>
                <CouponStatusAttribute hasAttributes="Y"/>
                <PricingRules>
                    <PricingRule hasAttributes="Y"/>
                </PricingRules>
                <References>
                    <Reference hasAttributes="Y">
                        <CouponStatusAttribute hasAttributes="Y"/>
                    </Reference>
                </References>
            </Coupon>
        </xsd:appinfo>
    </xsd:annotation>
    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="Coupon" type="yfc:CouponXSDType">
        <xsd:annotation yfc:tableName="YPM_COUPON">
            <xsd:documentation>This table stores information about the coupon. The coupon behavior is defined by the pricing rule associated with it.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="CouponXSDType">
        <xsd:sequence>
            <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:Order"/>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="CouponStatusAttribute">
                <xsd:complexType>
                    <xsd:attribute name="Name" type="yfctype:Text-100" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>Name of the attribute on the coupon or in the input due to which the coupon is not applicable. </div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="Value" type="yfctype:Text-100" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>
                                <div>Value of the attribute.</div>
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PricingRules"/>
            <xsd:element maxOccurs="1" minOccurs="0" name="References">
                <xsd:complexType>
                    <xsd:all>
                        <xsd:element maxOccurs="1" minOccurs="0" name="Reference">
                            <xsd:annotation yfc:tableName="YFS_PROMOTION_REFERENCE">
                                <xsd:documentation>This table maintains references against applied promotion on order or order line. For example a unique Coupon barcode.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element maxOccurs="unbounded"
                                    minOccurs="0" name="CouponStatusAttribute">
                                    <xsd:complexType>
                                    <xsd:attribute name="Name"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Name of the attribute on the coupon or in the input due to which the coupon is not applicable. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Value"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Value of the attribute.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                                <xsd:attribute name="ReferenceId"
                                    type="yfctype:VARCHAR2-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_ID" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Reference value within a particular promotion. For example a unique coupon barcode.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="ReferenceSequence" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_SEQUENCE" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Sequence number for each reference entry.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:PrecisionDigits"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                </xsd:attribute>
                                <xsd:attribute
                                    name="ReferenceStatusMsgCode"
                                    type="yfctype:Text-100" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div class="portlet"
                                    style="width:90%">
						Indicates the status of the reference validation. Following table lists the possible reference status codes: 
						<table
                                    border="0"
                                    cellpadding="0"
                                    cellspacing="0" class="table">
                                    <tr>
                                    <td
                                    class="tablecolumnheader" width="20%">CouponStatusMsgCode</td>
                                    <td
                                    align="center"
                                    class="tablecolumnheader" width="40%">Description</td>
                                    <td
                                    align="center" class="tablecolumnheader">Coupon Status Attributes returned</td>
                                    </tr>
                                    <tr>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_CODE_VALID</td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The reference is valid.</td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                                    </tr>
                                    <tr>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_CODE_INVALID</td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The reference does not exist.</td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                                    </tr>
                                    <tr>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_CUSTOMER</td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The reference is not assigned to the <code>CustomerID</code>
                                    </td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                                    </tr>
                                    <tr>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_CUSTOMER_CONTACT</td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The reference is not assigned to the <code>CustomerContactID</code>
                                    </td>
                                    <td
                                    align="center"
                                    class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                                    </tr>
                                    </table>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="Valid"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Indicates whether the reference is valid or not. Valid values are Y|N</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                </xsd:attribute>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:all>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="CouponID" type="yfctype:CouponId" use="optional">
            <xsd:annotation yfc:columnName="COUPON_ID" yfc:tableName="YPM_COUPON">
                <xsd:documentation>
                    <div>Unique identifier of a coupon.
						<Validation>
                            <Condition>
                                <code>CouponID</code> is not passed. </Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CouponStatusMsgCode"
            type="yfctype:Text-100" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div class="portlet"
                            style="width:90%">
						Indicates the status of the coupon validation. Following table lists the possible coupon status codes: 
						<table
                            border="0" cellpadding="0" cellspacing="0" class="table">
                            <tr>
                                <td class="tablecolumnheader" width="20%">CouponStatusMsgCode</td>
                                <td align="center"
                                    class="tablecolumnheader" width="40%">Description</td>
                                <td align="center" class="tablecolumnheader">Coupon Status Attributes returned</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_VALID</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is valid.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_INVALID</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon does not exist.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_PARTIAL</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is partially valid.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_NOT_ACTIVE</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not active.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>PricingStatus</code> of the coupon</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_NOT_YET_EFFECTIVE</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not yet active.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>StartDateActive</code> of the coupon</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_EXPIRED</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is expired.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>EndDateActive</code> of the coupon</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_CUSTOMER</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not assigned to the <code>CustomerID</code>
                                </td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>CustomerID</code> passed in the input. A blank <code>CustomerID</code> indicates that the coupon has no assignments with void <code>CustomerID</code> for the <code>SelectionCriteria</code>.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_CUSTOMER_CONTACT</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not assigned to the <code>CustomerContactID</code>
                                </td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>CustomerContactID</code> passed in the input. A blank <code>CustomerContactID</code> indicates that the coupon has no assignments with void <code>CustomerContactID</code> for the <code>SelectionCriteria</code>.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_REGION</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not assigned to the passed <code>RegionKey</code> or <code>PersonInfo</code>
                                </td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>RegionKey</code> passed or derived based on <code>PersonInfo</code> from the input. A blank <code>RegionKey</code> indicates that the coupon has no assignments with void <code>RegionKey</code> for the <code>SelectionCriteria</code>.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_ATTRIBUTE</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not assigned to the passed customer attribute</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Any one of the input <code>CustomerType</code>, <code>RelationshipType</code>, <code>Vertical</code> or any extended attribute of the <code>YPM_PRICING_RULE_ASSIGNMENT</code> that didn't match the coupon assignments.  A blank attribute value indicates that the coupon is assigned to all seller organization.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_NOT_ASSIGNED_TO_SELLER</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not created by or assigned to the passed seller organization</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>SellerOrganizationCode</code> passed in the input. A blank <code>SellerOrganizationCode</code> indicates that the coupon has assignments with void <code>SellerOrganizationCode</code>.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_TARGET_SKU_MISSING</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The item on which this coupon applies adjustments does not exist in this order</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>TargetItemID</code> and <code>TargetUnitOfMeasure</code> of the coupon.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_CONDITION_NOT_SATISFIED</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon condition is not satisfied. Hence the rule is not fired.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_NOT_APPLICABLE</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not applicable. For example a coupon is passed and it is not applicable when <code>RunCatalogOnlyRules</code> is set to <b>
                                    <code>Y</code>
                                    </b>.</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">None.</td>
                            </tr>
                            <tr>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">YPM_RULE_BAD_PAYMENT_METHOD</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">The coupon is not applicable to the passed <code>PaymentType</code>in <code>PaymentTypeList</code> from the input.  </td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">
                                    <code>PaymentType</code> passed in <code>PaymentTypeList</code> from the input.</td>
                            </tr>
                        </table>
						Note: The above codes are returned by the default coupon validation logic. The user exit implementation of the coupon validation can return any code including the ones in the above table.
						</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsItemRule" type="yfctype:String" use="optional"/>
        <xsd:attribute name="Valid" type="yfctype:Flag" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Indicates whether the coupon is valid or not. This does
							not indicate whether the coupon is applicable or not. Refer
							to <code>CouponStatusMsgCode</code> to determine whether the
							coupon is applicable or not. Refer to the 
							<code>validateCoupon</code> API for details on how a coupon
							is determined to be valid.  If <code>ValidateReferences</code> was
							'<code>Y</code>' the value for this attribute can be Y|N|P. The
							value '<code>P</code>' means that the references for coupon are
							partially valid. Some of the references are valid and some of them
							are not.
							</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="Order">
        <xsd:annotation>
            <xsd:documentation>
                <div>Order to be priced.</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:SelectionCriteria"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:HeaderTaxes"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="ManualRuleAdjustments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ManualRuleAdjustment">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Manual rule adjustment that needs to be applied on the entire order. Both order level as well as item level manual rules can be passed here. If an item level rule is passed, the adjustment is applied only to the applicable order lines. If a rule adjustment is passed multilple times, then only the last one is considered.
							<Validation>
                                    <Condition>If both <code>PricingRuleName</code> and <code>PricingRuleKey</code> is void.</Condition>
                                    <Error
                                    Description="Either PricingRuleKey or PricingRuleName is required for ManualRuleAdjustment." ErrorCode="YPM10085"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Adjustment"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment to apply as part of this manual rule. The adjustment passed is evaluated agains the manual rule action that is applicable on this order. If the passed adjustment is void and there is an <code>Adjustment</code> specified on the manual rules' action, then the manual rule action's <code>Adjustment</code> is used.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>
                                    <div>Primary key of the manual rule.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingRuleName"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_NAME" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>
                                    <div>Name of the manual rule.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ManualAdjustments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ManualAdjustment">
                                <xsd:complexType>
                                    <xsd:attribute name="Adjustment"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment to be applied on the order.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="AdjustmentID"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment identifier for the manual adjustment.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeCategory"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>The charge category. For example, Labeling or Handling. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeName"
                                    type="yfctype:ChargeName" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>The name of the charge. For example, Price Ticket. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Reference information about charge. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Shipping">
                    <xsd:complexType>
                        <xsd:attribute name="Carrier"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>The carrier assigned to the order. This is a pass through field available to user exits.</div>.
					</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierServiceCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YPM_PRICING_RULE">
                                <xsd:documentation>The carrier service that this rule applies on. Applicable only for shipping category of rules.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MinimizeNumberOfShipments"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="MINIMIZE_NUMBER_OF_SHIPMENTS" yfc:tableName="YPM_PRICING_RULE">
                                <xsd:documentation>Indicates whether to optimize on the number of shipments or not. Applicable only for shipping category of rules.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShippingCharge"
                            type="yfctype:Price" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>Shipping charge for the order.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ExternalCouponAdjustments">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ExternalCouponAdjustment">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Adjustment available for external coupon.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Adjustment"
                                    type="yfctype:ExternalAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_AMOUNT" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The amount of discount or charge that was applied as a part of this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CouponID"
                                    type="yfctype:CouponId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COUPON_ID" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>
                                    <div>Coupon identifier.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="1" ref="yfc:OrderLines"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Coupons"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="ModificationTypes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ModificationType">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Provided as input so the overrideGetOrderPriceUE can utilize this information for external calculations.</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="ImpactsPricing"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Pass through attribute that can indicate if the modification type provided may impact pricing.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Level"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Pass through attribute that indicates the level in which the modification type is applicable.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Name"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Pass through attribute that indicates the type of Modification that is applicable to the order.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentTypeList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionViolationList"/>
            </xsd:all>
            <xsd:attribute name="ApplyOnlyItemLevelPricingRules"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  			If <code>ApplyOnlyItemLevelPricingRules</code> is passed as <b>
                                <code>Y</code>
                            </b> then rules that are applicable at the item level will be run. 
						<ul>
                                <li> Pricing rules with <code>IsItemRule</code> value as <b>
                                    <code>'Y'</code>
                                    </b> are executed.</li>
                                <li> Passed coupons with <code>IsItemRule</code> value as <b>
                                    <code>'Y'</code>
                                    </b> are executed.</li>
                                <li> Order total pricing rules are suppressed.</li>
                                <li> Shipping charge calculation, shipping rules execution and shipping coupon execution are suppressed.</li>
                            </ul>
						Default value is <b>
                                <code>N</code>
                            </b>. 
				  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BuyerUserId"
                type="yfctype:ContactUserId" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Currency" type="yfctype:Currency" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Currency used for pricing data selection.
		  				<Validation>
                                <Condition>
                                    <code>Currency</code> is not passed. </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>Currency</code> is not valid.</Condition>
                                <Error
                                    Description="YFS: Invalid currency." ErrorCode="YFS10325"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerContactID" type="yfctype:ID-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>An identifier of the customer. This is a pass through field available to user exits.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerID" type="yfctype:CustomerId" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_ID" yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
                    <xsd:documentation>
                        <div>The identifier for the customer.
						<Validation>
                                <Condition>If not void then <code>CustomerID</code> must be of one of the valid customer.</Condition>
                                <Error Description="Invalid CustomerID." ErrorCode="YPM10028"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentType"
                type="yfctype:DocumentType" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Document type for order. For use ONLY with the manager overrides framework in the context of an order. 
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnableManagerOverrides"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div> Indicates whether manager overrides should be considered. If enabled, DocumentType must be passed, and preexisting transaction violations may be passed. If disabled, neither document type nor transaction violations will be considered. This functionality is intended for order context only. 
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					The <code>EnterpriseCode</code> on which behalf pricing inquiry is being made. If not passed <code>OrganizationCode</code> primary enterprise will be used.
				</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrderReference" type="yfctype:ID-24" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						The <code>OrderReference</code> is the Order Header Key.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Pricing data is read on behalf of the organization specified by <code>OrganizationCode</code>.
						<Validation>
                                <Condition>
                                    <code>OrganizationCode</code> is not passed. </Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                            </Validation>
                            <Validation>
                                <Condition>
                                    <code>OrganizationCode</code> is not valid. </Condition>
                                <Error
                                    Description="YFS:Invalid organization code." ErrorCode="YFS10395"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingDate" type="yfctype:TimeStamp" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Date on which the pricing is requested. If void it is defaulted to current date and time.
				 	If <code>SellerOrganizationCode</code> is found and enterprise level rule <code>"ENABLE_SELLER_TIMEZONE_FOR_PRICE_CALCULATION"</code> is set then <code>PricingDate</code> passed in the input will be converted in to time zone of the Seller organization and pricing happens based on this converted date.
				 	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RepriceAllLines" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  			If <code>RepriceAllLines</code> is passed as <b>
                                <code>Y</code>
                            </b> then unit price for all the lines except priceLocked will be recalculated by the pricing engine. Default value is <b>
                                <code>N</code>
                            </b>.
				  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RunCatalogOnlyRules"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  			If <code>RunCatalogOnlyRules</code> is passed as <b>
                                <code>Y</code>
                            </b> then rules that are applicable in item listing, i.e. 
						<ul>
                                <li> pricing rules with <code>RuleType</code> as <b>
                                    <code>ITEM_QUANTITY</code>
                                    </b> are executed.</li>
                                <li> manual rules with <code>RuleType</code> as <b>
                                    <code>ITEM_QUANTITY</code>
                                    </b> are executed for suggestion.</li>
                                <li> passed coupons with <code>RuleCategory</code> as <b>
                                    <code>PRICING</code>
                                    </b> are executed.</li>
                                <li> combination and order total pricing rules are suppressed.</li>
                                <li> shipping charge calculation, shipping rules execution and shipping coupon execution are suppressed.</li>
                            </ul>
						Default value is <b>
                                <code>N</code>
                            </b>. 
				  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SellerOrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Pricing data is either maintained or assigned to the organization specified by <code>SellerOrganizationCode</code>.
						<Validation>
                                <Condition>
                                    <code>SellerOrganizationCode</code> is not passed. </Condition>
                                <Error
                                    Description="SellerOrganizationCode can not be blank if all pricing data is maintained by seller." ErrorCode="YPM10146"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuggestManualRuleAdjustments"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  			If <code>SuggestManualRuleAdjustments</code> is passed as <b>
                                <code>Y</code>
                            </b> then any manual adjustment rules applicablable are returned in the output XML under <code>ApplicableManualRules</code> and <code>ApplicableManualRuleSummary</code> elements. Default value is <b>
                                <code>N</code>
                            </b>.
				  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuppressAutomaticRuleExecution"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  			If <code>SuppressAutomaticRuleExecution</code> is passed as <b>
                                <code>Y</code>
                            </b> then pricing rules and shipping rules are not applied unless they are manual rules or coupon rules. Default value is <b>
                                <code>N</code>
                            </b>.
				  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuppressRuleExecution"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			  			If <code>SuppressRuleExecution</code> is passed as <b>
                                <code>Y</code>
                            </b> then pricing rules, manual rules, shipping rules and coupons are not applied. Default value is <b>
                                <code>N</code>
                            </b>.
				  	</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuppressShipping" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>If <code>SuppressShipping</code> is passed as <b>
                                <code>Y</code>
                            </b> then none of the shipping calculation is executed. This includes, shipping charge calculation and shipping rules execution. Default value is <b>
                                <code>N</code>
                            </b>.
			  		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SelectionCriteria">
        <xsd:annotation yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
            <xsd:documentation>

                <div>Attributes of customer used for selecting the pricing entities. If this element is not passed and <code>CustomerID</code> is passed then the attributes of the customer are defaulted from the customer's profile. 
				Defaulting of <code>PersonInfo</code> from the customer's profile for different customer type is as below:
					<div
                        class="portlet" style="width:90%">
                        <table border="0" cellpadding="0"
                            cellspacing="0" class="table">
                            <tr>
                                <td align="center"
                                    class="tablecolumnheader"
                                    style="border-bottom:1px solid #8C92C6"
                                    valign="top" width="20%">Type of Customer</td>
                                <td class="tablecolumnheader">Default for PersonInfo</td>
                            </tr>
                            <tr>
                                <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">Business</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Default sold to address, if void then default bill to address.</td>
                            </tr>
                            <tr>
                                <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">Consumer</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">Default bill to address.</td>
                            </tr>
                            <tr>
                                <td class="tablecolumn" style="border-bottom:1px solid #8C92C6">Anonymous</td>
                                <td align="center" class="tablecolumn"
                                    style="border-bottom:1px solid #8C92C6" valign="top">No defaults.</td>
                            </tr>
                        </table>
                    </div>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PersonInfo"/>
            </xsd:all>
            <xsd:attribute name="CustomerLevel" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_LEVEL" yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
                    <xsd:documentation>
                        <div>The level of a customer with the enterprise.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerType"
                type="yfctype:EnumInputCustomerType" use="optional">
                <xsd:annotation yfc:columnName="CUSTOMER_TYPE" yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
                    <xsd:documentation>
                        <div>Type of customer. If neither <code>CustomerID</code> nor <code>CustomerType</code> are passed then <code>CustomerType</code> is defaulted to <b>
                                <code>03</code>
                            </b>. 
						<Validation>
                                <Condition>If not void then <code>CustomerType</code> must be one of the allowed values.</Condition>
                                <Error
                                    Description="Invalid CustomerType." ErrorCode="YPM10027"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RegionKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="REGION_KEY" yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
                    <xsd:documentation>
                        <div>The region of the customer. If <code>RegionKey</code> is void then <code>PersonInfo</code> element is used to determine region of the customer.
					If both <code>RegionKey</code> and <code>PersonInfo</code> are not void then <code>RegionKey</code> takes precedence over <code>PersonInfo</code> to
					determine region of the customer.
						<Validation>
                                <Condition>If not void then <code>RegionKey</code> should be a valid region within the region schema that is configured for purpose <b>
                                    <code>SELLING</code>
                                    </b>.</Condition>
                                <Error
                                    Description="Invalid region passed." ErrorCode="YCP0158"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RelationshipType"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="RELATIONSHIP_TYPE" yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
                    <xsd:documentation>
                        <div>The type of the relationship that exists between an organization and an enterprise.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Vertical" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="VERTICAL" yfc:tableName="YPM_PRICELIST_ASSIGNMENT">
                    <xsd:documentation>
                        <div>Vertical of the customer.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PersonInfo">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>
                <div>Address of the customer.
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID"
                type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HttpUrl" type="yfctype:URL" use="optional">
                <xsd:annotation yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The URL of the person or organization referred to in this contact information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsCommercialAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_COMMERCIAL_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>Indicates whether the address is commercial, residential or none. Values of this field can be Y -Commercial address.N - Residential address. undefined for Null.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreferredShipAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PREFERRED_SHIP_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxGeoCode" type="yfctype:GeoCode" use="optional">
                <xsd:annotation yfc:columnName="TAX_GEO_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The tax geo code for this location. Intended for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderTaxes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:HeaderTax"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="TaxSummary">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="TaxSummaryDetail">
                                <xsd:complexType>
                                    <xsd:attribute name="InvoicedTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount of this tax which had been invoiced.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OverallTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount of this tax.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RemainingTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount of this tax that has not been invoiced.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TaxName"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Tax name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="HeaderTax">
        <xsd:annotation yfc:tableName="YFS_TAX_BREAKUP">
            <xsd:documentation>
                <div>
							Contains tax related information for various entities in the
							system. If
							<code>ChargeCategory</code>
							and
							<code>ChargeName</code>
							are not provided, tax will be considered as a billable and
							positive
							charge. If not implementing
							<code>YFSRecalculateLineTaxUE</code>
							,
							<code>ChargeCategory</code>
							,
							<code>Tax</code>
							, and
							<code>TaxPercentage</code>
							are required for the default repricing logic.
						</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The charge category for which tax is being charged. For example, Labeling or Handling.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Charge name for which tax is being charged. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeNameKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME_KEY" yfc:tableName="YFS_CHARGE_NAME">
                    <xsd:documentation>Primary key for a charge name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InvoicedTax" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="INVOICED_TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Tax amount invoiced.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference1" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 1</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference2" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 2</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference3" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE3" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 3</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RemainingTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Remaining Tax</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tax" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
								The tax amount.
								<code>Tax</code>
								cannot be less than zero. During the default repricing logic,
								this is recalculated using the
								<code>TaxPercentage</code>
								and the price of the related
								<code>ChargeCategory</code>
								. This can be overridden with the YFSRecalulateHeaderTaxUE.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The tax name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxPercentage" type="yfctype:TaxType" use="optional">
                <xsd:annotation yfc:columnName="TAX_PERCENTAGE" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Percentage amount for tax calculation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxableFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TAXABLE_FLAG" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>This flag indicates if a charge can be taxed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLines">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="1" ref="yfc:OrderLine"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrderLine">
        <xsd:annotation>
            <xsd:documentation>Order line associated with the order.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:LineTaxes"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="ManualRuleAdjustments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ManualRuleAdjustment">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Manual rule adjustment that needs to be applied on this order line. Only item level manual rules can be passed here. If the same item level manual rule is passed at order as well as at order line, then the one passed at the order line wins only if the manual rule adjustment at order line is valid else the order level adjustment is considered. If a rule adjustment is passed multilple times, then only the last one is considered.
										<Validation>
                                    <Condition>If both <code>PricingRuleName</code> and <code>PricingRuleKey</code> is void.</Condition>
                                    <Error
                                    Description="Either PricingRuleKey or PricingRuleName is required for ManualRuleAdjustment." ErrorCode="YPM10085"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Adjustment"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment to apply as part of this manual rule. The adjustment passed is evaluated agains the manual rule action that is applicable on this order. If the passed adjustment is void and there is an <code>Adjustment</code> specified on the manual rules' action, then the manual rule action's <code>Adjustment</code> is used.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>
                                    <div>Primary key of the manual rule.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingRuleName"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_NAME" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>
                                    <div>Name of the manual rule.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ManualAdjustments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ManualAdjustment">
                                <xsd:complexType>
                                    <xsd:attribute name="AdjustmentID"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment identifier for the manaul adjustment.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AdjustmentPerLine"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment to apply on the entire order line.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AdjustmentPerUnit"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment for every unit of the order line item.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeCategory"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>The charge category. For example, Labeling or Handling. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeName"
                                    type="yfctype:ChargeName" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>The name of the charge. For example, Price Ticket. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Reference information about charge. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ExternalCouponAdjustments">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="ExternalCouponAdjustment">
                                <xsd:annotation yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>Adjustment available for external coupon.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Adjustment"
                                    type="yfctype:ExternalAmount" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="AWARD_AMOUNT" yfc:tableName="YFS_PROMOTION_AWARD">
                                    <xsd:documentation>The amount of discount or charge that was applied as a part of this award.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="CouponID"
                                    type="yfctype:CouponId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="COUPON_ID" yfc:tableName="YPM_COUPON">
                                    <xsd:documentation>
                                    <div>Coupon identifier.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ModificationTypes">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ModificationType">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Provided as input so the overrideGetOrderPriceUE can utilize this information for external calculations.</div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="ImpactsPricing"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Pass through attribute that can indicate if the modification type provided may impact pricing.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Level"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Pass through attribute that indicates the level in which the modification type is applicable.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Name"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Pass through attribute that indicates the type of Modification that is applicable to the order line.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="Shipping">
                    <xsd:complexType>
                        <xsd:attribute name="Carrier"
                            type="yfctype:OrgCode" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>The carrier assigned to the line. This is a pass through
									field available to user exits.</div>
								.
							</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CarrierServiceCode"
                            type="yfctype:Text-40" use="optional">
                            <xsd:annotation
                                yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YPM_PRICING_RULE">
                                <xsd:documentation>The carrier service that this rule applies on. Applicable only for shipping category of rules.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MinimizeNumberOfShipments"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="MINIMIZE_NUMBER_OF_SHIPMENTS" yfc:tableName="YPM_PRICING_RULE">
                                <xsd:documentation>Indicates whether to optimize on the number of shipments or not. Applicable only for shipping category of rules.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ShippingCharge"
                            type="yfctype:Price" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div>Shipping charge for the line.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AdditionalLinePrices"/>
            </xsd:all>
            <xsd:attribute name="DeliveryMethod" type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								The delivery method for the order line. 
								<br/>Valid Values: 
								<br/>
                            <b>
                                <code>SHP</code>
                            </b>  - Order line is being shipped. 
								<br/>
                            <b>
                                <code>DEL</code>
                            </b>  - Order line is being delivered. 
								<br/>
                            <b>
                                <code>PICK</code>
                            </b> - Order line is picked.
								<br/>Default value is <b>
                                <code>SHP</code>
                            </b>.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EligibleForShippingDiscount"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Indicates that the order line is eligible for shipping charge discounts. Default value is <b>
                                <code>Y</code>
                            </b>. If the flag is set then this order line is applicable for adjustments generated by pricing rule or coupon having <code>DistributeAdjustment</code> as <code>
                                <b>Y</b>
                            </code>, <code>RuleType</code> as <code>
                                <b>ORDER_TOTAL</b>
                            </code> and <code>RuleCategory</code> as <code>
                                <b>SHIPPING</b>
                            </code>.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsLinePriceForInformationOnly"
                type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Indicates that the price for the order line is for information purposes only. These order lines do not participate in any of the rules and coupons. Default value is <b>
                                <code>N</code>
                            </b>
                        </div>.
						</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPriceLocked" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Indicates that the <code>UnitPrice</code> for the order line is locked and is specified in the input. Default value is <b>
                                <code>N</code>
                            </b>.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Item identifier.
								<Validation>
                                <Condition>
                                    <code>ItemID</code> should not be void.</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineID" type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
                            <code>LineID</code> is used to identify a line. It is passed by the caller of pricing query.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ListPrice" type="yfctype:String" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>List price of an order line.
									<Condition>If <code>IsPriceLocked</code> is set to <b>
                                    <code>Y</code>
                                </b> then <code>ListPrice</code> will be used as a passthrough value.</Condition>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Quantity" type="yfctype:Quantity" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The quantity of item which needs to be priced.
								<Validation>
                                <Condition>
                                    <code>Quantity</code> is void.</Condition>
                                <Error
                                    Description="Mandatory Parameters for the Operation are missing." ErrorCode="YPM10000"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RepriceLine" type="yfctype:Flag" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Indicates that the <code>UnitPrice</code> for the order line is specified in the input or pricingEngine needs to get the price. Default value is <b>
                                <code>Y</code>
                            </b>.
							<br>Note that if multiple order lines exist with the same ItemID and UOM values, and at least one order line contains RepriceLine=Y, then all order lines are set as RepriceLine=Y.</br>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:UOM" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The unit of measure of the item.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitPrice" type="yfctype:UnitPrice" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Unit price for an item of the order line.
								<Validation>
                                <Condition>If <code>IsPriceLocked</code> is set to <b>
                                    <code>Y</code>
                                    </b> and <code>UnitPrice</code> is void.</Condition>
                                <Error
                                    Description="UnitPrice is missing but price is locked." ErrorCode="YPM10078"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineTaxes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:LineTax"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="TaxSummary">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="TaxSummaryDetail">
                                <xsd:complexType>
                                    <xsd:attribute name="InvoicedTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount of this tax which had been invoiced.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="OverallTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The total amount of this tax.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="RemainingTax"
                                    type="yfctype:Money" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>The amount of this tax that has not been invoiced.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="TaxName"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>Tax name.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="LineTax">
        <xsd:annotation yfc:tableName="YFS_TAX_BREAKUP">
            <xsd:documentation>
                <div>
									Contains tax related information for various entities in the
									system. If
									<code>ChargeCategory</code>
									and
									<code>ChargeName</code>
									are not provided, tax will be considered as a billable and
									positive charge. If not implementing
									<code>YFSRecalculateLineTaxUE</code>
									,
									<code>ChargeCategory</code>
									,
									<code>Tax</code>
									, and
									<code>TaxPercentage</code>
									are required for the default repricing logic.
								</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ChargeCategory"
                type="yfctype:ChargeCategory" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CATEGORY" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
									The charge category for which tax is being charged. For
									example, Labeling or Handling.
									To specify the tax for the line
									price, use the charge category
									<b>
                            <code>Price</code>
                        </b>
									.
								</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeName" type="yfctype:ChargeName" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Charge name for which tax is being charged. For example, Price Ticket.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeNameKey" type="yfctype:Key" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Link to YFS_CHARGE_NAME table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InvoicedTax" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="INVOICED_TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Tax amount invoiced.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference1" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE1" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 1</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference2" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE2" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 2</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Reference3" type="yfctype:Text-50" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE3" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Reference 3</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RemainingTax" type="yfctype:Money" use="optional">
                <xsd:annotation>
                    <xsd:documentation>Remaining tax.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Tax" type="yfctype:PlusMinusMoney" use="optional">
                <xsd:annotation yfc:columnName="TAX" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>
                        <div>
										The tax amount.
										<code>Tax</code>
										cannot be less than zero. During the default repricing logic,
										this is recalculated using the
										<code>TaxPercentage</code>
										and the price of the related
										<code>ChargeCategory</code>
										. This can be overridden with the YFSRecalulateLineTaxUE.
									</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxName" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="TAX_NAME" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>The tax name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxPercentage" type="yfctype:TaxType" use="optional">
                <xsd:annotation yfc:columnName="TAX_PERCENTAGE" yfc:tableName="YFS_TAX_BREAKUP">
                    <xsd:documentation>Percentage amount for tax calculation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalLinePrices">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:AdditionalLinePrice"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="AdditionalLinePrice">
        <xsd:annotation yfc:tableName="YFS_ADDNL_LINE_PRICE">
            <xsd:documentation>Maintains details of various Price Types applicable to an order line.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="ManualRuleAdjustments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ManualRuleAdjustment">
                                <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Manual rule adjustment that needs to be applied on this Additional Line Price. Only item level manual rules can be passed here. If the same item level manual rule is passed at order or order line as well as additional line price, then the one passed at the additional lnie price wins only if the manual rule adjustment at additional line price is valid else the order level or order line level adjustment is considered. If a rule adjustment is passed multilple times, then only the last one is considered.
											<Validation>
                                    <Condition>If both <code>PricingRuleName</code> and <code>PricingRuleKey</code> is void.</Condition>
                                    <Error
                                    Description="Either PricingRuleKey or PricingRuleName is required for ManualRuleAdjustment." ErrorCode="YPM10085"/>
                                    </Validation>
                                    </div>
                                    </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute name="Adjustment"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment to apply as part of this manual rule. The adjustment passed is evaluated agains the manual rule action that is applicable on this order. If the passed adjustment is void and there is an <code>Adjustment</code> specified on the manual rules' action, then the manual rule action's <code>Adjustment</code> is used.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="PricingRuleKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>
                                    <div>Primary key of the manual rule.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="PricingRuleName"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRICING_RULE_NAME" yfc:tableName="YPM_PRICING_RULE">
                                    <xsd:documentation>
                                    <div>Name of the manual rule.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" name="ManualAdjustments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="ManualAdjustment">
                                <xsd:complexType>
                                    <xsd:attribute name="AdjustmentID"
                                    type="yfctype:PricingRuleName" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment identifier for the manual adjustment.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AdjustmentPerLine"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment to apply on the entire additional line price.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="AdjustmentPerUnit"
                                    type="yfctype:Price" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Adjustment for every unit of the order line item.</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeCategory"
                                    type="yfctype:ChargeCategory" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>The charge category. For example, Labeling or Handling. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="ChargeName"
                                    type="yfctype:ChargeName" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>The name of the charge. For example, Price Ticket. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="Reference"
                                    type="yfctype:Text-50" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>Reference information about charge. </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="IsPriceLocked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_PRICE_LOCKED" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Indicates that the Price for this Price Type is locked and is specified in the input.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsPriceTypeForInformationOnly"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PRICE_TYPE_INFO_ONLY" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Flag to indicate that his Price Type is for informational use only.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LinePriceID" type="yfctype:String" use="optional"/>
            <xsd:attribute name="Price" use="required">
                <xsd:annotation yfc:columnName="PRICE" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Price value of the corresponding Price Type.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Price"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PriceTypeName"
                type="yfctype:PriceTypeName" use="required">
                <xsd:annotation yfc:columnName="PRICE_TYPE_NAME" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Price Type associated with this record.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RecurringType"
                type="yfctype:CommonCode" use="optional">
                <xsd:annotation yfc:columnName="RECURRING_TYPE" yfc:tableName="YFS_ADDNL_LINE_PRICE">
                    <xsd:documentation>Recurring Interval for Price Type; if null, then Price Type is non-recurring.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Coupons">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Coupon">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="References">
                                <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element maxOccurs="1"
                                    minOccurs="0" name="Reference">
                                    <xsd:annotation yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>This table maintains references against applied promotion on order or order line. For example a unique Coupon barcode.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:attribute
                                    name="ReferenceId"
                                    type="yfctype:VARCHAR2-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_ID" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Reference value within a particular promotion. For example a unique coupon barcode.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReferenceSequence" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="REFERENCE_SEQUENCE" yfc:tableName="YFS_PROMOTION_REFERENCE">
                                    <xsd:documentation>Sequence number for each reference entry.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:PrecisionDigits"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:all>
                        <xsd:attribute name="CouponCategory"
                            type="yfctype:VarcharID-24" use="optional">
                            <xsd:annotation
                                yfc:columnName="PROMOTION_CATEGORY" yfc:tableName="YFS_PROMOTION">
                                <xsd:documentation>Specifies category of external promotion. Valid values are ORDER,ITEM and SHIPPING</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="CouponID"
                            type="yfctype:CouponId" use="optional">
                            <xsd:annotation yfc:columnName="COUPON_ID" yfc:tableName="YPM_COUPON">
                                <xsd:documentation>
                                    <div>Coupon identifier.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExclusivityID"
                            type="yfctype:VARCHAR2-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXCLUSIVITY_ID" yfc:tableName="YFS_PROMOTION">
                                <xsd:documentation>ExclusivityID is used to group set of Promotions for mutual exclusivity. Exclusivity Type settings are applicable among Pricing Rules having same ExclusvityID.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ExclusivityType"
                            type="yfctype:VARCHAR2-100" use="optional">
                            <xsd:annotation
                                yfc:columnName="EXCLUSIVITY_TYPE" yfc:tableName="YFS_PROMOTION">
                                <xsd:documentation>Exclusivity Type of the Promotion. Valid Values are: ALL, RULE_TYPE_ITEM, RULE_TYPE_ORDER, ONE_RULE_WITHIN_ID and GLOBAL_RULE</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="IsExternal"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation yfc:columnName="IS_EXTERNAL" yfc:tableName="YFS_PROMOTION">
                                <xsd:documentation>This flag specifies whether the promotion is external.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OverrideAdjustment" use="optional">
                            <xsd:annotation
                                yfc:columnName="OVERRIDE_ADJUSTMENT_VALUE" yfc:tableName="YFS_PROMOTION">
                                <xsd:documentation>
                                    <div>Overrides the entire adjustment for the rule.  If supplied the value of this attribute will be the adjustment applied.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Price"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="OverrideMaxAdjustment" use="optional">
                            <xsd:annotation
                                yfc:columnName="MAX_ABSOLUTE_ADJUSTMENT" yfc:tableName="YPM_PRICING_RULE_ACTION">
                                <xsd:documentation>
                                    <div>Value of this attribute will override maximum adjustment limit defined for Coupon.</div>
                                </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:Price"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="Priority" use="optional">
                            <xsd:annotation yfc:columnName="PRIORITY" yfc:tableName="YFS_PROMOTION">
                                <xsd:documentation>This attribute is used to prioritize Promotion among a list of applicable Promotions with same Exclusivity settings. Higher the value of this attribute, higher the priority of Promotion, to be applied to an order/item.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                                <xsd:union>
                                    <xsd:simpleType>
                                    <xsd:restriction base="yfctype:NUMBER-10"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                </xsd:union>
                            </xsd:simpleType>
                        </xsd:attribute>
                        <xsd:attribute name="ValidateReferences"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation>
                                <xsd:documentation>
                                    <div> If '<code>Y</code>' the references will be validated.  When value is '<code>Y</code>' <code>CouponID</code> is optional.
            		 		Default value is '<code>N</code>'.
            				</div>
                                </xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentTypeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PaymentType"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentType">
        <xsd:annotation>
            <xsd:documentation>
                <div>Pricing Rule with this Payment type will be considered.</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="PaymentType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_TYPE" yfc:tableName="YPM_PRICING_RULE">
                    <xsd:documentation>
                        <div>Payment type of Pricing Rule.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionViolationList">
        <xsd:annotation>
            <xsd:documentation>
                <div>Represents preexisting violations against this transaction. For use with the manager overrides framework in the context of an order.</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:TransactionViolation"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionViolation">
        <xsd:annotation yfc:tableName="YFS_TRANSACTION_VIOLATION">
            <xsd:documentation>
                <div>Represents new violations against this transaction. For use with the manager overrides framework in the context of an order.
				</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionApprovalStatusList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionViolationReferenceList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionInfo"/>
            </xsd:all>
            <xsd:attribute name="ApprovalRuleID"
                type="yfctype:VARCHAR2-40" use="optional">
                <xsd:annotation yfc:columnName="APPROVAL_RULE_ID" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>ID of Approval Rule that caused violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Domain" type="yfctype:VARCHAR2-100" use="optional">
                <xsd:annotation yfc:columnName="DOMAIN" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>Domain of the corresponding Validation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MessageCode"
                type="yfctype:VARCHAR2-100" use="optional">
                <xsd:annotation yfc:columnName="MESSAGE_CODE" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>Message code to be displayed when approval rule is violated.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:CHAR-24" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>Organization Code of org that owns the rule.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SequenceNumber" use="optional">
                <xsd:annotation yfc:columnName="SEQUENCE_NUMBER" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>Number to identify the violation sequence within a transaction.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NUMBER-4"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Status" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>The status of the violation.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:ViolationStatus"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TransactionInfoID"
                type="yfctype:VARCHAR2-10" use="required">
                <xsd:annotation yfc:columnName="TRANSACTION_INFO_ID" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>The Reference to the transaction that resulted in a violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionViolationKey"
                type="yfctype:Key" use="required">
                <xsd:annotation
                    yfc:columnName="TRANSACTION_VIOLATION_KEY" yfc:tableName="YFS_TRANSACTION_VIOLATION">
                    <xsd:documentation>Primary Key attribute of YFS_TRANSACTION_VIOLATION.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionApprovalStatusList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionApprovalStatus"/>
            </xsd:all>
            <xsd:attribute name="TotalNumberOfRecords"
                type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
 							The total number of Approval Status records returned.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionApprovalStatus">
        <xsd:annotation yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
            <xsd:documentation>Table for storing approver information for the violation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionApprover"/>
            </xsd:all>
            <xsd:attribute name="ApprovalStatusKey" type="yfctype:Key" use="required">
                <xsd:annotation yfc:columnName="APPROVAL_STATUS_KEY" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>Primary Key attribute of YFS_TRANS_APPROVAL_STATUS.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ApproverComments"
                type="yfctype:VARCHAR2-2000" use="optional">
                <xsd:annotation yfc:columnName="APPROVER_COMMENTS" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>Place Holder to capture comments entered by the approver.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ApproverSequence" use="optional">
                <xsd:annotation yfc:columnName="APPROVER_SEQUENCE" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>The sequence of the approver.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NUMBER-10"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ApproverUserID"
                type="yfctype:VARCHAR2-50" use="optional">
                <xsd:annotation yfc:columnName="APPROVER_USER_ID" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>The UserID of the user who has approved or rejected the violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsMandatory" type="yfctype:CHAR-1" use="optional">
                <xsd:annotation yfc:columnName="IS_MANDATORY" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>Indicator to determine if approval is mandatory based on the signature policy of the validation definition.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreviousApproverSequence"
                type="yfctype:ApprovalPlanPredecessorSeq" use="optional">
                <xsd:annotation yfc:columnName="PREVIOUS_APPROVER_SEQ" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>The sequence of the previous approver.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Status" use="optional">
                <xsd:annotation yfc:columnName="STATUS" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>The status of the approval.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:ViolationStatus"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TransactionViolationKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="TRANSACTION_VIO_KEY" yfc:tableName="YFS_TRANS_APPROVAL_STATUS">
                    <xsd:documentation>The reference to the transaction violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionApprover">
        <xsd:annotation yfc:tableName="YFS_TRANSACTION_APPROVER">
            <xsd:documentation>Table for storing approver information for the violation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ApproverGroupKey"
                type="yfctype:CHAR-24" use="optional">
                <xsd:annotation yfc:columnName="APPROVER_GROUP_KEY" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>The reference to the Approval Group configuration.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ApproverUserGroupID"
                type="yfctype:CHAR-25" use="optional">
                <xsd:annotation yfc:columnName="APPROVER_USER_GROUP_ID" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>The User Group that can approve the violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ApproverUserTeam"
                type="yfctype:VARCHAR2-40" use="optional">
                <xsd:annotation yfc:columnName="APPROVER_USER_TEAM" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>The Team that can approve the violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DelegateUserID"
                type="yfctype:VARCHAR2-50" use="optional">
                <xsd:annotation yfc:columnName="DELEGATE_USER_ID" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>The UserID of the delegate that has been assigned to approve the violation on behalf on the main UserID.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:CHAR-24" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>Organization that is associated with the approval group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionApproverKey"
                type="yfctype:Key" use="required">
                <xsd:annotation
                    yfc:columnName="TRANSACTION_APPROVER_KEY" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>Primary Key attribute of YFS_TRANSACTION_APPROVER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionInfoID"
                type="yfctype:VARCHAR2-10" use="optional">
                <xsd:annotation yfc:columnName="TRANSACTION_INFO_ID" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>The reference to the transaction that resulted in a violation</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UserID" type="yfctype:VARCHAR2-50" use="optional">
                <xsd:annotation yfc:columnName="USER_ID" yfc:tableName="YFS_TRANSACTION_APPROVER">
                    <xsd:documentation>The UserID of the user that can approve the violation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionViolationReferenceList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:TransactionViolationReference"/>
            </xsd:all>
            <xsd:attribute name="TotalNumberOfRecords"
                type="yfctype:Integer" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
 							The total number of Transaction Violation Reference records returned.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionViolationReference">
        <xsd:annotation yfc:tableName="YFS_TRANS_VIOLATION_REFERENCE">
            <xsd:documentation>Table for storing additional reference information for identifying violations.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								Action to take.
								<br/>
								Valid Values:
								<br/>
                            <code>Create</code>
								- Creates new record. If the record already exists, it is
								neither created nor modified.
								<br/>
                            <code>Delete</code>
								- Deletes existing record.
								<br/>
								The default behavior is Create.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReferenceName"
                type="yfctype:VARCHAR2-40" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE_NAME" yfc:tableName="YFS_TRANS_VIOLATION_REFERENCE">
                    <xsd:documentation>The identifier for the additional reference value.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReferenceValue"
                type="yfctype:VARCHAR2-40" use="optional">
                <xsd:annotation yfc:columnName="REFERENCE_VALUE" yfc:tableName="YFS_TRANS_VIOLATION_REFERENCE">
                    <xsd:documentation>Value of the additional reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionViolationReferenceKey"
                type="yfctype:Key" use="required">
                <xsd:annotation
                    yfc:columnName="TRANS_VIOLATION_REFERENCE_KEY" yfc:tableName="YFS_TRANS_VIOLATION_REFERENCE">
                    <xsd:documentation>Primary Key attribute of YFS_TRANSACTION_VIOLATION_REFERENCE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="TransactionInfo">
        <xsd:annotation yfc:tableName="YFS_TRANSACTION_INFO">
            <xsd:documentation>Table for storing transaction information that resulted in violations.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrderHeaderKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORDER_HEADER_KEY" yfc:tableName="YFS_TRANSACTION_INFO">
                    <xsd:documentation>Primary key for YFS_ORDER_HEADER</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TransactionInfoID"
                type="yfctype:VARCHAR2-10" use="required">
                <xsd:annotation yfc:columnName="TRANSACTION_INFO_ID" yfc:tableName="YFS_TRANSACTION_INFO">
                    <xsd:documentation>Unique ID of Transaction.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRules">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PricingRule"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PricingRule">
        <xsd:annotation yfc:tableName="YPM_PRICING_RULE">
            <xsd:documentation>
                <div>Pricing Rule associated with the Coupon.</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Description"
                type="yfctype:PricingRuleDescription" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YPM_PRICING_RULE">
                    <xsd:documentation>
                        <div>Description of the Pricing Rule associated with the Coupon.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PricingRuleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PRICING_RULE_KEY" yfc:tableName="YPM_PRICING_RULE">
                    <xsd:documentation>
                        <div>Primary Key of Pricing Rule associated with the Coupon.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
