            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:UserGroups"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Enterprise"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgDocumentList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:SubOrganization"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:DepartmentList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ColonyMap"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CarrierPreferences"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProNoDataList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Scac"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ScacAndServiceList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentRuleList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:PaymentTypeList"/>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:OrgServiceList"/>
