                            <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The middle name of the person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="MobilePhone"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation
                                yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OtherPhone"
                            type="yfctype:PhoneNo" use="optional">
                            <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PersonID"
                            type="yfctype:PersonId" use="optional">
                            <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="PreferredShipAddress"
                            type="yfctype:Flag" use="optional">
                            <xsd:annotation
                                yfc:columnName="PREFERRED_SHIP_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="State" type="yfctype:State" use="optional">
                            <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The state in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Suffix"
                            type="yfctype:Suffix" use="optional">
                            <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="Title"
                            type="yfctype:Title-10" use="optional">
                            <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="UseCount"
                            type="yfctype:Count" use="optional">
                            <xsd:annotation yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>This field is currently not used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="VerificationStatus"
                            type="yfctype:ShortStatus" use="optional">
                            <xsd:annotation
                                yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>This field is currently not used.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="ZipCode"
                            type="yfctype:ZipCode" use="optional">
                            <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                                <xsd:documentation>The zip code in the address.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:QueueSubscriptionList"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="UserGroupLists">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="UserGroupList">
                                <xsd:annotation yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Captures group membership of various users.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                    name="UserGroupListKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_GROUP_LIST_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Primary key attribute of YFS_USER_GROUP_LIST.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="UsergroupKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>A group which maintains users with membership to the group.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="Activateflag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ACTIVATEFLAG" yfc:tableName="YFS_USER">
                    <xsd:documentation>This flag indicates whether a user is active.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BillingaddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILLINGADDRESS_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The billing address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BusinessKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BUSINESS_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The business associated with this user. Identifies a ship node when filled.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChangePassNext"
                type="yfctype:ChangePassNext" use="optional">
                <xsd:annotation yfc:columnName="CHANGE_PASS_NEXT" yfc:tableName="YFS_USER">
                    <xsd:documentation>Flag to change password on next login. Valid values are: 0 (for no) and 1 (for yes).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactaddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONTACTADDRESS_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The contact address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CreatorOrganizationKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="CREATOR_ORGANIZATION_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The organization that created the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DataSecurityGroupId"
                type="yfctype:DataSecurityGroupId" use="optional">
                <xsd:annotation yfc:columnName="DATA_SECURITY_GROUP_ID" yfc:tableName="YFS_USER">
                    <xsd:documentation>Data Security Group Id.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DisplayUserID" type="yfctype:Text-150" use="optional">
                <xsd:annotation yfc:columnName="DISPLAY_USER_ID" yfc:tableName="YFS_USER">
                    <xsd:documentation>Indicates Display User's ID which is used for login in to the system.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCode" type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISE_CODE" yfc:tableName="YFS_USER">
                    <xsd:documentation>The Customer Master Organization Code of the User's Organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Imagefile" type="yfctype:FileName" use="optional">
                <xsd:annotation yfc:columnName="IMAGEFILE" yfc:tableName="YFS_USER">
                    <xsd:documentation>The image associated with a user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Localecode" type="yfctype:Locale" use="optional">
                <xsd:annotation yfc:columnName="LOCALECODE" yfc:tableName="YFS_USER">
                    <xsd:documentation>The locale of a user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Loginid" type="yfctype:LoginId" use="optional">
                <xsd:annotation yfc:columnName="LOGINID" yfc:tableName="YFS_USER">
                    <xsd:documentation>The login ID for the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Longdesc"
                type="yfctype:Description-255" use="optional">
                <xsd:annotation yfc:columnName="LONGDESC" yfc:tableName="YFS_USER">
                    <xsd:documentation>The long description for the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MenuId" type="yfctype:Identifier" use="optional">
                <xsd:annotation yfc:columnName="MENU_ID" yfc:tableName="YFS_USER">
                    <xsd:documentation>The menu ID associated with the user. This is used to display the menus in various applications to which this user has access.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NoteKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NOTE_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>A generic note for the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ParentUserKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PARENT_USER_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The parent user associated with this user. May be used to capture reporting hierarchy. A user has only one parent but may have many children.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Password" type="yfctype:Password" use="optional">
                <xsd:annotation yfc:columnName="PASSWORD" yfc:tableName="YFS_USER">
                    <xsd:documentation>The password for the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PasswordPolicyKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PWD_POLICY_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>Primary key attribute of PLT_PWD_POLICY.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreferenceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PREFERENCE_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>Not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Pwdlastchangedon"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="PWDLASTCHANGEDON" yfc:tableName="YFS_USER">
                    <xsd:documentation>The last date on which the user's password was changed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SessionTimeout"
                type="yfctype:SessionTimeOut" use="optional">
                <xsd:annotation yfc:columnName="SESSION_TIMEOUT" yfc:tableName="YFS_USER">
                    <xsd:documentation>Number of seconds of inactivity before the session expires. If this value is negative or zero the session timeout is picked up from web.xml</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SuperUser" type="yfctype:SuperUser" use="optional">
                <xsd:annotation yfc:columnName="SUPER_USER" yfc:tableName="YFS_USER">
                    <xsd:documentation>Access Level of User. Valid values are: 0 (for no) and 1 (for yes).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SystemName" type="yfctype:SystemName" use="optional">
                <xsd:annotation yfc:columnName="SYSTEMNAME" yfc:tableName="YFS_USER">
                    <xsd:documentation>The external system that maintains a user's authentication information, such as a LDAP server.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Theme" type="yfctype:PresentationTheme" use="optional">
                <xsd:annotation yfc:columnName="THEME" yfc:tableName="YFS_USER">
                    <xsd:documentation>The presentation theme for a user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UserKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USER_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The primary key for the YFS_USER table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER">
                    <xsd:documentation>The user group associated with this user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Username" type="yfctype:UserName" use="optional">
                <xsd:annotation yfc:columnName="USERNAME" yfc:tableName="YFS_USER">
                    <xsd:documentation>The name of the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Usertype" type="yfctype:EnumUserType" use="optional">
                <xsd:annotation yfc:columnName="USERTYPE" yfc:tableName="YFS_USER">
                    <xsd:documentation>This field indicates the user type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueSubscriptionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:QueueSubscription"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="QueueSubscription">
        <xsd:annotation yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
            <xsd:documentation>Maintains Subscription of users to various queues. Users may subscribe to more than one Queue.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="QueueKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="QUEUE_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                    <xsd:documentation>A Queue for which subscription is maintained.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="QueueSubscriptionKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="QUEUE_SUBSCRIPTION_KEY" yfc:tableName="YFS_QUEUE_SUBSCRIPTION">
                    <xsd:documentation>Primary key attribute of YFS_QUEUE_SUBSCRIPTION.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="RelatedOrgList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrgEnterprise">
                    <xsd:annotation yfc:tableName="YFS_ORG_ENTERPRISE">
                        <xsd:documentation>Maintains relationship between an enterprise and its participants.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="EnterpriseOrganizationKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ENTERPRISE_ORGANIZATION_KEY" yfc:tableName="YFS_ORG_ENTERPRISE">
                                <xsd:documentation>An enterprise with which other organizations participate.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrgEnterpriseKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORG_ENTERPRISE_KEY" yfc:tableName="YFS_ORG_ENTERPRISE">
                                <xsd:documentation>Primary key attribute of YFS_ORG_ENTERPRISE.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrganizationKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORGANIZATION_KEY" yfc:tableName="YFS_ORG_ENTERPRISE">
                                <xsd:documentation>A participating organization.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="EnterpriseOrgList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrgEnterprise">
                    <xsd:annotation yfc:tableName="YFS_ORG_ENTERPRISE">
                        <xsd:documentation>Maintains relationship between an enterprise and its participants.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                        <xsd:attribute name="EnterpriseOrganizationKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ENTERPRISE_ORGANIZATION_KEY" yfc:tableName="YFS_ORG_ENTERPRISE">
                                <xsd:documentation>An enterprise with which other organizations participate.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                        <xsd:attribute name="OrgEnterpriseKey"
                            type="yfctype:Key" use="optional">
                            <xsd:annotation
                                yfc:columnName="ORG_ENTERPRISE_KEY" yfc:tableName="YFS_ORG_ENTERPRISE">
                                <xsd:documentation>Primary key attribute of YFS_ORG_ENTERPRISE.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:attribute>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="UserGroups">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:UserGroup"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="UserGroup">
        <xsd:annotation yfc:tableName="YFS_USER_GROUP">
            <xsd:documentation>Defines all user groups in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ResourcePermissionList"/>
                <xsd:element maxOccurs="1" minOccurs="0" name="UserGroupLists">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element maxOccurs="unbounded"
                                minOccurs="0" name="UserGroupList">
                                <xsd:annotation yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Captures group membership of various users.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                    <xsd:attribute
                                    name="UserGroupListKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_GROUP_LIST_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>Primary key attribute of YFS_USER_GROUP_LIST.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute name="UserKey"
                                    type="yfctype:Key" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="USER_KEY" yfc:tableName="YFS_USER_GROUP_LIST">
                                    <xsd:documentation>A user whose membership is maintained.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:all>
            <xsd:attribute name="BusinessKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BUSINESS_KEY" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GroupType" type="yfctype:GroupType-20" use="optional">
                <xsd:annotation yfc:columnName="GROUP_TYPE" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Type of group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OwnerType" type="yfctype:OwnerType" use="optional">
                <xsd:annotation yfc:columnName="OWNER_TYPE" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TemplateGroup" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="TEMPLATE_GROUP" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Template user group based on which a user group was derived.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupDescription"
                type="yfctype:Description-255" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_DESCRIPTION" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Description of a user group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupId" type="yfctype:UserGroup" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_ID" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>A unique identifier to the user group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_KEY" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Primary key attribute of YFS_USER_GROUP.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UsergroupName"
                type="yfctype:UserGroupName" use="optional">
                <xsd:annotation yfc:columnName="USERGROUP_NAME" yfc:tableName="YFS_USER_GROUP">
                    <xsd:documentation>Name of a user group.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ResourcePermissionList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:ResourcePermission"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ResourcePermission">
        <xsd:annotation yfc:tableName="YFS_RESOURCE_PERMISSION">
            <xsd:documentation>Permissions to various resources in the system are maintained here. Permissions are maintained at a user group level.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ActivateFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ACTIVATE_FLAG" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>Indicates if a permission is active.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ResourceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="RESOURCE_KEY" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>A permission controlled resource.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ResourcePermissionKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="RESOURCE_PERMISSION_KEY" yfc:tableName="YFS_RESOURCE_PERMISSION">
                    <xsd:documentation>Primary key attribute of YFS_RESOURCE_PERMISSION.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Enterprise">
        <xsd:annotation yfc:tableName="YFS_ENTERPRISE">
            <xsd:documentation>This table stores some of the attributes of an enterprise. Participants that play enterprise role have ability to service orders, view them and create a flow for these orders.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Activateflag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ACTIVATEFLAG" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>This flag indicates whether the enterprise is active. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BillingaddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILLINGADDRESS_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The billing address of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommercialContactKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="COMMERCIAL_CONTACT_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The reference to the commercial contact.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactaddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONTACTADDRESS_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The contact address of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CorporateaddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CORPORATEADDRESS_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The corporate address of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISE_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The primary key for the YFS_ENTERPRISE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Enterprisecode" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISECODE" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The organization that is associated with this enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Enterprisename"
                type="yfctype:EnterpriseName" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISENAME" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The name of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Latitude" use="optional">
                <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The latitude of this enterprise.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="LegalContactKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="LEGAL_CONTACT_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The reference to the legal contact.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Localecode" type="yfctype:Locale" use="optional">
                <xsd:annotation yfc:columnName="LOCALECODE" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The locale of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Logoimagefile" type="yfctype:FileName" use="optional">
                <xsd:annotation yfc:columnName="LOGOIMAGEFILE" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The logo of the enterprise</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Longdesc"
                type="yfctype:Description-255" use="optional">
                <xsd:annotation yfc:columnName="LONGDESC" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The long description of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Longitude" use="optional">
                <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The longitude of this enterprise.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="NoteKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="NOTE_KEY" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>A generic note.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TaxpayerId" type="yfctype:ID-40" use="optional">
                <xsd:annotation yfc:columnName="TAXPAYER_ID" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>The tax payer identifier of the enterprise.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UseDefaultDistribution"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="USE_DEFAULT_DISTRIBUTION" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>This flag indicates whether the default distribution needs to be used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UseDefaultOrderStatusRules"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="USE_DEFAULT_ORDER_STATUS_RULES" yfc:tableName="YFS_ENTERPRISE">
                    <xsd:documentation>This flag indicates whether the default order status rules needs to be used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgDocumentList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrgDocument"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgDocument">
        <xsd:annotation yfc:tableName="YFS_ORG_DOCUMENT">
            <xsd:documentation>All document requirements for an organization are captured here.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="BackupProtocolKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BACKUP_PROTOCOL_KEY" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Identifies a backup protocol to deliver a document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentFormatKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_FORMAT_KEY" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Format of the document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrgDocumentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORG_DOCUMENT_KEY" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Primary key attribute of YFS_ORG_DOCUMENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProtocolKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PROTOCOL_KEY" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Identifies protocol used to deliver a document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RoleDocumentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ROLE_DOCUMENT_KEY" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>Identifies a role document.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference1"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_1" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference2"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_2" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference3"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_3" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference4"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_4" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VariantReference5"
                type="yfctype:VariantReference" use="optional">
                <xsd:annotation yfc:columnName="VARIANT_REFERENCE_5" yfc:tableName="YFS_ORG_DOCUMENT">
                    <xsd:documentation>A variant reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="SubOrganization">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" name="Organization">
                    <xsd:annotation yfc:tableName="YFS_ORGANIZATION">
                        <xsd:documentation>This table stores attributes that are common to all oganizations. Organizations may play different roles in trade. For example,  Buyer, Seller, Enterprise, ShipNode.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType/>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="DepartmentList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Department"/>
            </xsd:sequence>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Action to take. If Action is passed in DepartmentList, the same action will be performed for each Department in the List.
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist or modifies an existing record if it does.
					<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record.
					<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Changes the value of an existing record.
					<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
					<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Department">
        <xsd:annotation yfc:tableName="YFS_DEPARTMENT">
            <xsd:documentation>
                <div>
		Maintains all departments for organizations.
			<Validation>
                        <Condition>
				 Either <code>DepartmentCode</code> and/or <code>OrganizationCode</code> must be passed.
				 </Condition>
                        <Error
                            Description="Mandatory Parameter Missing." ErrorCode="YCP0287"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Action to take
					<br/>Valid Values:
					<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist or modifies an existing record if it does.
					<br/>
                            <b>
                                <code>Create</code>
                            </b> - Creates a new record.
					<br/>
                            <b>
                                <code>Modify</code>
                            </b> - Changes the value of an existing record.
					<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
					<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultQueueId"
                type="yfctype:Identifier" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_QUEUE_ID" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Default Queue for a department.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentAbbreviation"
                type="yfctype:DepartmentAbbreviation" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_ABBR" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Abbreviation of the department name.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentCode"
                type="yfctype:DepartmentCode" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_CODE" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Identifier to the department.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_KEY" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Primary key attribute of YFS_DEPARTMENT.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DepartmentName"
                type="yfctype:DepartmentName" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT_NAME" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Name of the department.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EmailId" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAIL_ID" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Email Id for the department.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EmailTemplate"
                type="yfctype:EmailTemplate" use="optional">
                <xsd:annotation yfc:columnName="EMAIL_TEMPLATE" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>Template for email.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_DEPARTMENT">
                    <xsd:documentation>
                        <div>The organization code associated with this department.
		   	    	<Validation>
                                <Condition>This must match the Organization being managed by this API.</Condition>
                                <Error
                                    Description="Department should belong to the organization which is being managed." ErrorCode="YCP0286"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ColonyMap">
        <xsd:annotation yfc:tableName="PLT_COLONY_MAP">
            <xsd:documentation>This table maps data in a foreign table to a particular Colony.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ColonyId" type="yfctype:ColonyId" use="optional">
                <xsd:annotation yfc:columnName="COLONY_ID" yfc:tableName="YFS_ORGANIZATION">
                    <xsd:documentation>
                        <div>
                            <Validation>
                                <Condition>
                                    <code>ColonyId</code> is modified for an existing <code>Organization</code>. It should only be defined on create.
								</Condition>
                                <Error
                                    Description="Organization's Colony Id can not be changed." ErrorCode="YSC83_0010"/>
                            </Validation>
                            <Validation>
                                <Condition>All organizations including Capacity, Catalog, Customer Master, Inventory, Pricing Organizations must the same <code>ColonyId</code> unless the organization is a modeled as a store(seller organization)</Condition>
                                <Error
                                    Description="Organizations have different Colony IDs" ErrorCode="YSC83_0009"/>
                            </Validation>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CarrierPreferences">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CarrierPreference"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CarrierPreference">
        <xsd:annotation yfc:tableName="YFS_ORGANIZATION">
            <xsd:documentation>This table stores attributes that are common to all oganizations. Organizations may play different roles in trade. For example,  Buyer, Seller, Enterprise, ShipNode.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Account1" type="xsd:string" use="optional"/>
            <xsd:attribute name="Account2" type="xsd:string" use="optional"/>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Operation to perform
						<br/>Valid Values:
						<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AutoOpenManifest" type="xsd:string" use="optional"/>
            <xsd:attribute name="BillingAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILLING_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                    <xsd:documentation>The billing address that is associated with this organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierToCustomerReference"
                type="xsd:string" use="optional"/>
            <xsd:attribute name="ContactAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONTACT_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                    <xsd:documentation>The contact address that is associated with this organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CurrentTrackingNo" type="xsd:string" use="optional"/>
            <xsd:attribute name="CustomerReference1" type="xsd:string" use="optional"/>
            <xsd:attribute name="CustomerReference2" type="xsd:string" use="optional"/>
            <xsd:attribute name="CustomerReference3" type="xsd:string" use="optional"/>
            <xsd:attribute name="CustomerReference4" type="xsd:string" use="optional"/>
            <xsd:attribute name="CustomerReference5" type="xsd:string" use="optional"/>
            <xsd:attribute name="EnterpriseKey" type="xsd:string" use="required"/>
            <xsd:attribute name="InternalReference1" type="xsd:string" use="optional"/>
            <xsd:attribute name="InternalReference2" type="xsd:string" use="optional"/>
            <xsd:attribute name="InternalReference3" type="xsd:string" use="optional"/>
            <xsd:attribute name="InternalReference4" type="xsd:string" use="optional"/>
            <xsd:attribute name="InternalReference5" type="xsd:string" use="optional"/>
            <xsd:attribute name="MaxTrackingNo" type="xsd:string" use="optional"/>
            <xsd:attribute name="MinTrackingNo" type="xsd:string" use="optional"/>
            <xsd:attribute name="OnlineControlNumber" type="xsd:string" use="optional"/>
            <xsd:attribute name="PortalAccount1" type="xsd:string" use="optional"/>
            <xsd:attribute name="PortalAccount2" type="xsd:string" use="optional"/>
            <xsd:attribute name="PostOfficeCity" type="xsd:string" use="optional"/>
            <xsd:attribute name="PostOfficeState" type="xsd:string" use="optional"/>
            <xsd:attribute name="PostOfficeZipcode" type="xsd:string" use="optional"/>
            <xsd:attribute name="RecordKey" type="xsd:string" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						Participant identifier. Buyer or Node Organization_Key for which Parcel service has to be added.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RecordType" type="xsd:string" use="required">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
						Participant type identifier. Indicates if the attributes are maintained for a Buyer or a ship node. Valid values: BUYER OR SUPPLIER
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReferenceLength1" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceLength2" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceLength3" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceLength4" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceLength5" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferencePrefix1" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferencePrefix2" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferencePrefix3" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferencePrefix4" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferencePrefix5" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceStartIndex1" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceStartIndex2" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceStartIndex3" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceStartIndex4" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceStartIndex5" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceSuffix1" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceSuffix2" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceSuffix3" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceSuffix4" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceSuffix5" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceType1" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceType2" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceType3" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceType4" type="xsd:string" use="optional"/>
            <xsd:attribute name="ReferenceType5" type="xsd:string" use="optional"/>
            <xsd:attribute name="ScacExKey" type="xsd:string" use="optional"/>
            <xsd:attribute name="ScacKey" type="xsd:string" use="required"/>
            <xsd:attribute name="ServerAddress1" type="xsd:string" use="optional"/>
            <xsd:attribute name="ServerAddress2" type="xsd:string" use="optional"/>
            <xsd:attribute name="ServerPassword" type="xsd:string" use="optional"/>
            <xsd:attribute name="ServerUserid" type="xsd:string" use="optional"/>
            <xsd:attribute name="SystemIntgForDomesticReqd"
                type="xsd:string" use="optional"/>
            <xsd:attribute name="SystemIntgForIntlReqd"
                type="xsd:string" use="optional"/>
            <xsd:attribute name="ThirdPartyOrganizationKey"
                type="xsd:string" use="optional"/>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProNoDataList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ProNoData"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ProNoData">
        <xsd:annotation yfc:tableName="YFS_PRO_NO_DATA">
            <xsd:documentation>
                <div>
					Stores the data needed for generation of PRO numbers at node and carrier level. This element should be used to store the PRO number data corresponding to LTL carriers only.
					</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Action" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>Operation to perform
						<br/>Valid Values:
						<br/>
                            <b>
                                <code>Manage</code>
                            </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                            <b>
                                <code>Delete</code>
                            </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                                <code>Manage</code>
                            </b>.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CheckDigitAlgo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CHECK_DIGIT_ALGO" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>The Check Digit Algorithm to be used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GenerationScheme"
                type="yfctype:Text-10" use="required">
                <xsd:annotation yfc:columnName="GENERATION_SCHEME" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>The scheme for PRO number generation.Valid values are SYSTEM, EXTERNAL, NOGEN.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Node" type="yfctype:OrgCode" use="required">
                <xsd:annotation yfc:columnName="NODE_KEY" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>The Node for which PRO number generation is configured.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NotificationServiceKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="NOTIFICATION_SERVICE_KEY" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Primary key of the Service which will be invoked when Notification Threshold is reached.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NotificationThreshold"
                type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="NOTIFICATION_THRESHOLD" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>The number specifies when the notification action will be executed to intimate the user.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNo1End"
                type="yfctype:PositiveInteger" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO_1_END" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Maximum number for the first set of PRO numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNo1Prefix" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO_1_PREFIX" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Prefix for the first set of PRO numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNo1Start"
                type="yfctype:PositiveInteger" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO_1_START" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Starting number for the first set of PRO numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNo2End"
                type="yfctype:PositiveInteger" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO_2_END" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Maximum number for the second set of PRO numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNo2Prefix" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO_2_PREFIX" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Prefix for the second set of PRO numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProNo2Start"
                type="yfctype:PositiveInteger" use="optional">
                <xsd:annotation yfc:columnName="PRO_NO_2_START" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>Starting number for the second set of PRO numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Scac" type="yfctype:OrgCode" use="required">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_PRO_NO_DATA">
                    <xsd:documentation>
                        <div>
						The Carrier for which PRO number generation is configured. The Carrier should be one for which LTL services have been configured.
						</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Scac">
        <xsd:annotation yfc:tableName="YFS_SCAC">
            <xsd:documentation>This table stores carrier attributes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="Account1" type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="ACCOUNT1" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The carrier's account number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Account2" type="yfctype:AccountNo" use="optional">
                <xsd:annotation yfc:columnName="ACCOUNT2" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The alternate account number for the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="BillingAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="BILLING_ADDRESS_KEY" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The billing address with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CommProtocol" type="yfctype:Text-10" use="optional">
                <xsd:annotation yfc:columnName="COMM_PROTOCOL" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The communication protocol established with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ContactAddressKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CONTACT_ADDRESS_KEY" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The contact address with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CurrentTrackingNo"
                type="yfctype:Counter" use="optional">
                <xsd:annotation yfc:columnName="CURRENT_TRACKING_NO" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The last generated tracking number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HostInterfaceType"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="HOST_INTERFACE_TYPE" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The type of interface maintained with the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference1"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_1" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>An internal reference that may be stored for a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference2"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_2" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>An internal reference that may be stored for a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference3"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_3" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>An internal reference that may be stored for a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference4"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_4" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>An internal reference that may be stored for a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference5"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_5" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>An internal reference that may be stored for a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Localecode" type="yfctype:Locale" use="optional">
                <xsd:annotation yfc:columnName="LOCALECODE" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The carrier's locale.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxTrackingNo" type="yfctype:Counter" use="optional">
                <xsd:annotation yfc:columnName="MAX_TRACKING_NO" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The number until which the tracking numbers can be generated for a carrier. When this number is reached, a fresh range for tracking numbers may be requested from the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinTrackingNo" type="yfctype:Counter" use="optional">
                <xsd:annotation yfc:columnName="MIN_TRACKING_NO" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The number starting from which the tracking numbers are generated.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The organization code of organization that defines this carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Scac" type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="SCAC" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The code of the organization plays the role of carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacDesc" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="SCAC_DESC" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The description of the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_KEY" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The primary key for the YFS_SCAC table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServerAddress1" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SERVER_ADDRESS1" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The IP address of the carrier's server, which may be used for integrating purposes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServerAddress2" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SERVER_ADDRESS2" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The IP address of the carrier's server, which may be user for integrating purposes.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServerPassword" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SERVER_PASSWORD" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The password of the user used to connect to the carrier's server.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServerUserid" type="yfctype:LoginId-40" use="optional">
                <xsd:annotation yfc:columnName="SERVER_USERID" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The user identifier used to connect to the carrier's server.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SystemIntgForDomesticReqd"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="SYSTEM_INTG_FOR_DOMESTIC_REQD" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>This flag indicates whether integration with the carrier is required for domestic shipments.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SystemIntgForIntlReqd"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="SYSTEM_INTG_FOR_INTL_REQD" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>This flag indicates whether integration with the carrier is required for international shipments.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TrackNoAlgo" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="TRACK_NO_ALGO" yfc:tableName="YFS_SCAC">
                    <xsd:documentation>The algorithm used in the generation of tracking numbers.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ScacAndServiceList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:ScacAndService"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ScacAndService">
        <xsd:annotation yfc:tableName="YFS_SCAC_AND_SERVICE">
            <xsd:documentation>
                <div>
							This table stores Services offered by a Carrier. This element should be used only for those organizations which are Carriers. If you are creating a new Carrier ensure that Scac Element is also present also along with this element and one of the Organization Roles has CARRIER specified.
							</div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CarrierServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An identifier to a master service from which this service is derived.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_TYPE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Type of Carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ElectronicCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="ELECTRONIC_CODE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An electronic code.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HostCode" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="HOST_CODE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Host code.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference1"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_1" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An Internal Reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference2"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_2" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An Internal Reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference3"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_3" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An Internal Reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference4"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_4" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An Internal Reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternalReference5"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERNAL_REFERENCE_5" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An Internal Reference.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InternationalShipping"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="INTERNATIONAL_SHIPPING" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Indicates if the service covers international shipments.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>The organization code of organization that defines this service offered by the carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndService" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>An identifier for a service offered by a carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndServiceDesc"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE_DESC" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Description of the service.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacAndServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_AND_SERVICE_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_SCAC_AND_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ScacKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SCAC_KEY" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Servicing carrier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipMode" type="yfctype:ShipMode" use="optional">
                <xsd:annotation yfc:columnName="SHIP_MODE" yfc:tableName="YFS_SCAC_AND_SERVICE">
                    <xsd:documentation>Shipment mode serviced PARCEL, TL, etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentRuleList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PaymentRule"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentRule">
        <xsd:annotation yfc:tableName="YFS_PAYMENT_RULE">
            <xsd:documentation>This table stores all payment rules used by various organizations in the system.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AuthRequiredBeforeSchedule"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="AUTH_REQUIRED_BEFORE_SCHEDULE" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This flag enables or disables the requirement of acquiring an authorization before scheduling an order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CollectExternalThroughAr"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="COLLECT_EXTERNAL_THROUGH_AR" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This flag indicates whether the collection is done externally through account receivable.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CustomerAccountMaintainedInternally"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="CUSTOMER_ACCOUNT_MAINTAINED" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This flag indicates if customer accounts spending limit is established and are maintained internally. The default value is N which means customer accounts are not setup.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_FLAG" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This flag indicates whether a payment rule is the default rule for an organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Description" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="DESCRIPTION" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>Description of the Payment Rule.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FinancialHandlingRequired"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="FINANCIAL_HANDLING_REQUIRED" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This flag indicates whether financial handling is required.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IgnorePaymentStatusForPurge"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IGNORE_STATUS_FOR_PURGE" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>If Y, allows the order to be purged regardless of the PaymentStatus on the order.  This flag is only read by the purge agent if Settlement is not required.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InterfaceTime"
                type="yfctype:EnumInterfaceTime" use="optional">
                <xsd:annotation yfc:columnName="INTERFACE_TIME" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This field indicates when the invoice will be published.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MerchantId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="MERCHANT_ID" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>The merchant identifier used for the collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:YSCOperation" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The operation to perform on this record. Valid values are listed in the table below.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>The organization that is associated with this payment rule.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentRuleId" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_RULE_ID" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>The unique identifier for the payment rule.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentRuleKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_RULE_KEY" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>The primary key for the YFS_PAYMENT_RULE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PostponeReauthorization"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="POSTPONE_REAUTHORIZATION" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>This flag enables delaying re-authorization by a number of hours, configured separately for product and service lines.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReauthHoursBeforeAppointment"
                type="yfctype:NumberOfHours" use="optional">
                <xsd:annotation
                    yfc:columnName="REAUTH_HOURS_BEFORE_APPT" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>The number of hours before the Promised Appointment Start Date to begin acquiring authorizations again for Service lines.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReauthHoursBeforeRelease"
                type="yfctype:NumberOfHours" use="optional">
                <xsd:annotation
                    yfc:columnName="REAUTH_HOURS_BEFORE_RELEASE" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>The number of hours before the release date to begin acquiring authorizations again for Product lines.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute
                name="UseChargeTransactionRequestsForAuthorization"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="EXTERNALLY_REQUESTED_AUTH" yfc:tableName="YFS_PAYMENT_RULE">
                    <xsd:documentation>Identifier which prevents the normal authorization logic and bases auths on YFS_CHARGE_TRAN_REQUESTs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentTypeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:PaymentType"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="PaymentType">
        <xsd:annotation yfc:tableName="YFS_PAYMENT_TYPE">
            <xsd:documentation>
                <div>
							This table stores the types of payment methods the organization
							provides.
							<Validation>
                        <Condition>
									The AuthReversalStrategy passed is not one of the allowed values.
							</Condition>
                        <Error
                            Description="Invalid AuthReversalStrategy." ErrorCode="OMP711_0001"/>
                    </Validation>
                    <Validation>
                        <Condition>
									A reversal strategy may not be selected if the
									PaymentTypeGroup is CustomerAccount or the PaymentType is
									ChargeInsteadOfAuth.
							</Condition>
                        <Error
                            Description="AuthReversalStrategy is not compatible with CustomerAccount or ChargeInsteadOfAuth." ErrorCode="OMP711_0002"/>
                    </Validation>
                </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute
                name="AllowAuthorizationsToExceedSettlementRequest"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="ALLOW_AUTH_TO_EXCEED_SETTLE_RQ" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This rule determines whether authorizations larger than the settlement amount are able to be used for settlement for this payment type.  If IS_MULTIPLE_REQ_ALLOWED or CHARGE_CONSOL_ALLOWED are Y, this is implicitly Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AuthorizationExpirationHours" use="optional">
                <xsd:annotation yfc:columnName="AUTH_EXPIRATION_HOURS" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>Number of hours an authorization is valid before expiring for this payment type.  This value is not used internally by the system, but may be referenced for the computation of the AuthorizationExpirationDate when processing payment transactions.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfHours-7"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="AuthorizationReversalHours" use="optional">
                <xsd:annotation yfc:columnName="AUTH_REVERSAL_HOURS" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>Number of hours an authorization is valid for reversal for this payment type. This value is not used internally by the system and serves as an informational field.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:NumberOfHours-7"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="AuthorizationReversalStrategy"
                type="yfctype:AuthReversalStrategy" use="optional">
                <xsd:annotation yfc:columnName="AUTH_REVERSAL_STRATEGY" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This field controls when reverse authorization requests will be created for this payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CashBackLimit" type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="CASH_BACK_LIMIT" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The maximum amount that can be given back as cash for payment methods associated with this payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeConsolAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_CONSOL_ALLOWED" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether charge consolidation is allowed for a payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeInsteadOfAuth"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_INSTEAD_OF_AUTH" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates if the requestCollection API should create a CHARGE request against this Payment Type in all scenarios where an AUTHORIZATION request would be created.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeSequence"
                type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_SEQUENCE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The sequence in which charges needs to be collected if multiple payment types are supported by an organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ChargeUpToAvailable"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CHARGE_UP_TO_AVAILABLE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether the user exit getFundsAvailable should be invoked to determine the funds available on this payment account.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultForReturn" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_FOR_RETURN" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates the usage of a payment type as the default one while processing returned goods.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HasAltRefundConstraint"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="HAS_ALT_REFUND_CONSTRAINT" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether this payment type has some alternate refund payment type to use in case the refund amount meets the constraint.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MerchantId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="MERCHANT_ID" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The identifier of the merchant used for the collection.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinWaitTimeForConsol"
                type="yfctype:Number" use="optional">
                <xsd:annotation
                    yfc:columnName="MIN_WAIT_TIME_FOR_CONSOL" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The minimum time to wait for utilizing the opportunity to consolidate charges.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NewRefundPaymentType"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="NEW_REFUND_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The payment type that needs to be used for refunds and is only set if not refunding to the same account.  Payment type selection is valid only if it belongs to payment type group STORED_VALUE_CARD or OTHER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NoProcessingRequired"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="NO_PROCESSING_REQUIRED" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates that when the Payment Type is added on the Order, it is already processed. Examples of this Payment Type are checks/cash. A pre processed Charge Transaction record of type CHARGE is created for this Payment Type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:YSCOperation" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>The operation to perform on this record. Valid values are listed in the table below.</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationCode"
                type="yfctype:OrgCode" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_CODE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The organization that is associated with this payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PartialReversalSupported"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="PARTIAL_REVERSAL_SUPPORT" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This rule determines whether reverse authorization requests support partial reversal for this payment type.  If Y, partial reversal is supported. If N, only full reversal is supported.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentTemplateType"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_TEMPLATE_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This field indicates User Interfaces to use the template required to capture Payment Methods of this Payment Type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The unique identifier for the payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentTypeDescription"
                type="yfctype:FreeText-100" use="optional">
                <xsd:annotation
                    yfc:columnName="PAYMENT_TYPE_DESCRIPTION" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The description of the payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentTypeGroup"
                type="yfctype:EnumPaymentTypeGroup" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_TYPE_GROUP" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This filed indicates the payment type group, which will determine behavior.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PaymentTypeKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="PAYMENT_TYPE_KEY" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The primary key for the YFS_PAYMENT_TYPE table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundConstraintOperator"
                type="yfctype:EnumRefundConstraintOperator" use="optional">
                <xsd:annotation
                    yfc:columnName="REFUND_CONSTRAINT_OPERATOR" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The operator to be used in evaluating the refund amount against the Refund Constraint Value.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundConstraintPaymentType"
                type="yfctype:Text-20" use="optional">
                <xsd:annotation
                    yfc:columnName="REFUND_CONSTRAINT_PAYMENT_TYPE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The payment type that needs to be used instead if the refund amount meets the constraint formed by the refund constraint operator and value when a new payment is created for refund purposes.  The payment type selection is valid only if it belongs to payment type group STORED_VALUE_CARD or OTHER.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundConstraintValue"
                type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="REFUND_CONSTRAINT_VALUE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The value that will be used to compare the refund amount against.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="REFUND_ITEM_ID" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The item to be included in a new refund fulfillment order when a refund is issued to this payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundItemUom" type="yfctype:UOM" use="optional">
                <xsd:annotation yfc:columnName="REFUND_ITEM_UOM" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The unit of measure for the refund item.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundRequiresFulfillment"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="REFUND_REQUIRES_FULFILLMENT" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether a new order needs to be created for the Refund Item ID and UOM when a refund needs to be issued on a new payment account for this payment type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundSameAccount" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="REFUND_SAME_ACCOUNT" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether refunds should be issued to the same account on which the payment was made.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RefundSequence"
                type="yfctype:ShorterNumber" use="optional">
                <xsd:annotation yfc:columnName="REFUND_SEQUENCE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>The sequence in which charges need to to be refunded if multiple payment types are supported by an organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ValidForReturn" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="VALID_FOR_RETURN" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether this payment type is valid for returns.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Voidable" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="VOIDABLE" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates that the Payment Type allows a void operation.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZeroAmountAuthorizationSupported"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="ZERO_AMOUNT_AUTH_SUPPORTED" yfc:tableName="YFS_PAYMENT_TYPE">
                    <xsd:documentation>This flag indicates whether or not the system will attempt a zero authorization when this Payment Type is added on the Order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgServiceList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrgService"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgService">
        <xsd:annotation yfc:tableName="YFS_ORG_SERVICE">
            <xsd:documentation>Stores services provided by an organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="OrgServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORG_SERVICE_KEY" yfc:tableName="YFS_ORG_SERVICE">
                    <xsd:documentation>Primary key attribute of YFS_ORG_SERVICE.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ServiceKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SERVICE_KEY" yfc:tableName="YFS_ORG_SERVICE">
                    <xsd:documentation>A service being provided by an organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InboundCompliance">
        <xsd:annotation yfc:tableName="YFS_INBOUND_COMPLIANCE">
            <xsd:documentation>This table stores the configuration details for an inbound compliance.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="IsAppointmentReqd" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_APPOINTMENT_REQD" yfc:tableName="YFS_INBOUND_COMPLIANCE">
                    <xsd:documentation>This flag indicates whether this buyer organization requires a customer appointment check. The default value is 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsEspMaintained" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_ESP_MAINTAINED" yfc:tableName="YFS_INBOUND_COMPLIANCE">
                    <xsd:documentation>This flag indicates whether this buyer organization maintians ESP parameters. The default value is 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsRoutingGuideMaintained"
                type="yfctype:Text-2" use="optional">
                <xsd:annotation
                    yfc:columnName="IS_ROUTING_GUIDE_MAINTAINED" yfc:tableName="YFS_INBOUND_COMPLIANCE">
                    <xsd:documentation>This field indicates whether this buyer organization maintains routing guides. Valid values can be 01 (Routing guide is maintained within the system), 02 (Routing guide is maintained external to the system) and 03 (Routing guide is not maintained).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReadUomFromBuyer" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="READ_UOM_FROM_BUYER" yfc:tableName="YFS_INBOUND_COMPLIANCE">
                    <xsd:documentation>This flag specifies if the unit of measure's are to be read from the buyer's catalog or seller's catalog.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCMLabelLevel" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCM_LABEL_LEVEL" yfc:tableName="YFS_INBOUND_COMPLIANCE">
                    <xsd:documentation>A field which indicates if SCM labels are to be generated at the Case level or for every Shipping Container.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SCMLabelReqd" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="SCM_LABEL_REQD" yfc:tableName="YFS_INBOUND_COMPLIANCE">
                    <xsd:documentation>A field which indicates if SCM labels are required or not and if required, are lables.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="ConsumableOrganizationList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:InventoryOrgRelationship"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="InventoryOrgRelationship">
        <xsd:annotation yfc:tableName="YFS_INV_ORG_RELATIONSHIP">
            <xsd:documentation>This table stores relationships between consuming and consumable inventory organizations.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="ConsumableInventoryOrg"
                type="yfctype:OrgCode" use="required">
                <xsd:annotation
                    yfc:columnName="CONSUMABLE_INVENTORY_ORG" yfc:tableName="YFS_INV_ORG_RELATIONSHIP">
                    <xsd:documentation>The participating inventory organization in the inventory organization relationship.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgThemeList">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:OrgTheme"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="OrgTheme">
        <xsd:annotation yfc:tableName="YFS_ORG_THEME">
            <xsd:documentation>This table stores available themes for an organization.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="IsDefault" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_DEFAULT" yfc:tableName="YFS_ORG_THEME">
                    <xsd:documentation>Indicates if the Theme is default for the Organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
								Valid value is <b>
                                <code>Delete</code>
                            </b> or blank.  If not provided the information will be created or modified.
							</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrgThemeKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="ORG_THEME_KEY" yfc:tableName="YFS_ORG_THEME">
                    <xsd:documentation>Primary key of YFS_ORG_THEME table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OrganizationKey" type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="ORGANIZATION_KEY" yfc:tableName="YFS_ORG_THEME">
                    <xsd:documentation>Organization that owns the Theme.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ThemeID" type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="THEME_ID" yfc:tableName="YFS_ORG_THEME">
                    <xsd:documentation>Indicates an ID of a Theme.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
