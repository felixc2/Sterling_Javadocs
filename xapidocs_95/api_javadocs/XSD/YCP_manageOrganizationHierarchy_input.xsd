        </xsd:all>
        <xsd:attribute name="AccountWithHub" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="ACCOUNT_WITH_HUB" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The account with the hub.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AuthorityType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="AUTHORITY_TYPE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The nature of the authority that issued the tax exemption to the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BillingAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BILLING_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The billing address that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="BusinessCalendarKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="BUSINESS_CALENDAR_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The default calendar that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CapacityKeptExternally" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="CAPACITY_KEPT_EXTERNALLY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether the capacity for the organization is kept externally.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CapacityOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="CAPACITY_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The capacity organization for this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CatalogOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="CATALOG_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
						The organization that owns the catalog of this organization.  A Master Catalog and a default Attribute Domain will automatically be created when a Catalog Organization is created.
						</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ContactAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CONTACT_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The contact address that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CorporateAddressKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CORPORATE_ADDRESS_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The corporate address that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CreatorOrganizationKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="CREATOR_ORGANIZATION_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization that created this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerMaintainedExternally"
            type="yfctype:Flag" use="optional">
            <xsd:annotation
                yfc:columnName="CUSTOMER_MAINTAINED_EXTERNALLY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether the customers of the organization are maintained externally.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CustomerMasterOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="CUSTOMER_MASTER_ORG_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>Determines which organization maintains this organization's customers. If organization is an enterprise, it will default to itself. Otherwise, it will default to its primary enterprise's CustomerMasterOrganizationCode.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DefaultDistributionRuleId"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation
                yfc:columnName="DEFAULT_DISTRIBUTION_RULE_ID" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The default distribution rule that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DefaultFulfillmentType"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="DEFAULT_FULFILLMENT_TYPE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The default fulfillment type to be used for the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DefaultPaymentRuleId"
            type="yfctype:Text-20" use="optional">
            <xsd:annotation yfc:columnName="DEFAULT_PAYMENT_RULE_ID" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The default payment rule the organization adopts.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DefaultPriceProgramKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="DEFAULT_PRICE_PROGRAM_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The default price program that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DunsNumber" type="yfctype:DunsNumber" use="optional">
            <xsd:annotation yfc:columnName="DUNS_NUMBER" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The DUNS number of the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InheritConfigFromEnterprise"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation
                yfc:columnName="INHERIT_CONFIG_FROM_ENTERPRISE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The enterprise to inherit configuration parameters from.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InheritOrgConfig" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="INHERIT_ORG_CONFIG" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether the configuration was inherited from the parent organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryKeptExternally"
            type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_KEPT_EXTERNALLY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether inventory in an organization is maintained within or outside the system.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization that owns the inventory of this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="InventoryPublished" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="INVENTORY_PUBLISHED" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether inventory is published by the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsDeliverySourcingKept" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_DEL_SOURCING_KEPT" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether sourcing is maintained for delivery  items.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsHubOrganization" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_HUB_ORGANIZATION" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					This flag indicates whether this organization is hub. There can be only one hub organization in the system, and that organization is the owner of the entire market place. A hub organization has maximum visibility in the market place.

					<Validation>
                            <Condition>this flag has been set to Y for organization other then with OrganizationCode 'DEFAULT'.
						</Condition>
                            <Error
                                Description=" isHubOrganization flag can be set to 'Y' only for organizationCode 'DEFAULT'" ErrorCode="YCP0470 "/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsLegalEntity" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_LEGAL_ENTITY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization is a legal entity.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsServiceSourcingKept" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_SRVC_SOURCING_KEPT" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether sourcing is maintained for service items.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsSourcingKept" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_SOURCING_KEPT" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether sourcing is maintained for the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IsTemplate" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="IS_TEMPLATE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					Flag to indicate whether or not the Organization is a Template Org.
					<Validation>
                            <Condition>Attempt to create Template Org that is also a Buyer, Carrier, or a Hub Org.
						</Condition>
                            <Error
                                Description="Invalid Configuration for Template Org" ErrorCode="YCP921_0003 "/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="IssuingAuthority" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="ISSUING_AUTHORITY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The authority that issued tax exemption certificate.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ItemXrefRule" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="ITEM_XREF_RULE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The rule used for cross referencing items.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="LocaleCode" type="yfctype:Locale" use="optional">
            <xsd:annotation yfc:columnName="LOCALE_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The locale that is associated with this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Operation" type="yfctype:Action" use="optional">
            <xsd:annotation>
                <xsd:documentation>
                    <div>Operation to perform
						<br/>Valid Values:
						<br/>
                        <b>
                            <code>Manage</code>
                        </b> - Creates a new record if it does not exist, or modifies an existing record if it does.
						<br/>
                        <b>
                            <code>Create</code>
                        </b> - Creates a new record.
						<br/>
                        <b>
                            <code>Modify</code>
                        </b> - Change values in an existing record.
						<br/>
                        <b>
                            <code>Delete</code>
                        </b> - Deletes an existing record.
						<br/>The default behavior is <b>
                            <code>Manage</code>
                        </b>.
						</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationCode" type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_CODE"
                yfc:isConditional="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The unique identifier for an organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_KEY"
                yfc:isConditional="Y" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
						 The primary key attribute for the YFS_ORGANIZATION table.  This attribute corresponds to the OrganizationCode Fact. When the operation  is <b>
                            <code>Create</code>
                        </b> this attribute  will be set with  the value of the Attribute OrganizationCode.
						</div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationName"
            type="yfctype:OrganizationName" use="optional">
            <xsd:annotation yfc:columnName="ORGANIZATION_NAME" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The name of the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OrganizationSuffix"
            type="yfctype:OrgSuffix" use="optional">
            <xsd:annotation yfc:columnName="ORG_SUFFIX" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The suffix to apply to the organization code that is generated automatically when creating a customer. If this field is blank, organization code generation will not be performed.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ParentOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="PARENT_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					Identifies the parent of an organization. Organization can have multiple child organizations but only one parent.
					<Validation>
                            <Condition>Invalid value is passed.
						</Condition>
                            <Error Description=" Invalid Key" ErrorCode="YFS10376 "/>
                        </Validation>
                        <Validation>
                            <Condition>If the organization is also a node, <b>
                                    <code>ParentOrganizationCode</code>
                                </b> is mandatory.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PasswordPolicyKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PWD_POLICY_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>Primary key attribute of PLT_PWD_POLICY.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PaymentProcessingReqd" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="PAYMENT_PROCESSING_REQD" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization requires payment processing.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PricingOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="PRICING_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization that owns the pricing configuration for this organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimaryEnterpriseKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="PRIMARY_ENTERPRISE_KEY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					Primary enterprise of an organization.
					<Validation>
                            <Condition>
                                <b>
                                    <code>PrimaryEnterpriseKey</code>
                                </b> is mandatory.
						</Condition>
                            <Error
                                Description="Mandatory Parameters for the Operation are missing." ErrorCode="YFS10364"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimarySicCode" type="yfctype:Text-10" use="optional">
            <xsd:annotation yfc:columnName="PRIMARY_SIC_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The primary SIC code of the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="PrimaryUrl" type="yfctype:URL" use="optional">
            <xsd:annotation yfc:columnName="PRIMARY_URL" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The organization's primary URL.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequiresChainedOrder" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="REQUIRES_CHAINED_ORDER" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether this organization requires chained order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequiresChangeRequest" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="REQUIRES_CHANGE_REQUEST" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="RequiresVasCompliance" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="REQUIRES_VAS_COMPLIANCE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether the buyer organization requires value added services to be peformed on the item. If set to Y, the organization can specify a Service Item to be performed on the item.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ResourceIdentifier"
            type="yfctype:ResourceId-100" use="optional">
            <xsd:annotation yfc:columnName="RESOURCE_IDENTIFIER" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The unique identifier for the resources of the enterprise.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="StoreConfigOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation
                yfc:columnName="STORE_CONFIG_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>
                    <div>
					Determines which organization maintains the store configuration for this organization. If an Org is defined by this attribute, so long as the Org exists, it will be the Store Config Org for the given organization. If this attribute is not specified, the StoreConfigOrganizationCode of the Primary Enterprise will be used by default, but if the given organization is an Enterprise, its StoreConfigOrganizationCode will be set to its own OrgCode. 
					<Validation>
                            <Condition>Store Config Org passed does not exist.
						</Condition>
                            <Error
                                Description="Store Config Org does not Exist" ErrorCode="YFCP921_0002"/>
                        </Validation>
                        <Validation>
                            <Condition>Attempt to create cyclic dependancy by defining Store Config Orgs to inherit from each other.
						</Condition>
                            <Error
                                Description="Cannot Configure Cyclic Dependancy in Template Org Hierarchy" ErrorCode="YCP0369 "/>
                        </Validation>
                        <Validation>
                            <Condition>Org that has a Template Role must have a StoreConfigOrganizationCode defined in input.
						</Condition>
                            <Error
                                Description="Store Config Org must be defined for Template Org." ErrorCode="YCP921_0004"/>
                        </Validation>
                    </div>
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SupplyDemandKeptExternally"
            type="yfctype:Flag" use="optional">
            <xsd:annotation
                yfc:columnName="INVORG_SUPPLY_DEMAND_EXTERNAL" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether inventory organization keeps supply and demand externally. If set to 'Y', the system publishes supply and demand to external system through events and there is no inventory updates in the system. Valid values are 'Y' or 'N'.                     When no value is set, supply and demand is kept internally.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SupplyKeptExternally" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="SUPPLY_KEPT_EXTERNALLY" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This flag indicates whether the organization tracks supply externally. If set to 'Y', the system gets the supply picture from an external system and computes the availability based on the external supplies, and the demands kept within the system. Default value is 'N'.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="SyncTargetID" type="yfctype:SyncTableID-40" use="optional">
            <xsd:annotation yfc:columnName="SYNC_TARGET_ID" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The SyncTarget that this Organization is assigned.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxExemptFlag" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="TAX_EXEMPT_FLAG" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This indicates whether the organization is exempt from taxes.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxExemptionCertificate"
            type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="TAX_EXEMPTION_CERTIFICATE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The certificate of tax exemption.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxJurisdiction" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="TAX_JURISDICTION" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The tax jurisdiction to which the organization belongs.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TaxpayerId" type="yfctype:ID-40" use="optional">
            <xsd:annotation yfc:columnName="TAXPAYER_ID" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The tax payer identifier for the organization.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="XrefAliasType" type="yfctype:Text-40" use="optional">
            <xsd:annotation yfc:columnName="XREF_ALIAS_TYPE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>The cross reference alias type.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="XrefOrganizationCode"
            type="yfctype:OrgCode" use="optional">
            <xsd:annotation yfc:columnName="XREF_ORGANIZATION_CODE" yfc:tableName="YFS_ORGANIZATION">
                <xsd:documentation>This field is not currently used.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:element name="CorporatePersonInfo">
        <xsd:annotation yfc:tableName="YFS_PERSON_INFO">
            <xsd:documentation>This table stores relevant personal information and addresses. Addresses are typically organization address, ship node address, customer address, and so forth. Personal information is required in cases where person to person contact needs to be established; for example, taking appointments or speaking during negotiations. The key of this table is posted in various tables to allow access to people and address information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="AddressID" type="yfctype:Text-256" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>A user specified label for an address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine1" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE1" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 1 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine2" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE2" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 2 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine3" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE3" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 3 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine4" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE4" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 4 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine5" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE5" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 5 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AddressLine6" type="yfctype:Address" use="optional">
                <xsd:annotation yfc:columnName="ADDRESS_LINE6" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The address line 6 field.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AlternateEmailID"
                type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="ALTERNATE_EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Beeper" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="BEEPER" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The beeper number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="City" type="yfctype:City" use="optional">
                <xsd:annotation yfc:columnName="CITY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The city in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Company" type="yfctype:Company" use="optional">
                <xsd:annotation yfc:columnName="COMPANY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The company this person is associated with.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Country" type="yfctype:Country" use="optional">
                <xsd:annotation yfc:columnName="COUNTRY" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The country in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DayPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="DAY_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The day time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Department" type="yfctype:Department" use="optional">
                <xsd:annotation yfc:columnName="DEPARTMENT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The primary department in charge of procurement.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EMailID" type="yfctype:EMailID" use="optional">
                <xsd:annotation yfc:columnName="EMAILID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The email address at which the person or a predesignated employee of the organization referred to in the address can be contacted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ErrorTxt" type="yfctype:ErrorText" use="optional">
                <xsd:annotation yfc:columnName="ERROR_TXT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningFaxNo" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_FAX_NO" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time fax at which the person or a predesignated employee of the organization referred to in the address can be notified.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EveningPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="EVENING_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The evening time phone at which the person or organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FirstName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="FIRST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The first name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="HttpUrl" type="yfctype:URL" use="optional">
                <xsd:annotation yfc:columnName="HTTP_URL" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The URL of the person or organization referred to in this contact information.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="JobTitle" type="yfctype:Title-40" use="optional">
                <xsd:annotation yfc:columnName="JOB_TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The job title of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LastName" type="yfctype:PersonName" use="optional">
                <xsd:annotation yfc:columnName="LAST_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The last name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Latitude" use="optional">
                <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>
                        <div>The latitude of this location. If <code>City</code>, <code>State</code>, or <code>Country</code> is not passed,then <code>Latitude</code> will not be presisted in the ship node table.</div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Decimal"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Longitude" use="optional">
                <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>
                        <div>The latitude of this location. If <code>City</code>, <code>State</code>, or <code>Country</code> is not passed, then <code>Longitude</code> will not be presisted in the ship node table.</div>
                    </xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Decimal"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MiddleName" type="yfctype:MiddleName" use="optional">
                <xsd:annotation yfc:columnName="MIDDLE_NAME" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MobilePhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="MOBILE_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The mobile phone at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OtherPhone" type="yfctype:PhoneNo" use="optional">
                <xsd:annotation yfc:columnName="OTHER_PHONE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The alternate phone number at which the person or a predesignated employee of the organization referred to in the address can be called.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PersonID" type="yfctype:PersonId" use="optional">
                <xsd:annotation yfc:columnName="PERSON_ID" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The unique identifier for a person.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreferredShipAddress"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PREFERRED_SHIP_ADDRESS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The preferred ship address over and above the address in this record. Pointer to another record of this type in this table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="State" type="yfctype:State" use="optional">
                <xsd:annotation yfc:columnName="STATE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The state in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Suffix" type="yfctype:Suffix" use="optional">
                <xsd:annotation yfc:columnName="SUFFIX" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The suffix for a person, such as Jr., or Sr.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Title" type="yfctype:Title-10" use="optional">
                <xsd:annotation yfc:columnName="TITLE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The title for the person such as Ms., Mr., or Dr..</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UseCount" type="yfctype:Count" use="optional">
                <xsd:annotation yfc:columnName="USE_COUNT" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="VerificationStatus"
                type="yfctype:ShortStatus" use="optional">
                <xsd:annotation yfc:columnName="VERIFICATION_STATUS" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>This field is currently not used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ZipCode" type="yfctype:ZipCode" use="optional">
                <xsd:annotation yfc:columnName="ZIP_CODE" yfc:tableName="YFS_PERSON_INFO">
                    <xsd:documentation>The zip code in the address.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Node">
        <xsd:annotation yfc:tableName="YFS_SHIP_NODE">
            <xsd:documentation>This table stores receiving node, ship node, and distribution centers in the system for various organizations.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:CanShipToNodeTypes"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NodeCarrierServicePickupScheduleList"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NodeCarrierServicePickupOverrideList"/>
            </xsd:all>
            <xsd:attribute name="AcceptanceRequired" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ACCEPTANCE_REQUIRED" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether acceptance is required by the ship node before processing orders for shipping.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ActivateFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ACTIVATE_FLAG" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node is active.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AdvanceNotificationTime"
                type="yfctype:NumberOfDaysWithDecimal" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
                            <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of
					days before which the ship node needs notification of an impending receipt or shipment.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AgentCriteriaGroup"
                type="yfctype:AgentCriteriaGroup" use="optional">
                <xsd:annotation yfc:columnName="AGENT_CRITERIA_GROUP" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The agent criteria group that is used to classify nodes.  This value can be accepted by WMS agents which will only perform their tasks on the nodes with matching node transactional velocity value. Valid values are: LOW, HIGH and common code values defined by hub.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AllowGiftWrap" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="ALLOW_GIFT_WRAP" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates that the node allows gift wrapping.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CanShipToAllNodes" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CAN_SHIP_TO_ALL_NODES" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the node can ship to all node/node types.  Entries in YFS_NODE_CAN_SHIP_TO_NT table will be ignored if flag is Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CanShipToOtherAddresses"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="CAN_SHIP_TO_OTHER_ADDRESSES" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the node can ship to other addresses.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CurrentBolNumber"
                type="yfctype:Counter" use="optional">
                <xsd:annotation yfc:columnName="CURRENT_BOL_NUMBER" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The last generated BOL number.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DcmIntegrationRealTime"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="DCM_INTEGRATION_REAL_TIME" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether a ship node has WMS7.0+ installed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DefaultDeclaredValue"
                type="yfctype:Money" use="optional">
                <xsd:annotation yfc:columnName="DEFAULT_DECLARED_VALUE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The default value the ship node may declare for its shipments.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExportLicenseExpDate"
                type="yfctype:Date" use="optional">
                <xsd:annotation yfc:columnName="EXPORT_LICENSE_EXP_DATE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The date by which an export license given to a ship node expires.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ExportLicenseNo" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="EXPORT_LICENSE_NO" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The export license number given to the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GLN"
                type="yfctype:GlobalLocationNumber" use="optional">
                <xsd:annotation yfc:columnName="GLN" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The global location number defined for a node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="GenerateBolNumber" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="GENERATE_BOL_NUMBER" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the BOL number needs to be generated for this ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IdentifiedByParentAs"
                type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="IDENTIFIED_BY_PARENT_AS" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The store or distribution center identifier assigned by the parent organization.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InheritConfigFromShipNode"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="INHERIT_CONFIG_FROM_SHIPNODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The ship node to inherit configuration from. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InterfaceSubType"
                type="yfctype:Text-100" use="optional">
                <xsd:annotation yfc:columnName="INTERFACE_SUB_TYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>A sub-classification of the interface type.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InterfaceSubTypeCode"
                type="yfctype:ActionCode" use="optional">
                <xsd:annotation yfc:columnName="ACTIONCODE" yfc:tableName="YFS_ACTION">
                    <xsd:documentation>Unique identifier for the action.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InterfaceType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="INTERFACE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This field indicates the manner in which orders are passed to a ship node for servicing ( "WMS_YANTRA","YFX","EVENTS" ).</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="InventoryTracked" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="INVENTORY_TRACKED" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node tracks inventory. Valid values are 'Y' or 'N'. Default Value is 'Y'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Inventorytype"
                type="yfctype:EnumInventoryType" use="optional">
                <xsd:annotation yfc:columnName="INVENTORYTYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This field indicates whether the ship node tracks inventory. The value is defaulted to 'TRACK' if the flag InventoryTracked is set to 'Y' and it is defaulted to 'INFINITE' if flag InventoryTracked is set to 'N'.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsFulfillmentNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_FULFILLMENT_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>
                        <div>Indicates if this node is a fulfillment node.
					<br/>Valid Values: <code>Y</code>, <code>N</code>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsItemBasedAllocationAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_IBA_ALLOWED" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the node is allowed for Item-Based Allocation process.  Valid values: Y/N.  Default value: Y.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="IsYard" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="IS_YARD" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>
                        <div>Indicate if this node is a yard.
					<br/>Valid Values: <code>Y</code>, <code>N</code>
                        </div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Latitude" use="optional">
                <xsd:annotation yfc:columnName="LATITUDE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The latitude of this ship node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="Longitude" use="optional">
                <xsd:annotation yfc:columnName="LONGITUDE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The longitude of this ship node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:DecimalWithNegative"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MaintainInventoryCost"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="MAINTAIN_INVENTORY_COST" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node maintains inventory cost.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MaxDaysToScheduleBefore"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation yfc:columnName="MAX_DAYS_TO_SCH_BEFORE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The number of days before which an order line cannot be scheduled at the node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="MinNotificationTime"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
                            <b>Deprecated.</b> See <code>manageNodeNotification</code> API. The number of
					days before which the communication to ship the order to the shipping node or supplier should be
					made. This field is used to determine the earliest ship date on the line.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NodeType" type="yfctype:NodeType" use="optional">
                <xsd:annotation yfc:columnName="NODE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The type of the node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OwnerType" type="yfctype:OwnerType" use="optional">
                <xsd:annotation yfc:columnName="OWNER_TYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This field is not currently used.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PicklistType" type="yfctype:Text-20" use="optional">
                <xsd:annotation yfc:columnName="PICKLIST_TYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The type of pick-list that this ship node requires for picking.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProcureToShipAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="PROCURE_TO_SHIP_ALLOWED" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether this node can be setup for procuring inventory from other nodes for shipment.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceiptProcessingTime"
                type="yfctype:NumberOfHours" use="optional">
                <xsd:annotation yfc:columnName="RECEIPT_PROCESSING_TIME" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The time in hours required to process an incoming procurement (purchase or transfer) order after receiving it.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceiptProcessingTimeForForwarding"
                type="yfctype:NumberOfHours" use="optional">
                <xsd:annotation
                    yfc:columnName="RCPT_PRCSSNG_TIME_FOR_FRWRDNG" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The time in hours required to process an incoming shipment that is being forwarded to this node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute
                name="ReceiptProcessingTimeForFutureInventory"
                type="yfctype:NumberOfDays" use="optional">
                <xsd:annotation
                    yfc:columnName="RCPT_PRCSSNG_TIME_FOR_FUTURE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The time in days required to process incoming future supply before being available for orders. Default: 0.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingCalendarKey"
                type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="RECEIVING_CALENDAR_KEY" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The receiving calendar that is associated with this ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReceivingNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="RECEIVING_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node can perform receiving operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequireTransferAcceptance"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="REQUIRE_TRANSFER_ACCEPTANCE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the order line will need to wait for acceptance, if this node is used as procure from node on the line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequiresChangeRequest"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="REQUIRES_CHANGE_REQUEST" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node requires change requests to manage order modifications after it is released to the node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReturnCenterFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="RETURN_CENTER_FLAG" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node is a return center.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ReturnsNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="RETURNS_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node can perform reverse logistic operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SerialTracking" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SERIAL_TRACKING" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the node requires serial tracking in inventory. Supported values are Y and N. This attribute comes into play only when the catalog signifies that the serial number requires tracking in inventory. However, a node has a choice to override the inventory tracking preference to indicate that it is not needed.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNode" type="yfctype:ShipNode" use="optional">
                <xsd:annotation yfc:columnName="SHIP_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The unique identifier for the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipNodeClass"
                type="yfctype:ShipNodeClass" use="optional">
                <xsd:annotation yfc:columnName="SHIP_NODE_CLASS" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The class of the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShipnodeType"
                type="yfctype:ShipnodeType" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_TYPE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The type of the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShippingCalendarKey"
                type="yfctype:Text-24" use="optional">
                <xsd:annotation yfc:columnName="SHIPPING_CALENDAR_KEY" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The shipping calendar that is associated with this ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ShippingNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SHIPPING_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node can perform shipping operations. This flag does not have any functionality. Reserved for future use.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SubstitutionAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="SUBSTITUTION_ALLOWED" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This field determines whether a node is allowed to use substitute items.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SupplierKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SUPPLIER_KEY" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The supplier who owns the ship node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ThreePlNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="THREE_PL_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether the ship node is a 3PL node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="TimeDiff" type="yfctype:Number" use="optional">
                <xsd:annotation yfc:columnName="TIME_DIFF" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>The time difference between the ship node and the place where the database is hosted from.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UseEndOfShiftTime" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="USE_END_OF_SHIFT_TIME" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>Thie flag indicates whether the node will use the end of shift time.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="WorkOrderCreationAllowed"
                type="yfctype:Flag" use="optional">
                <xsd:annotation
                    yfc:columnName="WORK_ORDER_CREATION_ALLOWED" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This field determines whether a node is allowed to create work orders.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="XDocNode" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="X_DOC_NODE" yfc:tableName="YFS_SHIP_NODE">
                    <xsd:documentation>This flag indicates whether this node is only used a cross-doc node.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CanShipToNodeTypes">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:CanShipToNodeType"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="CanShipToNodeType">
        <xsd:annotation yfc:tableName="YFS_NODE_CAN_SHIP_TO_NT">
            <xsd:documentation>This table contains node types that ship node can ship to.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="NodeTypeID" type="yfctype:Identifier" use="optional">
                <xsd:annotation yfc:columnName="NODE_TYPE_ID" yfc:tableName="YFS_NODE_CAN_SHIP_TO_NT">
                    <xsd:documentation>The unique identifier for the node type that the node can ship to.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NodeCarrierServicePickupScheduleList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NodeCarrierServicePickupSchedule"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NodeCarrierServicePickupSchedule">
        <xsd:annotation yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
            <xsd:documentation>
                <div>
                    This table stores Pickup Schedules for a Carrier service for the node organization. This element should be used only for those organizations which are Nodes.
                  </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CarrierServiceCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The carrier service that this node has the associated pickup times with. To update this attribute in an exisiting Carrier service Pickup Schedule <code>NodeServicePickupDowKey</code> must be passed otherwise a new record will be inserted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EffectiveFromDate" use="optional">
                <xsd:annotation yfc:columnName="EFFECTIVE_FROM_DATE" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The date that this carrier pickup schedule goes into effect. To update this attribute in an exisiting Carrier service Pickup Schedule <code>NodeServicePickupDowKey</code> must be passed otherwise a new record will be inserted.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="EffectiveToDate" use="optional">
                <xsd:annotation yfc:columnName="EFFECTIVE_TO_DATE" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The last date that this carrier pickup schedule is in effect. To update this attribute in an exisiting Carrier service Pickup Schedule <code>NodeServicePickupDowKey</code> must be passed otherwise a new record will be inserted.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="FridayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="FRIDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Fridays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Friday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="MondayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="MONDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Mondays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Monday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="NodeServicePickupDowKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="NODE_SRVC_PICKUP_DOW_KEY" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The primary key of the YFS_NODE_SRVC_PICKUP_DOW table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SaturdayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="SATURDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Saturdays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Saturday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ShipnodeKey" type="yfctype:Key" use="optional">
                <xsd:annotation yfc:columnName="SHIPNODE_KEY" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The ship node to which this carrier pickup schedule applies to.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="SundayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="SUNDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Sundays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Sunday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="ThursdayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="THURSDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Thursdays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Thursday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="TuesdayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="TUESDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Tuesdays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Tuesday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="WednesdayPickupTime" use="optional">
                <xsd:annotation yfc:columnName="WEDNESDAY_PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_DOW">
                    <xsd:documentation>The time on Wednesdays in which this carrier service is picked up. If not specified, then it is assumed that the carrier service does not pickup on this Wednesday.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NodeCarrierServicePickupOverrideList">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:NodeCarrierServicePickupOverride"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="NodeCarrierServicePickupOverride">
        <xsd:annotation yfc:tableName="YFS_NODE_SRVC_PICKUP_OVR">
            <xsd:documentation>
                <div>
                This table stores Pickup Schedule Overrides for a Carrier service for the node organization. This element should be used only for those organizations which are Nodes.
              </div>
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:attribute name="CanPickup" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="CAN_PICKUP" yfc:tableName="YFS_NODE_SRVC_PICKUP_OVR">
                    <xsd:documentation>Determines if this date can perform pickup.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CarrierServiceCode"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_NODE_SRVC_PICKUP_OVR">
                    <xsd:documentation>The carrier service that this node has the associated pickup time. To update this attribute in an exisiting Pickup Schedule Override <code>NodeServicePickupOverrideKey</code> must be passed otherwise a new record will be inserted.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="NodeServicePickupOverrideKey"
                type="yfctype:Key" use="optional">
                <xsd:annotation
                    yfc:columnName="NODE_SRVC_PICKUP_OVR_KEY" yfc:tableName="YFS_NODE_SRVC_PICKUP_OVR">
                    <xsd:documentation>The primary key of the YFS_NODE_SRVC_PICKUP_OVR table.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="OverrideDate" use="optional">
                <xsd:annotation yfc:columnName="OVERRIDE_DATE" yfc:tableName="YFS_NODE_SRVC_PICKUP_OVR">
                    <xsd:documentation>The overridden date that this node can or cannot perform the Carrier service pickup. This Pickup Schedule Override will be  used to override a Carrier service Pickup Schedule only if the overridden date lies between the effective dates of the Carrier service Pickup Schedule.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Date"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
            </xsd:attribute>
            <xsd:attribute name="PickupTime" use="optional">
                <xsd:annotation yfc:columnName="PICKUP_TIME" yfc:tableName="YFS_NODE_SRVC_PICKUP_OVR">
                    <xsd:documentation>The time on which this carrier service is picked up on the node.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:union>
                        <xsd:simpleType>
                            <xsd:restriction base="yfctype:Time"/>
                        </xsd:simpleType>
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:enumeration value=""/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:union>
                </xsd:simpleType>
