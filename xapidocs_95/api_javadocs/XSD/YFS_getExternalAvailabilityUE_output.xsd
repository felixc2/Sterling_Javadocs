    <xsd:element name="UnavailableLine">
        <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
            <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:Messages"/>
            </xsd:all>
            <xsd:attribute name="AssignedQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			Assigned Quantity
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemGroupCode"
                type="yfctype:EnumItemGroupCode" use="optional">
                <xsd:annotation yfc:columnName="ITEM_GROUP_CODE" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>This field indicates what kind of item is on the order line.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ItemID" type="yfctype:ItemID" use="optional">
                <xsd:annotation yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The identifier for the item. The ITEM_ID, UOM, and PRODUCT_CLASS form the unique inventory identifier for an order line. The ITEM_ID, UOM and ORGANIZATION_CODE form the unique item identifier for the Product Management module.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="LineId" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:isConditional="Y">
                    <xsd:documentation>
                        <div>
			Unique identifier for the line. It is mandatory to pass this attribute. 
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="ProductClass"
                type="yfctype:ProductClass" use="optional">
                <xsd:annotation yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>The product class. This field is used as an inventory identifier.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="RequiredQty" type="yfctype:Quantity" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			Required Quantity
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnavailableReason"
                type="yfctype:Text-40" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
			Reason for non-availability of the Item
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="UnitOfMeasure" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="UOM" yfc:tableName="YFS_ORDER_LINE">
                    <xsd:documentation>
                        <div>
			Unit Of Measure
			</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Messages">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element maxOccurs="unbounded" minOccurs="0" ref="yfc:Message"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Message">
        <xsd:complexType>
            <xsd:attribute name="Text" type="yfctype:Text-200" use="optional">
                <xsd:annotation>
                    <xsd:documentation>
                        <div>
					Message specifying the reason for unavailability
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
    </xsd:element>
    <xsd:element name="Options">
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" name="Option">
                    <xsd:complexType>
                        <xsd:all>
                            <xsd:element maxOccurs="1" minOccurs="0" name="PromiseLines">
                                <xsd:complexType>
                                    <xsd:sequence>
                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="PromiseLine">
                                    <xsd:annotation yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>This table stores order line information, including item details and shipping information. Multiple lines can be created for a single order defined in the YFS_ORDER_HEADER table.</xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:all>
                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="Assignments">
                                    <xsd:complexType>
                                    <xsd:sequence>

                                    <xsd:element
                                    maxOccurs="unbounded"
                                    minOccurs="0" name="Assignment">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				Assignment node gives details on the availability of the inventory.
				</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:complexType>
                                    <xsd:all>

                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="SubstituteItems">
                                    <xsd:complexType>
                                    <xsd:all>

                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="SubstituteItem">
                                    <xsd:complexType>
                                    <xsd:all>

                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="Procurements">
                                    <xsd:complexType>
                                    <xsd:all>

                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="Procurement">
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="IsIdleAsset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Flag to indicate if procurement is against an Idle Asset
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProcureFromNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Indicates node from which Substitute is being procured.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Indicates quantity being procured.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
							Shipment Date from the procurement node.
							</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>

                                    <xsd:attribute
                                    name="IsIdleAsset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Flag to indicate if substitution is an Idle Asset
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ItemID"
                                    type="yfctype:ItemID" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					ItemID of the substitute Item
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProcuredQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Substitute Items product quantity that can be procured from another node
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProductClass"
                                    type="yfctype:ProductClass" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Product class of the substitute Item
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Quantity of the substitute Item
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReservedQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Substitute Items product quantity that is reserved at the node.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="UOM"
                                    type="yfctype:UOM" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Unit Of Measure of the substitute Item
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>

                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="Procurements">
                                    <xsd:complexType>
                                    <xsd:all>

                                    <xsd:element
                                    maxOccurs="1"
                                    minOccurs="0" name="Procurement">
                                    <xsd:complexType>

                                    <xsd:attribute
                                    name="AwaitingReservationAcceptance"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
			Indicates whether <code>ProcureFromNode</code> requires transfer acceptance.
			</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IsIdleAsset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				Flag to indicate if procurement is against an Idle Asset
				</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProcureFromNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
			Indicates node from which product is being procured.
			</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProductAvailDate"
                                    type="yfctype:Date" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				Date on which the product will be avaliable at the suggested Procurement Node.
				</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
			Indicates quantity being procured.
			</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				Shipment Date from the procurement node.
				</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>

                                    <xsd:attribute
                                    name="AcceptedPreReservedQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Pre reserved quantity from supporting lines.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="AvailableFromUnplannedInventory"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					The <code>AvailableFromUnplannedInventory</code> flag indicates that the assignment consumes from the unplanned inventory when the <code>UseUnplannedInventory</code> flag is set to <code>
                                    <b>Y</b>
                                    </code>.
					In case of bundle parents, this indicates if any component assignment 
					of this assignment has consumed unplanned inventory.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="DeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Delivery Date of the assignment
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Distance"
                                    type="yfctype:Number" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Distance between the Shipping Address and Node's address measured in <code>MILES</code>. 
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="EmptyAssignmentReason"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Specifies the reason for empty assignment.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ExternalNode"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Flag to indicate if the node is an External Node
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="InteractionNo"
                                    type="yfctype:Number" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Number of the interaction
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="InvQty"
                                    type="yfctype:Quantity" use="required">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					This is a required attribute for supporting external availability. The value indicates the actual available quantity at the node.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="InventoryAvailabilityDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					This is a required attribute for supporting external availability. The value indicates the actual inventory available date at the node.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="IsIdleAsset"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Flag to indicate if the promise assignment is against an Idle Asset
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MaxDeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Product can not be delivered after this date.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MergeNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Node which will be used for forwarding. 
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="MinDeliveryDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Product can not be delivered before this date.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="NodePriority"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Priority of the Node.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProcuredQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Product Quantity that can be procured from another node
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ProductAvailDate"
                                    type="yfctype:Date" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Date on which the product will be avaliable at the suggested ShipNode.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="Quantity"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Quantity that can be obtained for this assignemnt.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ReservedQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Product Quantity that is reserved at the node.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SCAC"
                                    type="yfctype:Text-24" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Carrier Code determined by routing.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SegmentChangeQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Quantity for which segment has been changed.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipDate"
                                    type="yfctype:TimeStamp" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Shipment Date 
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="ShipNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Node from which inventory is available
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="SubstitutedQty"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
					Quantity substituted for the original item.
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:sequence>

                                    <xsd:attribute
                                    name="TotalNumberOfRecords"
                                    type="yfctype:String" use="optional"/>
                                    </xsd:complexType>
                                    </xsd:element>
                                    </xsd:all>
                                    <xsd:attribute
                                    name="AutoReplacedByPreferredSubstitute"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				Flag to indicates whether inventory is replaced with the Preferred Substitute. 
				<br/>Valid Values: <b>
                                    <code>Y</code>
                                    </b> or <b>
                                    <code>N</code>
                                    </b>
                                    </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="BundleParentLineId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
			Unique identifier for the parent line.
			</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="CarrierServiceCode"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="CARRIER_SERVICE_CODE" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>The carrier service code for this order line.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DeliveryMethod"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DELIVERY_METHOD" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>The delivery method for the product line.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="DistributionRuleId"
                                    type="yfctype:DistributionRuleId" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="DISTRIBUTION_RULE_ID" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>the identifier of the distribution group that is used to determine the ship node(s) that will ship the order line. If not proviFded, the seller organization's default distribution rule ID is used. If provided, it must be a valid ID for the Seller organization.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="FillQuantity"
                                    type="yfctype:Quantity" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FILL_QUANTITY" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>The minimum order line quantity that must be fulfilled in an order release before it is shipped.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="FulfillmentType"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="FULFILLMENT_TYPE" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>The method of fulfilling the order line during scheduling. This field can be matched with the corresponding field in the YFS_SOURCING_RULE_HDR table and the appropriate sourcing rule can be selected.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="IsBundleParent"
                                    type="yfctype:Flag" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
				Flag to indicate if the promise line is a bundle parent.
				</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ItemID"
                                    type="yfctype:ItemID" use="required">
                                    <xsd:annotation
                                    yfc:columnName="ITEM_ID" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>The identifier for the item. The ITEM_ID, UOM, and PRODUCT_CLASS form the unique inventory identifier for an order line. The ITEM_ID, UOM and ORGANIZATION_CODE form the unique item identifier for the Product Management module.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="KitQty" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="KIT_QTY" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>
                                    <div>
							Indicates ratio between the promise lines. This should be passed when ShipTogetherInRatio under Promise is set to Y. Decimal Ratios are not allowed.
						</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:Quantity"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="LineId"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation yfc:isConditional="Y">
                                    <xsd:documentation>
                                    <div>
					Unique identifier for the line. 
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="MinShipByDate" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="MIN_SHIP_BY_DATE" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>
                                    <div>
					The preferred minimum ship by date of the available supply that promise line can schedule against.  Available supply with ship by date earlier than the minimum ship by date will not be considered.
					<br/>Default: None
					</div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    <xsd:simpleType>
                                    <xsd:union>
                                    <xsd:simpleType>

                                    <xsd:restriction base="yfctype:TimeStamp"/>
                                    </xsd:simpleType>
                                    <xsd:simpleType>

                                    <xsd:restriction base="xsd:string">

                                    <xsd:enumeration value=""/>
                                    </xsd:restriction>
                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="NewItemID"
                                    type="yfctype:Text-40" use="optional">
                                    <xsd:annotation>
                                    <xsd:documentation>
                                    <div>
			The ItemId of the substitute that is used in place of the original item.
            </div>
                                    </xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ProductClass"
                                    type="yfctype:ProductClass" use="optional">
                                    <xsd:annotation
                                    yfc:columnName="PRODUCT_CLASS" yfc:tableName="YFS_ORDER_LINE">
                                    <xsd:documentation>The product class. This field is used as an inventory identifier.</xsd:documentation>
                                    </xsd:annotation>
                                    </xsd:attribute>
                                    <xsd:attribute
                                    name="ReceivingNode"
                                    type="yfctype:ShipNode" use="optional">
                                    <xsd:annotation
