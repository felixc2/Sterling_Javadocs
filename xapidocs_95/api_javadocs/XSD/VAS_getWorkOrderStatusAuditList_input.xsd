            <xsd:attribute name="FromServiceItemGroupCode"
                type="yfctype:EnumServiceItemGroupCode" use="optional"/>
            <xsd:attribute name="FromServiceItemID"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromServiceSupervisorId"
