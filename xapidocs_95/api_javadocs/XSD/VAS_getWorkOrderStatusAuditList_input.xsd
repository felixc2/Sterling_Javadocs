                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="PreCallStatusQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="Priority" type="yfctype:Integer" use="optional">
                <xsd:annotation yfc:columnName="PRIORITY"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER">
