            <xsd:attribute name="FromSegment" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromSegmentType" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromSerialNo" type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromServiceComplexityLevel"
