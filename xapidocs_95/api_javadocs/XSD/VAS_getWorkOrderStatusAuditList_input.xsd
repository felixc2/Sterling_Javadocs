    <xsd:import
        namespace="http://www.sterlingcommerce.com/documentation/types" schemaLocation="datatypes.xsd"/>
    <xsd:element name="WorkOrderStatusAudit" type="yfc:WorkOrderStatusAuditXSDType">
        <xsd:annotation yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
            <xsd:documentation>This table stores work order status change audit. whenever the work order status changes because of a business process, an audit is posted along with the status, timestamp, reason code and text, user ID and program ID. All transactions in the work order pipeline in fact will have entries in this table upon completion of a transaction.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>
    <xsd:complexType name="WorkOrderStatusAuditXSDType">
        <xsd:all>
            <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:WorkOrder"/>
        </xsd:all>
        <xsd:attribute name="AuditTransactionId"
            type="yfctype:TransactionID" use="optional">
            <xsd:annotation yfc:columnName="AUDIT_TRANSACTION_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>The unique identifier of an external Transaction reference.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AuditTransactionIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="AuditUserId" type="yfctype:UserId" use="optional">
            <xsd:annotation yfc:columnName="AUDIT_USER_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>The unique identifier of a user who performed the transaction.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="AuditUserIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="Comments" type="yfctype:Text-1500" use="optional">
            <xsd:annotation yfc:columnName="COMMENTS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Free form comments that may be recorded during the execution of the transaction.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="CommentsQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="FromAuditTransactionId"
            type="yfctype:TransactionID" use="optional"/>
        <xsd:attribute name="FromAuditUserId" type="yfctype:UserId" use="optional"/>
        <xsd:attribute name="FromComments" type="yfctype:Text-1500" use="optional"/>
        <xsd:attribute name="FromNewStatus" type="yfctype:ShortStatus" use="optional"/>
        <xsd:attribute name="FromNewStatusDate" type="yfctype:TimeStamp" use="optional"/>
        <xsd:attribute name="FromOldStatus" type="yfctype:ShortStatus" use="optional"/>
        <xsd:attribute name="FromOldStatusDate" type="yfctype:TimeStamp" use="optional"/>
        <xsd:attribute name="FromReasonCode" type="yfctype:ReasonCode" use="optional"/>
        <xsd:attribute name="FromTransactionId"
            type="yfctype:TransactionID" use="optional"/>
        <xsd:attribute name="FromWorkOrderKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="FromWorkOrderStatusAuditKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="NewStatus" type="yfctype:ShortStatus" use="optional">
            <xsd:annotation yfc:columnName="NEW_STATUS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Status of the work order after the status change.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NewStatusDate" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="NEW_STATUS_DATE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Status date recorded on the work order at the most recent status change.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="NewStatusDateQryType"
            type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="NewStatusQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="OldStatus" type="yfctype:ShortStatus" use="optional">
            <xsd:annotation yfc:columnName="OLD_STATUS"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Status of the work order before the status change.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OldStatusDate" type="yfctype:TimeStamp" use="optional">
            <xsd:annotation yfc:columnName="OLD_STATUS_DATE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Status date as recorded on the work order at the previous status change.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="OldStatusDateQryType"
            type="yfctype:DateQryType" use="optional"/>
        <xsd:attribute name="OldStatusQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ReasonCode" type="yfctype:ReasonCode" use="optional">
            <xsd:annotation yfc:columnName="REASON_CODE"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Reason code indicating the reason for the status change.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="ReasonCodeQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="ToAuditTransactionId"
            type="yfctype:TransactionID" use="optional"/>
        <xsd:attribute name="ToAuditUserId" type="yfctype:UserId" use="optional"/>
        <xsd:attribute name="ToComments" type="yfctype:Text-1500" use="optional"/>
        <xsd:attribute name="ToNewStatus" type="yfctype:ShortStatus" use="optional"/>
        <xsd:attribute name="ToNewStatusDate" type="yfctype:TimeStamp" use="optional"/>
        <xsd:attribute name="ToOldStatus" type="yfctype:ShortStatus" use="optional"/>
        <xsd:attribute name="ToOldStatusDate" type="yfctype:TimeStamp" use="optional"/>
        <xsd:attribute name="ToReasonCode" type="yfctype:ReasonCode" use="optional"/>
        <xsd:attribute name="ToTransactionId"
            type="yfctype:TransactionID" use="optional"/>
        <xsd:attribute name="ToWorkOrderKey" type="yfctype:Key" use="optional"/>
        <xsd:attribute name="ToWorkOrderStatusAuditKey"
            type="yfctype:Key" use="optional"/>
        <xsd:attribute name="TransactionId" type="yfctype:TransactionID" use="optional">
            <xsd:annotation yfc:columnName="TRANSACTION_ID"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>The unique identifier of a transaction that was executed on the work order.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="TransactionIdQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="WorkOrderKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="WORK_ORDER_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Primary key of YFS_WORK_ORDER.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WorkOrderKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
        <xsd:attribute name="WorkOrderStatusAuditKey" type="yfctype:Key" use="optional">
            <xsd:annotation yfc:columnName="WORK_ORDER_STS_AUDIT_KEY"
                yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER_STS_AUDIT">
                <xsd:documentation>Primary key.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="WorkOrderStatusAuditKeyQryType"
            type="yfctype:StringQryType" use="optional"/>
    </xsd:complexType>
    <xsd:element name="WorkOrder">
        <xsd:annotation yfc:tableName="YFS_WORK_ORDER">
            <xsd:documentation>This table stores the information about the work orders to be done at a node on an inventory item or a license plate.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
            <xsd:all>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:WorkOrderComponents"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:WorkOrderActivities"/>
                <xsd:element maxOccurs="1" minOccurs="0" ref="yfc:WorkOrderTag"/>
            </xsd:all>
            <xsd:attribute name="AllocatedLevel" type="yfctype:String" use="optional">
                <xsd:annotation yfc:qryTypeSupported="Y">
                    <xsd:documentation>
                        <div>Indicates the extent of allocation for the work order. The valid values are FULL, PARTIAL, NONE, and ANY. The respective values tells wether the work order is fully, partially, not allocated or is in any of the the previously mentioned states.
	        		</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="AllocatedLevelQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="CaseId" type="yfctype:LPNNo" use="optional">
                <xsd:annotation yfc:qryTypeSupported="Y">
                    <xsd:documentation>
                        <div>Case for which the work order is processed.
					</div>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="CaseIdQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="DocumentType" type="yfctype:Text-40" use="optional">
                <xsd:annotation yfc:columnName="DOCUMENT_TYPE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER">
                    <xsd:documentation>The document type for this work order.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="DocumentTypeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="EnterpriseCode" type="yfctype:ID-24" use="optional">
                <xsd:annotation yfc:columnName="ENTERPRISE_CODE"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER">
                    <xsd:documentation>The code of the enterprise to which the item or license plate belongs.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="EnterpriseCodeQryType"
                type="yfctype:StringQryType" use="optional"/>
            <xsd:attribute name="FinishNoLaterThan"
                type="yfctype:TimeStamp" use="optional">
                <xsd:annotation yfc:columnName="FINISH_NO_LATER_THAN"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER">
                    <xsd:documentation>The date a task should not finish later than. Pass through fields to be stamped on each task.</xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="FinishNoLaterThanQryType"
                type="yfctype:DateQryType" use="optional"/>
            <xsd:attribute name="FromAllocatedLevel"
                type="yfctype:String" use="optional"/>
            <xsd:attribute name="FromCaseId" type="yfctype:LPNNo" use="optional"/>
            <xsd:attribute name="FromDocumentType"
                type="yfctype:Text-40" use="optional"/>
            <xsd:attribute name="FromEnterpriseCode"
                type="yfctype:ID-24" use="optional"/>
            <xsd:attribute name="FromFinishNoLaterThan"
