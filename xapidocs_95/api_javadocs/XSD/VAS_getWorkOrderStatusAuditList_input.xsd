            <xsd:attribute name="HoldFlag" type="yfctype:Flag" use="optional">
                <xsd:annotation yfc:columnName="HOLD_FLAG"
                    yfc:qryTypeSupported="Y" yfc:tableName="YFS_WORK_ORDER">
