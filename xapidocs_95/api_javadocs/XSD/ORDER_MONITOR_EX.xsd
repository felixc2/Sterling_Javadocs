            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Field3IsKey" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="FIELD_3_IS_KEY" yfc:tableName="YFS_MONITORING_CONSOLIDATION">
                <xsd:documentation>This flag indicates whether the field 3 specified in the YFS_MONITOR_TYPE table is the key field for that table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Field4IsKey" type="yfctype:Flag" use="optional">
            <xsd:annotation yfc:columnName="FIELD_4_IS_KEY" yfc:tableName="YFS_MONITORING_CONSOLIDATION">
                <xsd:documentation>This flag indicates whether the field 4 specified in the YFS_MONITOR_TYPE table is the key field for that table.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Field5IsKey" type="yfctype:Flag" use="optional">
